Dialogue: 0,0:00:09.30,0:00:11.97,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sial! Kita tak punya pilihan\Nselain menghentikan mereka sendiri!{\b\fn\fs}
Dialogue: 0,0:00:12.60,0:00:14.14,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tenang saja.{\b\fn\fs}
Dialogue: 0,0:00:14.23,0:00:16.27,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ini kali pertamamu melawan monster?{\b\fn\fs}
Dialogue: 0,0:00:16.35,0:00:17.35,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Apa?{\b\fn\fs}
Dialogue: 0,0:00:20.11,0:00:21.36,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\an8}Hei! Itu berbahaya!{\b\fn\fs}
Dialogue: 0,0:00:24.78,0:00:26.86,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Bentuk Ledakan!{\b\fn\fs}
Dialogue: 0,0:00:33.24,0:00:35.79,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Mereka... Bukankah mereka kadet?{\b\fn\fs}
Dialogue: 0,0:00:35.87,0:00:37.17,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ya.{\b\fn\fs}
Dialogue: 0,0:00:42.67,0:00:43.76,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tidak ada habisnya!{\b\fn\fs}
Dialogue: 0,0:00:43.84,0:00:47.26,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sepertinya hanya kita berempat\Nyang bisa bertarung dengan benar.{\b\fn\fs}
Dialogue: 0,0:00:56.56,0:00:57.73,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Urabe!{\b\fn\fs}
Dialogue: 0,0:00:59.48,0:01:00.77,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\an8\i1}Ada kaiju!{\i\b\fn\fs}
Dialogue: 0,0:01:00.86,0:01:03.69,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\an8\i1}Sejumlah besar kaiju bergegas{\i}\N{\i1}menuju warga sipil biasa,{\i\b\fn\fs}
Dialogue: 0,0:01:03.78,0:01:06.49,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\an8\i1}yang kehilangan tempat tujuan{\i}\N{\i1}setelah jembatan hancur!{\i\b\fn\fs}
Dialogue: 0,0:01:10.45,0:01:11.70,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Eigen!{\i\b\fn\fs}
Dialogue: 0,0:01:11.78,0:01:13.20,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Hei, kalian baik-baik saja?{\b\fn\fs}
Dialogue: 0,0:01:13.28,0:01:15.08,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Tolong kami! Ada banyak sekali kaiju...{\i\b\fn\fs}
Dialogue: 0,0:01:20.00,0:01:21.00,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ada apa?{\b\fn\fs}
Dialogue: 0,0:01:21.08,0:01:22.92,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Apa? Ada apa dengannya?{\b\fn\fs}
Dialogue: 0,0:01:26.84,0:01:27.97,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kumohon kepada kalian!{\b\fn\fs}
Dialogue: 0,0:01:28.05,0:01:29.63,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tolong pinjamkan kekuatan kalian!{\b\fn\fs}
Dialogue: 0,0:01:29.72,0:01:31.09,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Hei, ayolah!{\b\fn\fs}
Dialogue: 0,0:01:32.01,0:01:33.05,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Seperti yang kalian lihat!{\b\fn\fs}
Dialogue: 0,0:01:33.14,0:01:35.77,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Seseorang dari Ranger Force\Nmenundukkan kepalanya!{\b\fn\fs}
Dialogue: 0,0:01:35.85,0:01:39.02,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ya... Aku tidak mengerti\Napa yang terjadi...{\b\fn\fs}
Dialogue: 0,0:01:39.52,0:01:42.19,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tapi ini tidak terasa...{\b\fn\fs}
Dialogue: 0,0:01:42.27,0:01:43.94,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}- sama sekali!\N- Sama sekali!{\b\fn\fs}
Dialogue: 0,0:01:44.52,0:01:46.48,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}- Teman-Teman! Kalian semua aman!\N- Eigen!{\b\fn\fs}
Dialogue: 0,0:01:46.57,0:01:49.24,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}- Aku takut, Kakak!\N- Maaf aku membuat kalian khawatir.{\b\fn\fs}
Dialogue: 0,0:01:52.03,0:01:54.33,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Baiklah! Mari kita lakukan!{\b\fn\fs}
Dialogue: 0,0:02:05.96,0:02:07.38,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Manusia!{\b\fn\fs}
Dialogue: 0,0:02:07.46,0:02:09.88,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Langkahi kami untuk sampai ke sisi lain!{\b\fn\fs}
Dialogue: 0,0:02:10.51,0:02:11.76,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Monster-monster itu...{\b\fn\fs}
Dialogue: 0,0:02:11.84,0:02:13.68,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Bisakah kita memercayai mereka?{\b\fn\fs}
Dialogue: 0,0:02:13.76,0:02:14.76,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Mari memercayai mereka!{\b\fn\fs}
Dialogue: 0,0:02:14.85,0:02:16.56,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tidak ada waktu untuk ragu. Ayo!{\b\fn\fs}
Dialogue: 0,0:02:16.64,0:02:17.85,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kau benar, ayo!{\b\fn\fs}
Dialogue: 0,0:02:17.93,0:02:19.14,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Cepat!{\b\fn\fs}
Dialogue: 0,0:02:19.23,0:02:20.77,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}- Terima kasih!\N- Maaf!{\b\fn\fs}
Dialogue: 0,0:02:20.85,0:02:21.89,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Terima kasih!{\b\fn\fs}
Dialogue: 0,0:02:25.31,0:02:26.82,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Apa yang terjadi?{\b\fn\fs}
Dialogue: 0,0:02:26.90,0:02:28.65,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kalian juga, cepat lari!{\b\fn\fs}
Dialogue: 0,0:02:28.73,0:02:29.78,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}- Tapi...\N- Tapi...{\b\fn\fs}
Dialogue: 0,0:02:29.86,0:02:31.11,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Jangan khawatir.{\b\fn\fs}
Dialogue: 0,0:02:31.82,0:02:32.95,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kalian tahu, bukan?{\b\fn\fs}
Dialogue: 0,0:02:33.03,0:02:34.41,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kakak kalian kuat!{\b\fn\fs}
Dialogue: 0,0:02:35.99,0:02:36.99,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}- Baik!\N- Baik!{\b\fn\fs}
Dialogue: 0,0:02:37.08,0:02:38.45,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Baiklah, pergilah!{\b\fn\fs}
Dialogue: 0,0:02:40.79,0:02:41.96,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kami berutang kepadamu.{\b\fn\fs}
Dialogue: 0,0:02:43.67,0:02:45.21,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tolong!{\b\fn\fs}
Dialogue: 0,0:02:45.29,0:02:46.29,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Awas!{\b\fn\fs}
Dialogue: 0,0:02:51.38,0:02:52.55,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Shippou!{\b\fn\fs}
Dialogue: 0,0:02:53.43,0:02:54.93,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Salah!{\b\fn\fs}
Dialogue: 0,0:02:55.01,0:02:58.56,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku senjata rahasia terakhir\Ndan terkuat Ranger Force,{\b\fn\fs}
Dialogue: 0,0:02:58.64,0:03:02.06,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}prajurit keenam, Penjaga Putih!{\b\fn\fs}
Dialogue: 0,0:03:04.56,0:03:06.56,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}- Baiklah, kau harus lari!\N- Baik!{\b\fn\fs}
Dialogue: 0,0:03:06.65,0:03:08.32,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Penjaga Putih?{\b\fn\fs}
Dialogue: 0,0:03:08.86,0:03:10.57,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku memanggilnya untuk berjaga-jaga.{\b\fn\fs}
Dialogue: 0,0:03:10.65,0:03:12.57,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Lagi pula, kami bertukar kontak.{\b\fn\fs}
Dialogue: 0,0:03:12.65,0:03:14.57,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Monster komunikasi ini...{\b\fn\fs}
Dialogue: 0,0:03:14.66,0:03:16.32,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tetap saja, itu sangat membantu.{\b\fn\fs}
Dialogue: 0,0:03:20.24,0:03:22.83,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Dia bodoh, tapi dia yang terkuat.{\b\fn\fs}
Dialogue: 0,0:03:26.04,0:03:27.04,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ayo!{\b\fn\fs}
Dialogue: 0,0:03:29.30,0:03:31.96,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kita akan bertarung dan meraih harapan\Ndengan tangan kita sendiri.{\b\fn\fs}
Dialogue: 0,0:03:32.05,0:03:33.72,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Itulah yang diajarkan kepada kita!{\b\fn\fs}
Dialogue: 0,0:03:35.26,0:03:36.93,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Warga menyeberang ke sisi lain{\i\b\fn\fs}
Dialogue: 0,0:03:37.01,0:03:38.93,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}menggunakan jembatan{\i}\N{\i1}yang dibentuk oleh monster.{\i\b\fn\fs}
Dialogue: 0,0:03:39.01,0:03:42.68,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Kami sudah memastikan{\i}\N{\i1}para Ranger menuju kaiju!{\i\b\fn\fs}
Dialogue: 0,0:03:42.77,0:03:44.69,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Menanglah, Teman-Teman...{\b\fn\fs}
Dialogue: 0,0:03:45.10,0:03:49.69,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Aku yakin kalian juga di sana sekarang,{\i}\N{\i1}memperjuangkan keyakinan.{\i\b\fn\fs}
Dialogue: 0,0:03:52.07,0:03:55.95,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Jangan kalah juga, Fighter D!{\i\b\fn\fs}
Dialogue: 0,0:04:06.75,0:04:08.33,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Luar biasa.{\b\fn\fs}
Dialogue: 0,0:04:08.71,0:04:10.84,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Manusia dan monster bekerja sama...{\b\fn\fs}
Dialogue: 0,0:04:11.38,0:04:13.59,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ini penampilan ideal.{\b\fn\fs}
Dialogue: 0,0:04:14.01,0:04:16.09,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Mari kita capai bersama.{\b\fn\fs}
Dialogue: 0,0:04:16.18,0:04:20.89,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Masyarakat ideal yang manusia dan monster\Nhidup berdampingan dalam arti sebenarnya!{\b\fn\fs}
Dialogue: 0,0:04:21.68,0:04:22.81,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}- Kami menolak!\N- Kami menolak!{\b\fn\fs}
Dialogue: 0,0:04:22.89,0:04:25.73,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Metodemu sangat buruk\Nhingga membuatku takut.{\b\fn\fs}
Dialogue: 0,0:04:25.81,0:04:28.48,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Menjadi monster\Nbukan berarti kau bisa berbuat sesukamu!{\b\fn\fs}
Dialogue: 0,0:04:28.56,0:04:29.65,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Begitu rupanya.{\b\fn\fs}
Dialogue: 0,0:04:30.44,0:04:32.23,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kalau begitu, menghilanglah.{\b\fn\fs}
Dialogue: 0,0:04:41.83,0:04:44.91,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Pukulan keadilan! Tinju Putih!{\b\fn\fs}
Dialogue: 0,0:04:46.91,0:04:48.17,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kau melakukan sesuatu?{\b\fn\fs}
Dialogue: 0,0:04:54.51,0:04:56.05,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Shippou!{\b\fn\fs}
Dialogue: 0,0:04:56.13,0:04:57.76,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Itu terlalu gila...{\b\fn\fs}
Dialogue: 0,0:04:57.84,0:05:00.18,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Mustahil kita bisa menang\Nmelawan hal seperti itu!{\b\fn\fs}
Dialogue: 0,0:05:07.94,0:05:08.94,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Usukubo?{\b\fn\fs}
Dialogue: 0,0:05:09.02,0:05:10.15,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kau tampak babak belur!{\b\fn\fs}
Dialogue: 0,0:05:12.11,0:05:13.48,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kau datang, Angel.{\b\fn\fs}
Dialogue: 0,0:05:13.57,0:05:14.94,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Selesaikan misimu.{\b\fn\fs}
Dialogue: 0,0:05:19.07,0:05:21.20,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku akan menyelesaikan misiku!{\b\fn\fs}
Dialogue: 0,0:05:25.70,0:05:26.75,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Hentikan, Usukubo!{\b\fn\fs}
Dialogue: 0,0:05:29.71,0:05:30.88,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}- Hentikan!\N- Hentikan!{\b\fn\fs}
Dialogue: 0,0:05:30.96,0:05:33.25,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}- Usukubo!\N- Hentikan!{\b\fn\fs}
Dialogue: 0,0:05:39.18,0:05:41.47,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Bagus, Angel.{\b\fn\fs}
Dialogue: 0,0:05:43.89,0:05:45.22,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Euforia ini...{\b\fn\fs}
Dialogue: 0,0:05:45.97,0:05:47.85,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku bisa merasakan cintanya kepadaku...{\b\fn\fs}
Dialogue: 0,0:05:48.48,0:05:50.48,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Usukubo...{\b\fn\fs}
Dialogue: 0,0:05:54.77,0:05:56.61,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kenapa...{\b\fn\fs}
Dialogue: 0,0:05:57.36,0:06:00.07,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Maaf, Kurusu.{\b\fn\fs}
Dialogue: 0,0:06:01.61,0:06:05.16,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Jadilah rasa sakit\Nyang menghubungkan manusia dan monster.{\b\fn\fs}
Dialogue: 0,0:06:05.24,0:06:08.16,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kenapa kau membantuku...{\b\fn\fs}
Dialogue: 0,0:06:09.00,0:06:10.41,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Saat itu...{\b\fn\fs}
Dialogue: 0,0:06:11.12,0:06:12.25,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kau akan baik-baik saja, Kurusu.{\b\fn\fs}
Dialogue: 0,0:06:12.33,0:06:13.63,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Pegang burung itu di atas kepalamu!{\b\fn\fs}
Dialogue: 0,0:06:14.67,0:06:17.34,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kau melindungiku.{\b\fn\fs}
Dialogue: 0,0:06:22.72,0:06:24.72,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Apa yang kau lakukan?{\b\fn\fs}
Dialogue: 0,0:06:27.68,0:06:29.48,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Kenapa kau ragu?{\i\b\fn\fs}
Dialogue: 0,0:06:29.56,0:06:32.85,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Keretakan antara manusia dan monster{\i}\N{\i1}tidak bisa dikubur dengan mudah.{\i\b\fn\fs}
Dialogue: 0,0:06:32.94,0:06:36.02,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Kita harus memberi mereka{\i}\N{\i1}lebih banyak rasa sakit.{\i\b\fn\fs}
Dialogue: 0,0:06:36.11,0:06:37.53,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Kau harus membunuhnya.{\i\b\fn\fs}
Dialogue: 0,0:06:37.90,0:06:39.15,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Bunuh dia!{\i\b\fn\fs}
Dialogue: 0,0:06:41.07,0:06:43.16,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ayah... Dia...{\b\fn\fs}
Dialogue: 0,0:06:43.66,0:06:46.41,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tidak bisakah kita menutup mata\Nterhadap penghuni permukaan?{\b\fn\fs}
Dialogue: 0,0:06:47.70,0:06:49.75,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Begitu kita mengalahkan Penjaga Naga,{\b\fn\fs}
Dialogue: 0,0:06:49.83,0:06:52.29,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}itu akan lebih dari cukup\Nuntuk menunjukkan kekuatan kita.{\b\fn\fs}
Dialogue: 0,0:06:52.37,0:06:54.83,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku yakin dunia akan mengerti.{\b\fn\fs}
Dialogue: 0,0:06:55.42,0:06:58.13,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kita bisa bekerja sama!{\b\fn\fs}
Dialogue: 0,0:06:58.71,0:07:01.34,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Angel, inikah yang kau inginkan?{\b\fn\fs}
Dialogue: 0,0:07:02.34,0:07:03.88,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ya! Ini adalah...{\b\fn\fs}
Dialogue: 0,0:07:11.43,0:07:13.39,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kalau begitu, tutup mulutmu.{\b\fn\fs}
Dialogue: 0,0:07:13.48,0:07:15.60,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tidak ada gunanya mendengar pendapatmu.{\b\fn\fs}
Dialogue: 0,0:07:16.06,0:07:17.23,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Apa yang kau lakukan?{\b\fn\fs}
Dialogue: 0,0:07:17.31,0:07:18.86,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Dia anakmu!{\b\fn\fs}
Dialogue: 0,0:07:18.94,0:07:21.11,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Dia bukan putri kandungku.{\b\fn\fs}
Dialogue: 0,0:07:21.61,0:07:22.61,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Apa?{\b\fn\fs}
Dialogue: 0,0:07:26.74,0:07:30.74,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Jangan memihak monster!{\b\fn\fs}
Dialogue: 0,0:07:31.54,0:07:34.83,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Dikucilkan dan putus asa,{\i\b\fn\fs}
Dialogue: 0,0:07:34.92,0:07:36.92,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}aku bertemu monster takdirku,{\i\b\fn\fs}
Dialogue: 0,0:07:37.29,0:07:39.00,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}seseorang bernama Julimerida.{\i\b\fn\fs}
Dialogue: 0,0:07:42.80,0:07:46.59,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Namun, kebahagiaan kami tak bertahan lama.{\i\b\fn\fs}
Dialogue: 0,0:07:49.14,0:07:52.02,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Monster berperingkat tinggi{\i}\N{\i1}mampu mengambil kulit manusia{\i\b\fn\fs}
Dialogue: 0,0:07:52.10,0:07:55.60,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}dan menggunakan kemampuan meniru{\i}\N{\i1}untuk berubah menjadi manusia, tapi...{\i\b\fn\fs}
Dialogue: 0,0:07:59.65,0:08:02.53,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Di ambang kematian{\i}\N{\i1}dan dalam kondisi yang sangat lemah,{\i\b\fn\fs}
Dialogue: 0,0:08:02.61,0:08:05.70,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}mereka hanya bisa menggunakan{\i}\N{\i1}sebagian kemampuan.{\i\b\fn\fs}
Dialogue: 0,0:08:10.24,0:08:12.24,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Apa maksudmu?{\b\fn\fs}
Dialogue: 0,0:08:12.33,0:08:15.12,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku...{\b\fn\fs}
Dialogue: 0,0:08:15.87,0:08:19.08,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kau hanya wadah,\Njadi, jangan terbawa suasana.{\b\fn\fs}
Dialogue: 0,0:08:20.46,0:08:23.96,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku sudah bicara\Ndengan Julimerida di dalam dirimu.{\b\fn\fs}
Dialogue: 0,0:08:27.26,0:08:30.72,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kau tak tahu bahwa orang tua tidak boleh\Nbicara tentang sesuatu ke anaknya?{\b\fn\fs}
Dialogue: 0,0:08:30.80,0:08:32.39,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku tidak mengerti apa yang terjadi,{\b\fn\fs}
Dialogue: 0,0:08:32.47,0:08:34.77,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}tapi apa ini artinya\Ndia kalah di lotre orang tua?{\b\fn\fs}
Dialogue: 0,0:08:46.65,0:08:47.66,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Teman-Teman...{\b\fn\fs}
Dialogue: 0,0:08:48.91,0:08:50.41,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ingat, Angel.{\b\fn\fs}
Dialogue: 0,0:08:50.49,0:08:52.24,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Apa misimu?{\b\fn\fs}
Dialogue: 0,0:08:52.79,0:08:55.08,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tunjukkan tujuan hidupmu.{\b\fn\fs}
Dialogue: 0,0:08:55.16,0:08:57.16,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Manusia atau monster...{\b\fn\fs}
Dialogue: 0,0:08:57.25,0:08:59.17,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Demi siapa kau hidup?{\b\fn\fs}
Dialogue: 0,0:09:02.67,0:09:05.84,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Monster. Aku monster.{\i\b\fn\fs}
Dialogue: 0,0:09:06.59,0:09:10.26,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Entahlah. Tapi jika Ayah bilang begitu...{\b\fn\fs}
Dialogue: 0,0:09:10.34,0:09:11.60,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku menemukanmu.{\b\fn\fs}
Dialogue: 0,0:09:15.14,0:09:16.23,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Hisui...{\b\fn\fs}
Dialogue: 0,0:09:18.56,0:09:21.86,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Beraninya kau meninggalkan bekas\Ndi tubuh seorang gadis.{\b\fn\fs}
Dialogue: 0,0:09:24.19,0:09:25.86,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku akan membuatmu membayarnya!{\b\fn\fs}
Dialogue: 0,0:09:30.86,0:09:33.70,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Usukubo. Kau masih bersama sisi lain?{\b\fn\fs}
Dialogue: 0,0:09:36.45,0:09:40.79,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku tak tahu\Napa aku bisa terus memercayai Ayah.{\b\fn\fs}
Dialogue: 0,0:09:40.87,0:09:43.13,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Apa maksudmu, Angel?{\i\b\fn\fs}
Dialogue: 0,0:09:43.21,0:09:45.92,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Kau tak perlu{\i}\N{\i1}mengkhawatirkan hal-hal rumit.{\i\b\fn\fs}
Dialogue: 0,0:09:46.30,0:09:48.13,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Dengarkan ayahmu.{\i\b\fn\fs}
Dialogue: 0,0:09:48.22,0:09:49.30,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Diam!{\b\fn\fs}
Dialogue: 0,0:09:49.38,0:09:51.97,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku tidak tahu apa pun tentang Ayah...{\b\fn\fs}
Dialogue: 0,0:09:53.14,0:09:54.14,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tapi...{\b\fn\fs}
Dialogue: 0,0:09:56.39,0:09:59.02,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}dari yang kulihat tentang perasaannya{\i}\N{\i1}terhadap monster,{\i\b\fn\fs}
Dialogue: 0,0:09:59.10,0:10:01.85,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}aku bisa merasakan cintanya tulus.{\i\b\fn\fs}
Dialogue: 0,0:10:02.48,0:10:04.61,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Jadi, katakan...{\b\fn\fs}
Dialogue: 0,0:10:05.23,0:10:08.40,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Apa yang kita...\NApa yang telah kulakukan...{\b\fn\fs}
Dialogue: 0,0:10:08.90,0:10:12.07,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Itu semua akan menjadi batu loncatan\Nuntuk menyelamatkan nyawa monster, bukan?{\b\fn\fs}
Dialogue: 0,0:10:12.16,0:10:13.37,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Benar.{\b\fn\fs}
Dialogue: 0,0:10:13.45,0:10:15.78,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Dari sana, kesetaraan sejati akan...{\b\fn\fs}
Dialogue: 0,0:10:22.50,0:10:26.05,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Semuanya demi kebaikan monster...{\i\b\fn\fs}
Dialogue: 0,0:10:32.97,0:10:33.97,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tidak!{\b\fn\fs}
Dialogue: 0,0:10:34.05,0:10:38.18,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Setidaknya,\Nmonster itu tidak ingin ini terjadi!{\b\fn\fs}
Dialogue: 0,0:10:41.44,0:10:42.98,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Baiklah!{\b\fn\fs}
Dialogue: 0,0:10:43.06,0:10:45.27,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku mengalahkan Deathmecia palsu!{\b\fn\fs}
Dialogue: 0,0:10:45.36,0:10:47.82,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}- Fighter D masih hidup!\N- Sudah kuduga!{\b\fn\fs}
Dialogue: 0,0:10:47.90,0:10:49.28,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}- Dia masih hidup!\N- Aku yang terkuat!{\b\fn\fs}
Dialogue: 0,0:10:56.95,0:10:58.12,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Tangguh sekali!{\i\b\fn\fs}
Dialogue: 0,0:11:00.50,0:11:02.67,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Jadi, bahkan Artefak Agung\Ntidak akan mempan padanya.{\b\fn\fs}
Dialogue: 0,0:11:03.25,0:11:05.96,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kalau begitu, tidak ada pilihan\Nselain mengikuti strategi itu!{\b\fn\fs}
Dialogue: 0,0:11:07.50,0:11:10.80,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Hei, Merah! Kau yakin tidak apa-apa\Nbagi pahlawan keadilan yang hebat{\b\fn\fs}
Dialogue: 0,0:11:10.88,0:11:13.38,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}untuk tetap berbaring di tanah\Ndengan menyedihkan seperti ini?{\b\fn\fs}
Dialogue: 0,0:11:13.80,0:11:15.14,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Diam...{\b\fn\fs}
Dialogue: 0,0:11:15.22,0:11:16.26,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Aku tidak percaya ini!{\i\b\fn\fs}
Dialogue: 0,0:11:16.35,0:11:18.89,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Seorang petarung melancarkan{\i}\N{\i1}pukulan kuat kepada Deathmecia!{\i\b\fn\fs}
Dialogue: 0,0:11:18.97,0:11:21.23,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Mungkinkah itu konflik internal?{\i\b\fn\fs}
Dialogue: 0,0:11:21.31,0:11:24.44,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Bagaimanapun, keberanian hebat{\i}\N{\i1}dari petarung yang bisa diandalkan!{\i\b\fn\fs}
Dialogue: 0,0:11:25.19,0:11:27.44,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku protagonisnya!{\b\fn\fs}
Dialogue: 0,0:11:29.53,0:11:30.82,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Yakushi Usukubo!{\b\fn\fs}
Dialogue: 0,0:11:31.86,0:11:34.95,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Biar kupastikan cintamu\Nterhadap para Penjaga Naga.{\b\fn\fs}
Dialogue: 0,0:11:35.49,0:11:38.20,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Episode {\i1}Penjaga Naga{\i}\Nversi tokusatsu mana{\b\fn\fs}
Dialogue: 0,0:11:38.28,0:11:39.54,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}yang menurutmu terbaik?{\b\fn\fs}
Dialogue: 0,0:11:39.62,0:11:44.21,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Episode 35, "Penjaga Naga Kalah!\NHarapan Terakhir yang Tersisa."{\b\fn\fs}
Dialogue: 0,0:11:45.04,0:11:46.71,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Menurutku juga!{\b\fn\fs}
Dialogue: 0,0:11:49.71,0:11:51.96,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Jangan kasar!{\b\fn\fs}
Dialogue: 0,0:11:52.76,0:11:54.59,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Pergi selamatkan Ranger di jembatan!{\i\b\fn\fs}
Dialogue: 0,0:11:54.68,0:11:55.68,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Baik!{\i\b\fn\fs}
Dialogue: 0,0:12:00.35,0:12:02.77,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Baiklah, Usukubo, giliranmu.{\b\fn\fs}
Dialogue: 0,0:12:03.23,0:12:06.02,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Jangan biarkan orang lain\Nmengendalikan rodamu!{\b\fn\fs}
Dialogue: 0,0:12:06.85,0:12:08.61,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Baiklah. Maaf, tapi...{\b\fn\fs}
Dialogue: 0,0:12:09.40,0:12:12.99,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku akan mengalahkan ayahmu.{\b\fn\fs}
Dialogue: 0,0:12:13.61,0:12:18.53,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Akan kuajari monster palsu itu\Nbetapa menakutkannya yang asli.{\b\fn\fs}
Dialogue: 0,0:12:19.03,0:12:20.37,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sakurama...{\b\fn\fs}
Dialogue: 0,0:12:21.41,0:12:23.20,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Apa selalu seperti ini?{\i\b\fn\fs}
Dialogue: 0,0:12:23.29,0:12:25.29,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Demi kebaikanmu, kau...{\i\b\fn\fs}
Dialogue: 0,0:12:27.33,0:12:30.59,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Tanpa kusadari, aku tidak bisa{\i}\N{\i1}mendengar suara di dalam hatiku.{\i\b\fn\fs}
Dialogue: 0,0:12:31.17,0:12:35.55,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Saat ini, emosiku sangat tinggi{\i}\N{\i1}hingga meredam suara apa pun.{\i\b\fn\fs}
Dialogue: 0,0:12:41.35,0:12:42.81,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Beraninya kau melawan!{\b\fn\fs}
Dialogue: 0,0:12:42.89,0:12:45.06,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Maksudmu kau tidak akan\Nmenuruti perkataanku?{\b\fn\fs}
Dialogue: 0,0:12:45.14,0:12:46.56,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Berhentilah merajuk!{\b\fn\fs}
Dialogue: 0,0:12:47.02,0:12:49.65,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kau mengingatkanku pada adikku\Ndalam fase memberontak!{\b\fn\fs}
Dialogue: 0,0:12:54.07,0:12:55.90,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Tunggu, apa itu benar?{\i\b\fn\fs}
Dialogue: 0,0:12:55.99,0:12:57.95,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Aku harus punya adik perempuan...{\i\b\fn\fs}
Dialogue: 0,0:13:05.58,0:13:07.33,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Jadi, akhirnya kau patuh?{\b\fn\fs}
Dialogue: 0,0:13:07.41,0:13:10.17,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tunjukkan kekuatanmu!{\b\fn\fs}
Dialogue: 0,0:13:17.42,0:13:19.43,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Inilah kekuatan sejatiku.{\b\fn\fs}
Dialogue: 0,0:13:19.93,0:13:22.85,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Membungkuklah, Deathmecia!{\b\fn\fs}
Dialogue: 0,0:13:23.68,0:13:24.89,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Pertanda.{\b\fn\fs}
Dialogue: 0,0:13:25.02,0:13:28.35,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Deathmecia bisa membaca serangan lawan\Ndan menghindarinya!{\b\fn\fs}
Dialogue: 0,0:13:34.69,0:13:36.61,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Bip. Berhenti.{\b\fn\fs}
Dialogue: 0,0:13:40.20,0:13:41.62,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Bidik tanduknya, Merah!{\b\fn\fs}
Dialogue: 0,0:14:00.59,0:14:02.01,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Wanita itu...{\i\b\fn\fs}
Dialogue: 0,0:14:08.68,0:14:09.85,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Astaga...{\b\fn\fs}
Dialogue: 0,0:14:10.69,0:14:13.23,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Bukankah sulit berjalan dengan kaki itu?{\b\fn\fs}
Dialogue: 0,0:14:13.31,0:14:14.73,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku akan menyokongmu,{\b\fn\fs}
Dialogue: 0,0:14:15.73,0:14:17.44,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}jadi, bisakah kau mengajakku?{\b\fn\fs}
Dialogue: 0,0:14:17.53,0:14:20.86,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ke kekuasaan militermu\Nuntuk menguasai dunia.{\b\fn\fs}
Dialogue: 0,0:14:20.95,0:14:22.36,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}- Apa?\N- Apa?{\b\fn\fs}
Dialogue: 0,0:14:25.20,0:14:29.58,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Suatu hari, kau juga akan bertemu\Ndengan monster yang kau sebut takdir.{\b\fn\fs}
Dialogue: 0,0:14:33.67,0:14:35.84,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ya, aku pernah bertemu dengannya...{\b\fn\fs}
Dialogue: 0,0:14:36.25,0:14:39.59,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Fighter D, kau adalah takdirku.{\b\fn\fs}
Dialogue: 0,0:14:41.17,0:14:42.55,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Apa maksudmu?{\b\fn\fs}
Dialogue: 0,0:14:43.30,0:14:45.97,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Nanti saja menggodanya.{\b\fn\fs}
Dialogue: 0,0:14:46.05,0:14:50.48,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Bisa kita mulai\NOperasi Menaklukkan Deathmecia?{\b\fn\fs}
Dialogue: 0,0:14:58.53,0:15:02.70,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kau seperti bertanya,\N"Kenapa dia tahu soal tanduk itu?"{\b\fn\fs}
Dialogue: 0,0:15:03.03,0:15:04.24,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Itu karena...{\b\fn\fs}
Dialogue: 0,0:15:04.32,0:15:07.49,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku informan brilian!{\b\fn\fs}
Dialogue: 0,0:15:13.04,0:15:15.46,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Rasakan ini!{\b\fn\fs}
Dialogue: 0,0:15:39.07,0:15:42.15,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Bawa dia kembali ke markas komunitas.{\b\fn\fs}
Dialogue: 0,0:15:42.24,0:15:45.66,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ini yang harus kita lakukan, bukan, Hisui?{\b\fn\fs}
Dialogue: 0,0:15:51.24,0:15:53.16,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kalau begitu, mari kita lakukan!{\b\fn\fs}
Dialogue: 0,0:15:56.75,0:15:58.75,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kekuatan kemampuannya meningkat?{\b\fn\fs}
Dialogue: 0,0:15:59.54,0:16:00.67,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Apa?{\b\fn\fs}
Dialogue: 0,0:16:00.75,0:16:03.51,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kau penggemar tokusatsu,\Ntapi kau tak tahu soal itu?{\b\fn\fs}
Dialogue: 0,0:16:03.92,0:16:06.18,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Makin berbahaya situasi\Nyang dihadapi pahlawan,{\b\fn\fs}
Dialogue: 0,0:16:06.26,0:16:08.01,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}mereka makin kuat.{\b\fn\fs}
Dialogue: 0,0:16:10.93,0:16:13.10,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Kuserahkan kepadamu, Merah Muda.{\i\b\fn\fs}
Dialogue: 0,0:16:16.69,0:16:17.81,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Ini aneh.{\i\b\fn\fs}
Dialogue: 0,0:16:17.90,0:16:21.27,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Serangkaian serangan ini{\i}\N{\i1}bukan untuk mengalahkanku,{\i\b\fn\fs}
Dialogue: 0,0:16:21.36,0:16:23.11,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}tapi untuk memancingku ke suatu tempat.{\i\b\fn\fs}
Dialogue: 0,0:16:23.69,0:16:24.70,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Jangan bilang...{\i\b\fn\fs}
Dialogue: 0,0:16:32.70,0:16:34.12,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kau tidak akan mendapatkan keinginanmu!{\b\fn\fs}
Dialogue: 0,0:16:34.20,0:16:35.62,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ame no Fuyu Kinu no Kami!{\b\fn\fs}
Dialogue: 0,0:16:42.05,0:16:43.92,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ame no Tajikarao no Kami!{\b\fn\fs}
Dialogue: 0,0:16:50.72,0:16:52.89,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Astaga. Kerja bagus.{\b\fn\fs}
Dialogue: 0,0:16:53.68,0:16:54.93,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Hibiki...{\b\fn\fs}
Dialogue: 0,0:17:03.15,0:17:04.32,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sudah kuduga.{\b\fn\fs}
Dialogue: 0,0:17:05.07,0:17:08.03,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Memang benar\Njika kau mengurungku di ruangan ini{\b\fn\fs}
Dialogue: 0,0:17:08.11,0:17:11.03,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}aku tidak akan bisa pergi\Nbahkan dengan kekuatanku,{\b\fn\fs}
Dialogue: 0,0:17:11.62,0:17:13.58,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}tapi kau kekurangan satu langkah.{\b\fn\fs}
Dialogue: 0,0:17:13.66,0:17:17.54,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Dengan kekuatan sekalibermu,\Nkau bahkan tidak bisa menyenggolku...{\b\fn\fs}
Dialogue: 0,0:17:18.00,0:17:20.29,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Rasakan ini!{\b\fn\fs}
Dialogue: 0,0:17:21.88,0:17:24.80,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Rasakan ini! Hancurkan!{\b\fn\fs}
Dialogue: 0,0:17:30.76,0:17:32.22,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Bidik tanduknya?{\b\fn\fs}
Dialogue: 0,0:17:32.30,0:17:33.56,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Benar.{\b\fn\fs}
Dialogue: 0,0:17:33.64,0:17:35.77,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Dalam {\i1}Penjaga Naga{\i} versi tokusatsu,{\b\fn\fs}
Dialogue: 0,0:17:35.85,0:17:39.64,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Deathmecia dirancang sedemikian rupa\Nsehingga melemah jika tanduknya patah.{\b\fn\fs}
Dialogue: 0,0:17:41.23,0:17:48.24,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}- Jika dia penggemar sejati, mungkin...{\i}\N- Rasakan ini!{\b\fn\fs}
Dialogue: 0,0:17:58.41,0:17:59.96,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku akan mengakuinya.{\b\fn\fs}
Dialogue: 0,0:18:00.04,0:18:02.58,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kau pemuja monster nomor satu!{\b\fn\fs}
Dialogue: 0,0:18:06.76,0:18:07.84,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tutup!{\b\fn\fs}
Dialogue: 0,0:18:07.92,0:18:08.92,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Lakukan!{\b\fn\fs}
Dialogue: 0,0:18:10.80,0:18:11.93,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Maafkan aku.{\b\fn\fs}
Dialogue: 0,0:18:20.02,0:18:21.90,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kau mencoba mengulur waktu?{\b\fn\fs}
Dialogue: 0,0:18:21.98,0:18:23.65,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tidak, kita akhiri hari ini.{\b\fn\fs}
Dialogue: 0,0:18:23.73,0:18:26.90,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Yakushi Usukubo, kau akan mati di sini.{\b\fn\fs}
Dialogue: 0,0:18:26.98,0:18:28.86,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku akan mati, katamu?{\b\fn\fs}
Dialogue: 0,0:18:28.94,0:18:32.61,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ayah, kenapa kau masih punya ingatan\Nmenjadi manusia?{\b\fn\fs}
Dialogue: 0,0:18:33.74,0:18:37.58,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}Menggunakan dua obat transformasi monster{\i}\N{\i1}menimpa seluruh kepribadianmu,{\i\b\fn\fs}
Dialogue: 0,0:18:37.66,0:18:39.87,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}dan kau akan terlahir kembali{\i}\N{\i1}sebagai monster.{\i\b\fn\fs}
Dialogue: 0,0:18:40.58,0:18:42.25,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Bukankah kau sudah menjelaskannya?{\b\fn\fs}
Dialogue: 0,0:18:43.33,0:18:46.13,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Meski begitu, kenapa kau masih bisa\Nmenjadi dirimu sendiri?{\b\fn\fs}
Dialogue: 0,0:18:46.96,0:18:49.34,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kau tidak menyuntikkan\Nobat itu sepenuhnya.{\b\fn\fs}
Dialogue: 0,0:18:49.71,0:18:51.42,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Jika perkataan Usukubo benar,{\b\fn\fs}
Dialogue: 0,0:18:51.51,0:18:54.30,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}berarti sisi kemanusiaanmu masih ada.{\b\fn\fs}
Dialogue: 0,0:18:54.80,0:18:56.93,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sebesar apa pun kau mengagumi monster,{\b\fn\fs}
Dialogue: 0,0:18:57.64,0:19:01.52,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\i1}kau tidak bisa{\i}\N{\i1}mengabaikan ingatanmu tentang Ibu.{\i\b\fn\fs}
Dialogue: 0,0:19:05.11,0:19:06.15,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Lalu kenapa?{\b\fn\fs}
Dialogue: 0,0:19:06.94,0:19:08.98,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Meski aku tak bisa berubah total\Nmenjadi monster,{\b\fn\fs}
Dialogue: 0,0:19:09.07,0:19:11.65,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}kau tidak bisa membunuhku.{\b\fn\fs}
Dialogue: 0,0:19:11.74,0:19:13.41,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Akan kukatakan sekali lagi.{\b\fn\fs}
Dialogue: 0,0:19:13.49,0:19:14.91,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kau akan mati.{\b\fn\fs}
Dialogue: 0,0:19:14.99,0:19:15.99,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kenapa?{\b\fn\fs}
Dialogue: 0,0:19:16.32,0:19:17.41,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Karena kelaparan.{\b\fn\fs}
Dialogue: 0,0:19:21.25,0:19:23.92,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kami tidak bisa menemukan cara\Nuntuk mengalahkanmu.{\b\fn\fs}
Dialogue: 0,0:19:24.00,0:19:25.67,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Itu sebabnya kami akan\Nmembuatmu mati kelaparan.{\b\fn\fs}
Dialogue: 0,0:19:26.17,0:19:28.67,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Salah satu perbedaan\Nantara manusia dan monster...{\b\fn\fs}
Dialogue: 0,0:19:29.30,0:19:31.67,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ayah pasti tahu, bukan?{\b\fn\fs}
Dialogue: 0,0:19:31.76,0:19:35.68,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Maaf, tapi kami yang terbaik\Ndalam hal tidak mati.{\b\fn\fs}
Dialogue: 0,0:19:36.14,0:19:39.18,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kami akan mengawasimu sampai kau mati.{\b\fn\fs}
Dialogue: 0,0:19:40.35,0:19:42.60,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Maaf, Julimerida...{\b\fn\fs}
Dialogue: 0,0:19:44.85,0:19:45.85,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kau mau bertarung?{\b\fn\fs}
Dialogue: 0,0:19:46.61,0:19:48.27,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tidak usah.{\b\fn\fs}
Dialogue: 0,0:19:48.36,0:19:49.86,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Meskipun aku tidak lengkap,{\b\fn\fs}
Dialogue: 0,0:19:49.94,0:19:52.28,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}tubuhku hampir sepenuhnya seperti monster.{\b\fn\fs}
Dialogue: 0,0:19:52.36,0:19:55.70,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Masih ada waktu yang sangat lama\Nsampai hidupku berakhir.{\b\fn\fs}
Dialogue: 0,0:19:56.28,0:19:59.20,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku akan senang punya teman bicara.{\b\fn\fs}
Dialogue: 0,0:19:59.28,0:20:01.24,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tentu, tidak apa-apa. Bicaralah kepadaku.{\b\fn\fs}
Dialogue: 0,0:20:01.33,0:20:02.95,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku akan mendengarkanmu sesukamu.{\b\fn\fs}
Dialogue: 0,0:20:04.25,0:20:07.38,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ini impian penggemar\Nyang menjadi kenyataan.{\b\fn\fs}
Dialogue: 0,0:20:11.46,0:20:15.30,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Komunitas Perlindungan Monster\Nsudah tamat.{\b\fn\fs}
Dialogue: 0,0:20:15.38,0:20:17.34,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ikut aku.{\b\fn\fs}
Dialogue: 0,0:20:19.72,0:20:21.18,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Maafkan aku.{\b\fn\fs}
Dialogue: 0,0:20:21.26,0:20:23.48,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku akan berjalan bersama manusia.{\b\fn\fs}
Dialogue: 0,0:20:23.56,0:20:24.68,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Begitu rupanya.{\b\fn\fs}
Dialogue: 0,0:20:25.81,0:20:29.48,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kau tahu apa yang akan terjadi\Njika melawanku, bukan?{\b\fn\fs}
Dialogue: 0,0:20:29.98,0:20:31.02,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Semua akan baik-baik saja!{\b\fn\fs}
Dialogue: 0,0:20:31.44,0:20:33.36,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku berjanji akan melindungimu,\Napa pun yang terjadi!{\b\fn\fs}
Dialogue: 0,0:20:34.19,0:20:35.45,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tunggu!{\b\fn\fs}
Dialogue: 0,0:20:38.49,0:20:41.41,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Dia pahlawan yang menyelamatkan kita!{\b\fn\fs}
Dialogue: 0,0:20:41.49,0:20:43.37,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kami tidak akan membiarkanmu menyentuhnya!{\b\fn\fs}
Dialogue: 0,0:20:46.16,0:20:47.37,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}XX...{\b\fn\fs}
Dialogue: 0,0:20:47.46,0:20:49.54,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Karena kau melepaskan tanganku,{\b\fn\fs}
Dialogue: 0,0:20:49.63,0:20:54.13,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}aku menantikan akhir yang menantimu.{\b\fn\fs}
Dialogue: 0,0:21:00.60,0:21:04.31,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\an8}Iblis yang merayakan kesetaraan\Ntelah menghilang.{\b\fn\fs}
Dialogue: 0,0:21:05.06,0:21:07.48,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\an8\i1}Neraca kesetaraan{\i}\N{\i1}selalu berat sebelah.{\i\b\fn\fs}
Dialogue: 0,0:21:08.10,0:21:09.81,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\an8\i1}Di sana, konflik terjadi.{\i\b\fn\fs}
Dialogue: 0,0:21:10.65,0:21:12.73,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\an8\i1}Konflik tidak akan pernah hilang.{\i\b\fn\fs}
Dialogue: 0,0:21:13.28,0:21:15.82,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\an8\i1}Musuh baru{\i}\N{\i1}kemungkinan akan terus muncul.{\i\b\fn\fs}
Dialogue: 0,0:21:17.24,0:21:19.74,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Untuk itulah kami di sini!{\b\fn\fs}
Dialogue: 0,0:21:20.87,0:21:23.37,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kami bersumpah\Nakan terus melindungi semua orang.{\b\fn\fs}
Dialogue: 0,0:21:23.45,0:21:26.45,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Para Penjaga Naga penuh dengan penipu.{\b\fn\fs}
Dialogue: 0,0:21:26.54,0:21:27.96,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ranger Force itu palsu.{\b\fn\fs}
Dialogue: 0,0:21:28.04,0:21:29.79,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Monster-monster itu juga palsu.{\b\fn\fs}
Dialogue: 0,0:21:31.13,0:21:32.88,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\an8}Mari kita kembalikan\Nke penampilan aslinya.{\b\fn\fs}
Dialogue: 0,0:21:34.13,0:21:38.63,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1\an8}Aku akan memperbaiki semua yang palsu.{\b\fn\fs}
Dialogue: 0,0:21:39.26,0:21:43.72,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Saatnya Ranger Force bangkit sekali lagi!{\b\fn\fs}
Dialogue: 0,0:21:43.81,0:21:45.93,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}- Merah!\N- Merah!{\b\fn\fs}
Dialogue: 0,0:23:13.90,0:23:15.90,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Diterjemahkan oleh Chairunnisa Rahmanindya{\b\fn\fs}
Dialogue: 0,0:23:15.98,0:23:18.07,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Satu tahun kemudian{\b\fn\fs}
Dialogue: 0,0:23:27.99,0:23:29.12,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Hei.{\b\fn\fs}
Dialogue: 0,0:23:35.29,0:23:37.42,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Selamat datang kembali, D.{\b\fn\fs}
Dialogue: 0,0:23:37.92,0:23:40.51,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kalau begitu, bisa kita mulai lagi?{\b\fn\fs}
Dialogue: 0,0:23:40.76,0:23:41.92,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Go! Go! Loser Ranger!{\b\fn\fs}
