Dialogue: 0,0:11:52.12,0:11:55.00,tanda,,0,0,0,,GURU MATEMATIKA, TATSUMA SAKAMOTO
Dialogue: 0,0:11:55.59,0:11:57.96,tanda,,0,0,0,,GINTAMA - MR.GINPACHI'S ZANY CLASS
Dialogue: 0,0:16:12.51,0:16:14.64,tanda,,0,0,0,,GURU KESEHATAN DAN OLAHRAGA, TSUKUYO
Dialogue: 0,0:16:27.57,0:16:29.65,tanda,,0,0,0,,"KESEHATAN DAN OLAHRAGA YANG INDAH"
Dialogue: 0,0:17:28.79,0:17:31.26,tanda,,0,0,0,,PENGARAHAN KARYAWISATA SEKOLAH
Dialogue: 0,0:17:35.18,0:17:37.43,tanda,,0,0,0,,"MENGANTUK"
Dialogue: 0,0:21:29.08,0:21:30.79,tanda,,0,0,0,,SEPEKAN KEMUDIAN
Dialogue: 0,0:21:34.96,0:21:36.67,tanda,,0,0,0,,CINTA DAN EMAS
Dialogue: 0,0:23:31.57,0:23:34.08,tanda,,0,0,0,,TAKASUGI PEKAN INI
Dialogue: 0,0:23:56.72,0:23:58.06,tanda,,0,0,0,,EPISODE 6
Dialogue: 0,0:23:58.14,0:23:59.89,tanda,,0,0,0,,PINDAH SEKOLAH KARENA PEKERJAAN AYAH\NDAN SEMACAMNYA TAMPAK CUKUP MENARIK
Dialogue: 0,0:23:59.98,0:24:04.90,tanda,,0,0,0,,EPISODE SELANJUTNYA
Dialogue: 0,0:24:04.98,0:24:09.90,tanda,,0,0,0,,BERPISAH DI KELAS 3-Z
Dialogue: 0,0:00:02.08,0:00:04.04,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jika ingin damai,
Dialogue: 0,0:00:04.12,0:00:05.88,Default,,0,0,0,,{\blur3}{\fad(00,40)}bersiaplah untuk perang.
Dialogue: 0,0:00:05.96,0:00:09.05,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jika ingin damai,\Nselalu siapkan obat wasir.
Dialogue: 0,0:00:09.13,0:00:10.88,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kau harus melakukannya.
Dialogue: 0,0:00:14.34,0:00:18.56,Default,,0,0,0,,{\blur3}{\fad(00,40)}Divisi Satu Kepolisian Kabukicho bilang
Dialogue: 0,0:00:18.93,0:00:19.77,Default,,0,0,0,,{\blur3}{\fad(00,40)}perampok bank
Dialogue: 0,0:00:19.85,0:00:21.48,Default,,0,0,0,,{\blur3}{\fad(00,40)}yang mencuri 50 juta yen
Dialogue: 0,0:00:21.56,0:00:24.02,Default,,0,0,0,,{\blur3}{\fad(00,40)}telah buron selama dua pekan.
Dialogue: 0,0:00:24.10,0:00:29.19,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jika melihat wajah-wajah ini,\Ntangkap mereka.
Dialogue: 0,0:00:29.27,0:00:31.15,Default,,0,0,0,,{\blur3}{\fad(00,40)}Bukankah kita harus hubungi 110?
Dialogue: 0,0:00:31.24,0:00:33.40,Default,,0,0,0,,{\blur3}{\fad(00,40)}Mereka ingin kita menangkap mereka.
Dialogue: 0,0:00:33.49,0:00:36.82,Default,,0,0,0,,{\blur3}{\fad(00,40)}Imbalannya satu juta yen.
Dialogue: 0,0:00:42.66,0:00:45.83,Default,,0,0,0,,{\blur3}{\fad(00,40)}Lagi pula,\Nmereka mungkin sudah kabur sekarang.
Dialogue: 0,0:00:45.92,0:00:47.38,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ayo kembali bekerja.
Dialogue: 0,0:01:01.47,0:01:05.69,Default,,0,0,0,,{\blur3}{\fad(00,40)}Bersembunyi di sekolah ternyata ide bagus.
Dialogue: 0,0:01:05.77,0:01:08.11,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tapi dua pekan terlalu lama.
Dialogue: 0,0:01:08.19,0:01:10.23,Default,,0,0,0,,{\blur3}{\fad(00,40)}Makanan kita juga hampir habis.
Dialogue: 0,0:01:10.32,0:01:11.48,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kita harus bagaimana, Kakak?
Dialogue: 0,0:01:11.57,0:01:13.11,Default,,0,0,0,,{\blur3}{\fad(00,40)}Apa yang harus kita lakukan, Kakak?
Dialogue: 0,0:01:14.95,0:01:18.32,Default,,0,0,0,,{\blur3}{\fad(00,40)}Apa ini saatnya berpisah\Ndengan sekolah ini?
Dialogue: 0,0:01:18.70,0:01:20.08,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Setuju.\N- Setuju.
Dialogue: 0,0:01:21.79,0:01:24.58,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kelas 3-Z.
Dialogue: 0,0:01:25.04,0:01:28.29,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Pak Ginpachi.\N- Pak Ginpachi.
Dialogue: 0,0:03:22.82,0:03:23.82,Default,,0,0,0,,{\blur3}{\fad(00,40)}Sudah hampir waktunya
Dialogue: 0,0:03:23.91,0:03:28.20,Default,,0,0,0,,{\blur3}{\fad(00,40)}untuk karyawisata sekolah!
Dialogue: 0,0:03:28.37,0:03:29.83,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tapi dengarkan baik-baik.
Dialogue: 0,0:03:29.91,0:03:33.29,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jangan terlalu terbawa suasana\Nhanya karena ini karyawisata sekolah.
Dialogue: 0,0:03:33.38,0:03:38.21,Default,,0,0,0,,{\blur3}{\fad(00,40)}Sejarah Kyoto berusia sekitar 2.000 tahun.\NDom Perignon.
Dialogue: 0,0:03:38.30,0:03:42.72,Default,,0,0,0,,{\blur3}{\fad(00,40)}Mari kita kunjungi kuil-kuil Kyoto\Ndengan serius. Permainan Osama.
Dialogue: 0,0:03:42.80,0:03:48.14,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ini pengetahuan yang tidak bisa dipelajari\Ndari buku. Pacaran sepulang kerja.
Dialogue: 0,0:03:48.22,0:03:49.18,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tidak, kau mengatakan semuanya.
Dialogue: 0,0:03:49.27,0:03:51.39,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kau tidak sadar mengatakan keinginanmu.
Dialogue: 0,0:03:51.48,0:03:57.77,Default,,0,0,0,,{\blur3}{\fad(00,40)}Komite Disiplin punya pengumuman penting.
Dialogue: 0,0:03:58.78,0:04:00.03,Default,,0,0,0,,{\blur3}{\fad(00,40)}Berdiri!
Dialogue: 0,0:04:02.95,0:04:07.12,Default,,0,0,0,,{\blur3}{\fad(00,40)}Peraturan Pertama Karyawisata Sekolah.
Dialogue: 0,0:04:07.20,0:04:09.66,Default,,0,0,0,,{\blur3}{\fad(00,40)}Mereka yang menyusahkan guru
Dialogue: 0,0:04:09.75,0:04:11.66,Default,,0,0,0,,{\blur3}{\fad(00,40)}akan melakukan seppuku!
Dialogue: 0,0:04:13.42,0:04:15.25,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ulangi.
Dialogue: 0,0:04:15.33,0:04:16.79,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Peraturan Pertama.\N- Peraturan pertama.
Dialogue: 0,0:04:16.88,0:04:19.20,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Mereka yang menyusahkan guru\N- Mereka yang menyusahkan guru
Dialogue: 0,0:04:19.30,0:04:20.76,Default,,0,0,0,,{\blur3}{\fad(00,40)}- akan melakukan seppuku.\N- akan melakukan seppuku.
Dialogue: 0,0:04:20.83,0:04:22.30,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tidak, ini seharusnya\Nkisah sekolah yang seru.
Dialogue: 0,0:04:22.38,0:04:24.72,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kenapa mereka menambahkan peraturan\Nhidup atau mati seperti itu?
Dialogue: 0,0:04:25.22,0:04:26.60,Default,,0,0,0,,{\blur3}{\fad(00,40)}Peraturan Kedua.
Dialogue: 0,0:04:26.68,0:04:29.10,Default,,0,0,0,,{\blur3}{\fad(00,40)}Mereka yang menyusahkan Komite Disiplin
Dialogue: 0,0:04:29.18,0:04:30.64,Default,,0,0,0,,{\blur3}{\fad(00,40)}akan melakukan seppuku.
Dialogue: 0,0:04:30.72,0:04:32.14,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Peraturan Kedua.\N- Peraturan Kedua.
Dialogue: 0,0:04:32.23,0:04:33.23,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Mereka yang menyusahkan\N- Mereka yang menyusahkan
Dialogue: 0,0:04:33.31,0:04:35.06,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Komite Disiplin\N- Komite Disiplin
Dialogue: 0,0:04:35.15,0:04:36.81,Default,,0,0,0,,{\blur3}{\fad(00,40)}- akan melakukan seppuku.\N- akan melakukan seppuku.
Dialogue: 0,0:04:36.90,0:04:41.15,Default,,0,0,0,,{\blur3}{\fad(00,40)}Peraturan Ketiga, mereka yang masuk\Nke kamar lawan jenis
Dialogue: 0,0:04:41.24,0:04:43.11,Default,,0,0,0,,{\blur3}{\fad(00,40)}akan melakukan seppuku.
Dialogue: 0,0:04:45.95,0:04:46.99,Default,,0,0,0,,{\blur3}{\fad(00,40)}Perampok?
Dialogue: 0,0:04:47.07,0:04:48.95,Default,,0,0,0,,{\blur3}{\fad(00,40)}Mereka memang terlihat seperti preman.
Dialogue: 0,0:04:49.03,0:04:51.58,Default,,0,0,0,,{\blur3}{\fad(00,40)}Lihat siapa yang bicara.
Dialogue: 0,0:04:51.66,0:04:53.50,Default,,0,0,0,,{\blur3}{\fad(00,40)}Bukankah sudah kubilang\Nuntuk memakai honorifik?
Dialogue: 0,0:04:53.58,0:04:55.42,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Kau juga terlihat\N- Lewat sini.
Dialogue: 0,0:04:55.50,0:04:56.33,Default,,0,0,0,,{\blur3}{\fad(00,40)}- seperti preman.\N- Kakak,
Dialogue: 0,0:04:56.42,0:04:57.25,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Hei,\N- bagaimana dengan di sana?
Dialogue: 0,0:04:57.33,0:04:58.63,Default,,0,0,0,,{\blur3}{\fad(00,40)}- kau mendengarkan?\N- Ini aman.
Dialogue: 0,0:05:00.25,0:05:01.26,Default,,0,0,0,,{\blur3}{\fad(00,40)}Sanggul…
Dialogue: 0,0:05:01.71,0:05:03.59,Default,,0,0,0,,{\blur3}{\fad(00,40)}Sanggul pria.
Dialogue: 0,0:05:03.67,0:05:05.68,Default,,0,0,0,,{\blur3}{\fad(00,40)}Hubungi polisi.
Dialogue: 0,0:05:05.76,0:05:06.76,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tunggu.
Dialogue: 0,0:05:08.35,0:05:11.56,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku punya ide bagus.
Dialogue: 0,0:05:11.64,0:05:16.10,Default,,0,0,0,,{\blur3}{\fad(00,40)}Peraturan Ketujuh,\Nmereka yang melempar bantal…
Dialogue: 0,0:05:16.19,0:05:22.40,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ini pesan dari Kantor Kepala Sekolah\Nuntuk semua guru…
Dialogue: 0,0:05:22.48,0:05:23.74,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ada keadaan darurat.
Dialogue: 0,0:05:23.82,0:05:27.41,Default,,0,0,0,,{\blur3}{\fad(00,40)}Perampok bank yang kabur
Dialogue: 0,0:05:27.49,0:05:29.12,Default,,0,0,0,,{\blur3}{\fad(00,40)}telah menyusup ke sekolah kita!
Dialogue: 0,0:05:29.20,0:05:30.62,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Apa maksudnya?\N- Tidak mungkin.
Dialogue: 0,0:05:30.70,0:05:31.79,Default,,0,0,0,,{\blur3}{\fad(00,40)}Hei, sungguh?
Dialogue: 0,0:05:32.29,0:05:33.91,Default,,0,0,0,,{\blur3}{\fad(00,40)}Pe… Perampok?
Dialogue: 0,0:05:34.00,0:05:36.92,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jika guru kita menangkap pelakunya,
Dialogue: 0,0:05:37.00,0:05:39.88,Default,,0,0,0,,{\blur3}{\fad(00,40)}SMA Gintama akan terkenal di seluruh negeri
Dialogue: 0,0:05:39.96,0:05:42.17,Default,,0,0,0,,{\blur3}{\fad(00,40)}dan naik ke puncak peringkat SMA populer.
Dialogue: 0,0:05:42.25,0:05:44.09,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jumlah murid yang mendaftar akan meroket.
Dialogue: 0,0:05:44.17,0:05:47.68,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ini kesempatan bagus\Nbagi sekolah kita untuk untung!
Dialogue: 0,0:05:47.76,0:05:50.89,Default,,0,0,0,,{\blur3}{\fad(00,40)}Siapa pun yang menangkap pelakunya\Nakan dapat satu juta!
Dialogue: 0,0:05:52.22,0:05:54.22,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Satu juta?\N- Satu juta?
Dialogue: 0,0:05:54.39,0:05:57.14,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ayo, para guru pemberani!
Dialogue: 0,0:05:57.23,0:05:59.61,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Satu juta itu milikku!\N- Satu juta itu milikku!
Dialogue: 0,0:06:04.65,0:06:06.03,Default,,0,0,0,,{\blur3}{\fad(00,40)}Lalu…
Dialogue: 0,0:06:06.11,0:06:07.24,Default,,0,0,0,,{\blur3}{\fad(00,40)}aku akan menekannya.
Dialogue: 0,0:06:21.79,0:06:24.59,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kehidupan Sekolah: Penghalang Armstrong!
Dialogue: 0,0:06:24.67,0:06:25.92,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aktifkan!
Dialogue: 0,0:06:30.80,0:06:34.01,Default,,0,0,0,,{\blur3}{\fad(00,40)}Apa-apaan ini?
Dialogue: 0,0:06:35.47,0:06:36.60,Default,,0,0,0,,{\blur3}{\fad(00,40)}Penghalang?
Dialogue: 0,0:06:41.69,0:06:43.57,Default,,0,0,0,,{\blur3}{\fad(00,40)}Sekolah zaman sekarang menakutkan.
Dialogue: 0,0:06:43.65,0:06:44.48,Default,,0,0,0,,{\blur3}{\fad(00,40)}Hei,
Dialogue: 0,0:06:44.57,0:06:45.40,Default,,0,0,0,,{\blur3}{\fad(00,40)}gunakan "Berjalan Tanpa Cedera"!
Dialogue: 0,0:06:46.40,0:06:47.49,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Siapa yang tahu caranya?\N- Siapa yang tahu caranya?
Dialogue: 0,0:06:50.74,0:06:52.37,Default,,0,0,0,,{\blur3}{\fad(00,40)}Pak, kau tidak mau pergi?
Dialogue: 0,0:06:52.45,0:06:54.62,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kau tidak mau imbalannya?
Dialogue: 0,0:06:54.70,0:06:56.45,Default,,0,0,0,,{\blur3}{\fad(00,40)}Bodoh, siapa yang mau terlibat\Ndalam hal itu?
Dialogue: 0,0:06:56.54,0:06:57.87,Default,,0,0,0,,{\blur3}{\fad(00,40)}Pak Sakata!
Dialogue: 0,0:06:57.96,0:06:59.67,Default,,0,0,0,,{\blur3}{\fad(00,40)}Para perampok ada\Ndi dekat gerbang belakang.
Dialogue: 0,0:06:59.75,0:07:02.67,Default,,0,0,0,,{\blur3}{\fad(00,40)}Itu yang terdekat dari tempatmu sekarang!
Dialogue: 0,0:07:02.75,0:07:04.63,Default,,0,0,0,,{\blur3}{\fad(00,40)}Cepat bergerak!
Dialogue: 0,0:07:06.05,0:07:09.22,Default,,0,0,0,,{\blur3}{\fad(00,40)}Omong-omong, aku guru sekolah sekarang.
Dialogue: 0,0:07:09.30,0:07:11.64,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jika Gin dari Yorozuya,\Nmungkin dia bisa melakukannya.
Dialogue: 0,0:07:11.72,0:07:13.97,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tapi bagaimana bisa seorang guru\Nmengalahkan perampok?
Dialogue: 0,0:07:14.05,0:07:17.06,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku mengerti
Dialogue: 0,0:07:17.14,0:07:18.60,Default,,0,0,0,,{\blur3}{\fad(00,40)}latar dan kondisi ini.
Dialogue: 0,0:07:18.68,0:07:21.94,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jika tidak pergi, kau dipecat!
Dialogue: 0,0:07:22.02,0:07:24.36,Default,,0,0,0,,{\blur3}{\fad(00,40)}Baiklah.
Dialogue: 0,0:07:28.03,0:07:31.95,Default,,0,0,0,,{\blur3}{\fad(00,40)}Sebagai balasannya, aku akan mendapatkan\Nsetiap sen imbalannya.
Dialogue: 0,0:07:32.99,0:07:35.37,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ayo cari tempat bersembunyi.
Dialogue: 0,0:07:35.45,0:07:36.58,Default,,0,0,0,,{\blur3}{\fad(00,40)}Selain Ruang Pemanas,
Dialogue: 0,0:07:36.66,0:07:39.08,Default,,0,0,0,,{\blur3}{\fad(00,40)}apa ada tempat lain di sekolah\Nuntuk kita sembunyi?
Dialogue: 0,0:07:39.66,0:07:41.67,Default,,0,0,0,,{\blur3}{\fad(00,40)}Bisakah kita masuk ke kotak kisi-kisi?
Dialogue: 0,0:07:41.75,0:07:43.00,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tentu saja tidak…
Dialogue: 0,0:07:43.08,0:07:44.33,Default,,0,0,0,,{\blur3}{\fad(00,40)}Yang benar saja.
Dialogue: 0,0:07:44.42,0:07:46.25,Default,,0,0,0,,{\blur3}{\fad(00,40)}Apa aku sungguh yang pertama?
Dialogue: 0,0:07:47.25,0:07:48.59,Default,,0,0,0,,{\blur3}{\fad(00,40)}Halo.
Dialogue: 0,0:07:49.51,0:07:52.43,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kakak, kita harus bagaimana…
Dialogue: 0,0:07:52.51,0:07:53.34,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kakak, apa…
Dialogue: 0,0:07:53.43,0:07:54.85,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tenanglah, Adik-Adikku.
Dialogue: 0,0:07:55.43,0:07:57.68,Default,,0,0,0,,{\blur3}{\fad(00,40)}Hei, Semuanya.
Dialogue: 0,0:07:57.76,0:08:00.85,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jika merampok bank,\Nsetidaknya rambutnya jangan mencolok.
Dialogue: 0,0:08:00.93,0:08:02.14,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kalian punya sanggul pria.
Dialogue: 0,0:08:02.23,0:08:04.23,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jangan meremehkan sanggul pria!
Dialogue: 0,0:08:04.31,0:08:05.69,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ada apa dengan sanggul pria?
Dialogue: 0,0:08:05.77,0:08:08.57,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ya, ini terlihat sangat keren\Nsetelah beberapa lama.
Dialogue: 0,0:08:08.65,0:08:11.24,Default,,0,0,0,,{\blur3}{\fad(00,40)}Setidaknya satu dari kalian\Nharus coba yang berbeda.
Dialogue: 0,0:08:11.32,0:08:13.45,Default,,0,0,0,,{\blur3}{\fad(00,40)}Seperti menaruh éclair di atasnya.
Dialogue: 0,0:08:13.53,0:08:15.03,Default,,0,0,0,,{\blur3}{\fad(00,40)}Itu konyol.
Dialogue: 0,0:08:15.12,0:08:16.03,Default,,0,0,0,,{\blur3}{\fad(00,40)}Agar tak mirip penjahat,
Dialogue: 0,0:08:16.12,0:08:17.45,Default,,0,0,0,,{\blur3}{\fad(00,40)}tapi pencinta makanan manis?
Dialogue: 0,0:08:21.00,0:08:23.00,Default,,0,0,0,,{\blur3}{\fad(00,40)}Maaf, Pak Sakata.
Dialogue: 0,0:08:23.58,0:08:26.08,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku akan mengambil satu juta itu.
Dialogue: 0,0:08:26.42,0:08:27.88,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tidak, yang lebih penting,
Dialogue: 0,0:08:27.96,0:08:30.21,Default,,0,0,0,,{\blur3}{\fad(00,40)}bisakah kau menang? Mereka bertiga.
Dialogue: 0,0:08:30.30,0:08:31.34,Default,,0,0,0,,{\blur3}{\fad(00,40)}Lucu sekali.
Dialogue: 0,0:08:31.42,0:08:34.47,Default,,0,0,0,,{\blur3}{\fad(00,40)}Dengan latar saat ini, aku memang guru SMA.
Dialogue: 0,0:08:34.54,0:08:38.10,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tapi di cerita utama,\Naku dikenal sebagai "Marishiten".
Dialogue: 0,0:08:38.17,0:08:39.89,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku bisa membantai perampok
Dialogue: 0,0:08:39.97,0:08:42.42,Default,,0,0,0,,{\blur3}{\fad(00,40)}sekaliber ini dalam sekejap.
Dialogue: 0,0:08:43.02,0:08:44.65,Default,,0,0,0,,{\blur3}{\fad(00,40)}Sudah kubilang,
Dialogue: 0,0:08:44.73,0:08:45.69,Default,,0,0,0,,{\blur3}{\fad(00,40)}itu latar cerita utamanya…
Dialogue: 0,0:08:47.40,0:08:49.23,Default,,0,0,0,,{\blur3}{\fad(00,40)}Itu…
Dialogue: 0,0:08:49.32,0:08:50.15,Default,,0,0,0,,{\blur3}{\fad(00,40)}obat wasir.
Dialogue: 0,0:08:50.23,0:08:52.57,Default,,0,0,0,,{\blur3}{\fad(00,40)}Yang benar saja! Ini kapur!
Dialogue: 0,0:08:52.65,0:08:54.07,Default,,0,0,0,,{\blur3}{\fad(00,40)}Lari, Adik-Adikku!
Dialogue: 0,0:08:54.95,0:08:56.03,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kalian takkan lolos!
Dialogue: 0,0:08:56.74,0:08:57.74,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aduh…
Dialogue: 0,0:08:59.12,0:09:00.54,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Mustahil itu berhasil!\N- Mustahil itu berhasil!
Dialogue: 0,0:09:00.62,0:09:03.37,Default,,0,0,0,,{\blur3}{\fad(00,40)}Saat kelas delapan,\Nkupikir para guru itu sampah.
Dialogue: 0,0:09:03.46,0:09:05.00,Default,,0,0,0,,{\blur3}{\fad(00,40)}Sepertinya aku benar!
Dialogue: 0,0:09:05.67,0:09:07.79,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kapur itu sebenarnya umpan.
Dialogue: 0,0:09:10.46,0:09:12.76,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku menangkap perampoknya.
Dialogue: 0,0:09:26.94,0:09:31.98,Default,,0,0,0,,{\blur3}{\fad(00,40)}Wasir!
Dialogue: 0,0:09:33.15,0:09:34.95,Default,,0,0,0,,{\blur3}{\fad(00,40)}Dasar bodoh. Kau bodoh.
Dialogue: 0,0:09:35.40,0:09:37.28,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ayo lari sekarang!
Dialogue: 0,0:09:37.70,0:09:39.62,Default,,0,0,0,,{\blur3}{\fad(00,40)}Hei, ayo sembunyi di sana.
Dialogue: 0,0:09:39.70,0:09:40.53,Default,,0,0,0,,{\blur3}{\fad(00,40)}Baiklah.
Dialogue: 0,0:09:41.37,0:09:42.41,Default,,0,0,0,,{\blur3}{\fad(00,40)}Sial.
Dialogue: 0,0:09:42.49,0:09:44.50,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ada apa dengan guru itu?
Dialogue: 0,0:09:46.67,0:09:48.17,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ayo lari.
Dialogue: 0,0:09:48.25,0:09:49.63,Default,,0,0,0,,{\blur3}{\fad(00,40)}Berhenti!
Dialogue: 0,0:09:50.04,0:09:52.55,Default,,0,0,0,,{\blur3}{\fad(00,40)}Hei, kalian! Jangan pergi!
Dialogue: 0,0:09:54.51,0:09:55.72,Default,,0,0,0,,{\blur3}{\fad(00,40)}Maaf, Ginpachi.
Dialogue: 0,0:09:55.80,0:09:56.76,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku akan mengambil
Dialogue: 0,0:09:56.84,0:09:58.93,Default,,0,0,0,,{\blur3}{\fad(00,40)}satu juta itu.
Dialogue: 0,0:09:59.80,0:10:02.26,Default,,0,0,0,,{\blur3}{\fad(00,40)}Hei, Pak Tua. Kotak apa itu?
Dialogue: 0,0:10:02.76,0:10:05.06,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ini senjataku.
Dialogue: 0,0:10:05.48,0:10:06.31,Default,,0,0,0,,{\blur3}{\fad(00,40)}Begini,
Dialogue: 0,0:10:06.39,0:10:11.73,Default,,0,0,0,,{\blur3}{\fad(00,40)}kau seharusnya menonton\NDenjiro atau Zenjiro itu di TV, kan?
Dialogue: 0,0:10:11.82,0:10:12.69,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ada lubang
Dialogue: 0,0:10:12.77,0:10:13.61,Default,,0,0,0,,{\blur3}{\fad(00,40)}di kotak ini.
Dialogue: 0,0:10:13.69,0:10:17.11,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jika kau pukul di samping,\Nini menembakkan peluru udara.
Dialogue: 0,0:10:17.20,0:10:19.91,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ini yang disebut meriam udara.
Dialogue: 0,0:10:19.99,0:10:21.45,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku pernah melihatnya.
Dialogue: 0,0:10:21.53,0:10:24.41,Default,,0,0,0,,{\blur3}{\fad(00,40)}Lingkaran asap akan keluar dari kotak.
Dialogue: 0,0:10:24.49,0:10:25.62,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku juga pernah melihatnya.
Dialogue: 0,0:10:25.70,0:10:27.79,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kau menontonnya bersama kami, bukan, Kakak?
Dialogue: 0,0:10:27.87,0:10:28.83,Default,,0,0,0,,{\blur3}{\fad(00,40)}Oh, itu.
Dialogue: 0,0:10:29.92,0:10:33.17,Default,,0,0,0,,{\blur3}{\fad(00,40)}Dan ini versi modifikasinya.
Dialogue: 0,0:10:33.25,0:10:37.97,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jika terkena udara terkompresi,\Nkau akan terlempar sejauh sepuluh meter.
Dialogue: 0,0:10:38.05,0:10:38.88,Default,,0,0,0,,{\blur3}{\fad(00,40)}Bukan.
Dialogue: 0,0:10:38.97,0:10:40.47,Default,,0,0,0,,{\blur3}{\fad(00,40)}Mungkin sampai 11 meter.
Dialogue: 0,0:10:40.55,0:10:42.55,Default,,0,0,0,,{\blur3}{\fad(00,40)}Atau sembilan meter.
Dialogue: 0,0:10:42.64,0:10:44.52,Default,,0,0,0,,{\blur3}{\fad(00,40)}Itu terlalu spesifik. Masa bodoh.
Dialogue: 0,0:10:46.10,0:10:48.81,Default,,0,0,0,,{\blur3}{\fad(00,40)}Meriam udaraku!
Dialogue: 0,0:10:59.11,0:11:00.82,Default,,0,0,0,,{\blur3}{\fad(00,40)}Apa-apaan ini, Pak Tua?
Dialogue: 0,0:11:01.24,0:11:03.33,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kau mengarahkan meriam ke dirimu sendiri!
Dialogue: 0,0:11:03.66,0:11:08.50,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ini… jebakan yang tidak terduga.
Dialogue: 0,0:11:08.58,0:11:12.13,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jebakan apa? Arah meriamnya salah!
Dialogue: 0,0:11:14.92,0:11:16.09,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku tidak punya pilihan.
Dialogue: 0,0:11:16.51,0:11:19.01,Default,,0,0,0,,{\blur3}{\fad(00,40)}Meraunglah, meriam petirku!
Dialogue: 0,0:11:28.14,0:11:29.14,Default,,0,0,0,,{\blur3}{\fad(00,40)}Apa ini?
Dialogue: 0,0:11:29.69,0:11:33.06,Default,,0,0,0,,{\blur3}{\fad(00,40)}Omong-omong, tembakan kedua dirancang\Nuntuk menyemprotkan kecap.
Dialogue: 0,0:11:34.40,0:11:36.44,Default,,0,0,0,,{\blur3}{\fad(00,40)}Desain itu tidak perlu!
Dialogue: 0,0:11:36.53,0:11:38.36,Default,,0,0,0,,{\blur3}{\fad(00,40)}Omong-omong,\Ntembakan keempat juga menyemprotkan kecap.
Dialogue: 0,0:11:38.44,0:11:41.95,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku akan menyingkirkanmu\Nsebelum itu menyemprotkan kecap!
Dialogue: 0,0:11:42.03,0:11:43.62,Default,,0,0,0,,{\blur3}{\fad(00,40)}Untungnya, dia bodoh.
Dialogue: 0,0:12:01.68,0:12:03.84,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku tersesat di lorong\Ndan tidak tahu apa-apa.
Dialogue: 0,0:12:03.93,0:12:07.43,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tapi perampok bank yang mencuri 50 juta\Nada di sekolah kita.
Dialogue: 0,0:12:07.51,0:12:10.43,Default,,0,0,0,,{\blur3}{\fad(00,40)}Bukan begitu, Kintoki?
Dialogue: 0,0:12:12.23,0:12:15.11,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kapan kau akan mengingat namaku?
Dialogue: 0,0:12:15.19,0:12:18.07,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku Ginpachi sekarang,\Njadi, semua suku katanya salah.
Dialogue: 0,0:12:18.65,0:12:21.32,Default,,0,0,0,,{\blur3}{\fad(00,40)}Pokoknya, bantu aku.\NKita akan bekerja sama lawan mereka.
Dialogue: 0,0:12:22.07,0:12:24.03,Default,,0,0,0,,{\blur3}{\fad(00,40)}Bagaimana dengan ini?
Dialogue: 0,0:12:24.36,0:12:27.45,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku akan membeli tas itu seharga 60 juta.
Dialogue: 0,0:12:27.53,0:12:31.33,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tinggalkan di sini dan keluar dari sekolah.
Dialogue: 0,0:12:31.41,0:12:32.58,Default,,0,0,0,,{\blur3}{\fad(00,40)}Bagaimana menurutmu?
Dialogue: 0,0:12:32.71,0:12:34.71,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kau akan membelinya seharga 60 juta?
Dialogue: 0,0:12:34.79,0:12:36.59,Default,,0,0,0,,{\blur3}{\fad(00,40)}Apa maksudnya?
Dialogue: 0,0:12:36.67,0:12:38.09,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kenapa kau melakukan ini…
Dialogue: 0,0:12:38.17,0:12:41.72,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku hanya ingin kalian pergi dengan tenang.
Dialogue: 0,0:12:42.72,0:12:44.30,Default,,0,0,0,,{\blur3}{\fad(00,40)}Biar kujelaskan.
Dialogue: 0,0:12:44.38,0:12:46.39,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ini sepuluh juta.
Dialogue: 0,0:12:46.47,0:12:49.10,Default,,0,0,0,,{\blur3}{\fad(00,40)}Itu artinya ada 60 juta di sini.
Dialogue: 0,0:12:49.77,0:12:50.77,Default,,0,0,0,,{\blur3}{\fad(00,40)}Benar?
Dialogue: 0,0:12:52.18,0:12:53.31,Default,,0,0,0,,{\blur3}{\fad(00,40)}Benar.
Dialogue: 0,0:12:53.39,0:12:55.81,Default,,0,0,0,,{\blur3}{\fad(00,40)}Dan untuk 60 juta,
Dialogue: 0,0:12:55.90,0:12:57.52,Default,,0,0,0,,{\blur3}{\fad(00,40)}aku akan membeli tas yang kau pegang.
Dialogue: 0,0:12:57.61,0:12:59.36,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kau mengerti?
Dialogue: 0,0:12:59.73,0:13:01.11,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ya…
Dialogue: 0,0:13:01.32,0:13:03.99,Default,,0,0,0,,{\blur3}{\fad(00,40)}Sisanya sederhana.
Dialogue: 0,0:13:04.53,0:13:06.99,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku akan mengambil tas yang kubeli,
Dialogue: 0,0:13:07.07,0:13:09.62,Default,,0,0,0,,{\blur3}{\fad(00,40)}dan kalian akan pergi dengan sepuluh juta.
Dialogue: 0,0:13:09.70,0:13:11.37,Default,,0,0,0,,{\blur3}{\fad(00,40)}Begitulah.
Dialogue: 0,0:13:13.33,0:13:16.79,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tidak, tunggu. Itu tidak benar, itu aneh.
Dialogue: 0,0:13:16.88,0:13:20.30,Default,,0,0,0,,{\blur3}{\fad(00,40)}Entah kenapa, tapi aku merasa kita rugi.
Dialogue: 0,0:13:20.38,0:13:24.09,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Ya, benar.\N- Jika kau tidak bisa berhitung,
Dialogue: 0,0:13:24.17,0:13:27.26,Default,,0,0,0,,{\blur3}{\fad(00,40)}kau harus kembali ke SMP.
Dialogue: 0,0:13:27.85,0:13:31.81,Default,,0,0,0,,{\blur3}{\fad(00,40)}Sebutkan rumus luas permukaan bola.
Dialogue: 0,0:13:32.22,0:13:34.10,Default,,0,0,0,,{\blur3}{\fad(00,40)}Bola?
Dialogue: 0,0:13:34.18,0:13:35.06,Default,,0,0,0,,{\blur3}{\fad(00,40)}Rumusnya?
Dialogue: 0,0:13:35.14,0:13:36.90,Default,,0,0,0,,{\blur3}{\fad(00,40)}Bukankah itu tentang angka sembilan?
Dialogue: 0,0:13:38.36,0:13:41.65,Default,,0,0,0,,{\blur3}{\fad(00,40)}Setelah bertahun-tahun\Nmengikuti pendidikan wajib,
Dialogue: 0,0:13:41.73,0:13:45.15,Default,,0,0,0,,{\blur3}{\fad(00,40)}kalian sangat menyedihkan.
Dialogue: 0,0:13:45.24,0:13:48.57,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tidak apa-apa. Kintoki akan menjawab.
Dialogue: 0,0:13:48.66,0:13:50.28,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tidak, kenapa aku?
Dialogue: 0,0:13:50.37,0:13:51.41,Default,,0,0,0,,{\blur3}{\fad(00,40)}Seharusnya kau.
Dialogue: 0,0:13:51.49,0:13:52.87,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kau yang bertanya.
Dialogue: 0,0:13:52.95,0:13:56.58,Default,,0,0,0,,{\blur3}{\fad(00,40)}Karena kau tidak bisa menjawab?
Dialogue: 0,0:13:57.08,0:13:58.54,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tentu saja tidak.
Dialogue: 0,0:13:58.79,0:13:59.79,Default,,0,0,0,,{\blur3}{\fad(00,40)}Itu karena
Dialogue: 0,0:13:59.88,0:14:02.30,Default,,0,0,0,,{\blur3}{\fad(00,40)}saat bola diajarkan,
Dialogue: 0,0:14:02.38,0:14:04.09,Default,,0,0,0,,{\blur3}{\fad(00,40)}ayahku dipindahkan dan kami pindah.
Dialogue: 0,0:14:04.17,0:14:07.51,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku melewatkan pelajaran itu.
Dialogue: 0,0:14:10.05,0:14:13.56,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kintoki, kau sangat payah\Ndalam membuat alasan.
Dialogue: 0,0:14:13.64,0:14:15.14,Default,,0,0,0,,{\blur3}{\fad(00,40)}Apa jawaban yang benar?
Dialogue: 0,0:14:17.19,0:14:18.73,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ini pertanyaan kedua.
Dialogue: 0,0:14:18.81,0:14:20.81,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kau sendiri mungkin tidak tahu jawabannya!
Dialogue: 0,0:14:22.11,0:14:24.07,Default,,0,0,0,,{\blur3}{\fad(00,40)}4πr kuadrat.
Dialogue: 0,0:14:24.15,0:14:27.11,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aduh…
Dialogue: 0,0:14:34.62,0:14:35.54,Default,,0,0,0,,{\blur3}{\fad(00,40)}Pak tua.
Dialogue: 0,0:14:35.62,0:14:36.75,Default,,0,0,0,,{\blur3}{\fad(00,40)}Pak Sakata.
Dialogue: 0,0:14:36.83,0:14:39.46,Default,,0,0,0,,{\blur3}{\fad(00,40)}Terima kasih sudah memojokkan mereka.
Dialogue: 0,0:14:39.83,0:14:43.05,Default,,0,0,0,,{\blur3}{\fad(00,40)}Namun, sekarang giliranku.
Dialogue: 0,0:14:45.46,0:14:47.51,Default,,0,0,0,,{\blur3}{\fad(00,40)}Dengar, Perampok!
Dialogue: 0,0:14:47.59,0:14:49.76,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tidak ada jalan keluar!
Dialogue: 0,0:14:49.84,0:14:51.43,Default,,0,0,0,,{\blur3}{\fad(00,40)}Maaf, tapi karena aku di sini,
Dialogue: 0,0:14:51.51,0:14:53.47,Default,,0,0,0,,{\blur3}{\fad(00,40)}akan kuhajar kalian habis-habisan\Ndalam sekejap
Dialogue: 0,0:14:53.56,0:14:55.52,Default,,0,0,0,,{\blur3}{\fad(00,40)}dan mengalahkan kalian!
Dialogue: 0,0:14:55.60,0:14:58.60,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jika tidak mau dihajar,\Nterimalah nasib kalian!
Dialogue: 0,0:14:58.69,0:15:00.94,Default,,0,0,0,,{\blur3}{\fad(00,40)}Minta maaf dan menyerahlah!
Dialogue: 0,0:15:01.52,0:15:02.69,Default,,0,0,0,,{\blur3}{\fad(00,40)}Berisik sekali!
Dialogue: 0,0:15:02.77,0:15:05.73,Default,,0,0,0,,{\blur3}{\fad(00,40)}Suara alamimu sudah cukup kuat, bukan?
Dialogue: 0,0:15:09.28,0:15:13.08,Default,,0,0,0,,{\blur3}{\fad(00,40)}Meriam Komatsu, Tipe Portabel.
Dialogue: 0,0:15:14.12,0:15:15.83,Default,,0,0,0,,{\blur3}{\fad(00,40)}Itu berlebihan!
Dialogue: 0,0:15:15.91,0:15:17.96,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ini seharusnya cerita sekolah!
Dialogue: 0,0:15:18.04,0:15:21.63,Default,,0,0,0,,{\blur3}{\fad(00,40)}Pria tua tidak mengerti latar.
Dialogue: 0,0:15:21.71,0:15:22.92,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kau sama sekali tidak manis.
Dialogue: 0,0:15:23.42,0:15:26.26,Default,,0,0,0,,{\blur3}{\fad(00,40)}Belajarlah dari Pak Gengai,\Nyang mati mengikuti latar!
Dialogue: 0,0:15:27.51,0:15:29.26,Default,,0,0,0,,{\blur3}{\fad(00,40)}Di siang hari, ada bazoka di sini.
Dialogue: 0,0:15:29.34,0:15:31.68,Default,,0,0,0,,{\blur3}{\fad(00,40)}Di malam hari,\Napa ada bazoka di bawah sana?
Dialogue: 0,0:15:31.76,0:15:34.72,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ini buruk. Pria tua ini\Nmulai membuat lelucon jorok!
Dialogue: 0,0:15:35.89,0:15:37.22,Default,,0,0,0,,{\blur3}{\fad(00,40)}Hei, tunggu sebentar!
Dialogue: 0,0:15:37.31,0:15:39.18,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kenapa kau juga membidikku?
Dialogue: 0,0:15:39.27,0:15:41.56,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jangan khawatir.
Dialogue: 0,0:15:41.65,0:15:43.15,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tembak!
Dialogue: 0,0:15:43.23,0:15:44.98,Default,,0,0,0,,{\blur3}{\fad(00,40)}Gemuruh…
Dialogue: 0,0:15:45.07,0:15:47.15,Default,,0,0,0,,{\blur3}{\fad(00,40)}Begitukah cara menggunakannya?
Dialogue: 0,0:15:47.23,0:15:49.32,Default,,0,0,0,,{\blur3}{\fad(00,40)}Rasakan ini!
Dialogue: 0,0:15:54.74,0:15:56.54,Default,,0,0,0,,{\blur3}{\fad(00,40)}Sekarang kesempatan kita kabur!
Dialogue: 0,0:16:00.33,0:16:01.62,Default,,0,0,0,,{\blur3}{\fad(00,40)}Bedebah, berhenti!
Dialogue: 0,0:16:03.54,0:16:05.50,Default,,0,0,0,,{\blur3}{\fad(00,40)}Bazoka? Lucu sekali.
Dialogue: 0,0:16:05.59,0:16:08.92,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ke mana pun kau pergi,\Nkau harus bertarung di tanah.
Dialogue: 0,0:16:09.01,0:16:11.38,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ini yang disebut bunga tanah.
Dialogue: 0,0:16:11.47,0:16:12.43,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tsukuyo.
Dialogue: 0,0:16:14.89,0:16:18.39,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tolong jangan abaikan latarnya.
Dialogue: 0,0:16:18.47,0:16:19.52,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tentu saja.
Dialogue: 0,0:16:19.60,0:16:24.44,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku akan bertarung sesuai latar\Nguru Kesehatan dan Olahraga.
Dialogue: 0,0:16:27.57,0:16:29.65,Default,,0,0,0,,{\blur3}{\fad(00,40)}Begitu memasuki masa pubertas,
Dialogue: 0,0:16:29.74,0:16:32.15,Default,,0,0,0,,{\blur3}{\fad(00,40)}sekresi hormon seks menjadi aktif.
Dialogue: 0,0:16:32.24,0:16:34.91,Default,,0,0,0,,{\blur3}{\fad(00,40)}Payudara wanita mulai berkembang,
Dialogue: 0,0:16:35.70,0:16:38.37,Default,,0,0,0,,{\blur3}{\fad(00,40)}dan seluruh tubuh mereka\Nmenjadi bulat dan penuh.
Dialogue: 0,0:16:38.45,0:16:40.33,Default,,0,0,0,,{\blur3}{\fad(00,40)}Pinggang mereka menjadi berlekuk,
Dialogue: 0,0:16:40.41,0:16:42.92,Default,,0,0,0,,{\blur3}{\fad(00,40)}berkembang menjadi tubuh\Nyang lebih feminin.
Dialogue: 0,0:16:44.79,0:16:46.88,Default,,0,0,0,,{\blur3}{\fad(00,40)}Untuk apa periode waktu ini?
Dialogue: 0,0:16:46.96,0:16:48.38,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kenapa kau tiba-tiba mulai mengajar?
Dialogue: 0,0:16:48.46,0:16:53.38,Default,,0,0,0,,{\blur3}{\fad(00,40)}Lihat, para perampok lupa lari\Ndan menggeliat.
Dialogue: 0,0:16:53.47,0:16:55.05,Default,,0,0,0,,{\blur3}{\fad(00,40)}Serangan seperti ini sangat membosankan!
Dialogue: 0,0:16:55.14,0:16:56.60,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kita harus apa selanjutnya?
Dialogue: 0,0:16:58.35,0:17:00.56,Default,,0,0,0,,{\blur3}{\fad(00,40)}Meriam Komatsu, Tipe Portabel.
Dialogue: 0,0:17:00.64,0:17:02.02,Default,,0,0,0,,{\blur3}{\fad(00,40)}Rasakan ini!
Dialogue: 0,0:17:02.10,0:17:04.56,Default,,0,0,0,,{\blur3}{\fad(00,40)}Setelah semua itu, malah sama saja!
Dialogue: 0,0:17:04.65,0:17:06.65,Default,,0,0,0,,{\blur3}{\fad(00,40)}Hei, jangan melambaikannya!
Dialogue: 0,0:17:16.16,0:17:18.49,Default,,0,0,0,,{\blur3}{\fad(00,40)}Apa yang kau lakukan?
Dialogue: 0,0:17:19.54,0:17:21.29,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aduh…
Dialogue: 0,0:17:21.75,0:17:23.75,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ada apa dengan sekolah ini?
Dialogue: 0,0:17:23.82,0:17:25.54,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kita harus bagaimana, Kakak?
Dialogue: 0,0:17:26.29,0:17:28.71,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kalau begitu…
Dialogue: 0,0:17:31.67,0:17:33.59,Default,,0,0,0,,{\blur3}{\fad(00,40)}Berapa lama kita harus di sini?
Dialogue: 0,0:17:33.67,0:17:35.09,Default,,0,0,0,,{\blur3}{\fad(00,40)}Menyebalkan sekali.
Dialogue: 0,0:17:35.18,0:17:37.43,Default,,0,0,0,,{\blur3}{\fad(00,40)}Selesaikan ini dengan cepat.
Dialogue: 0,0:17:41.43,0:17:42.56,Default,,0,0,0,,{\blur3}{\fad(00,40)}Orang-orang itu…
Dialogue: 0,0:17:42.64,0:17:44.10,Default,,0,0,0,,{\blur3}{\fad(00,40)}Mereka punya sanggul pria\Nseperti di poster buronan.
Dialogue: 0,0:17:44.18,0:17:45.69,Default,,0,0,0,,{\blur3}{\fad(00,40)}Pe… Perampok…
Dialogue: 0,0:17:48.52,0:17:50.57,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jangan bergerak!
Dialogue: 0,0:17:53.57,0:17:56.99,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kami mengambil alih sekolah ini.
Dialogue: 0,0:17:59.91,0:18:02.37,Default,,0,0,0,,{\blur3}{\fad(00,40)}Mereka punya senjata!
Dialogue: 0,0:18:02.45,0:18:06.33,Default,,0,0,0,,{\blur3}{\fad(00,40)}Apa yang perlu ditakutkan,\NPak Kepala Pelayan?
Dialogue: 0,0:18:06.67,0:18:10.21,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kenapa kita kabur\Nke tempat yang merepotkan lagi?
Dialogue: 0,0:18:12.63,0:18:13.67,Default,,0,0,0,,{\blur3}{\fad(00,40)}Pak Ginpachi!
Dialogue: 0,0:18:13.76,0:18:14.92,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ginpachi!
Dialogue: 0,0:18:15.38,0:18:18.14,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kau pria yang merepotkan.
Dialogue: 0,0:18:18.22,0:18:21.64,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jika kau gegabah,\Nmurid-muridmu yang manis akan terluka.
Dialogue: 0,0:18:22.14,0:18:23.35,Default,,0,0,0,,{\blur3}{\fad(00,40)}Mari mulai.
Dialogue: 0,0:18:23.43,0:18:24.85,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tidak kusangka kau hebat.
Dialogue: 0,0:18:24.93,0:18:27.23,Default,,0,0,0,,{\blur3}{\fad(00,40)}Akting penjahatnya juga cukup bagus.
Dialogue: 0,0:18:27.90,0:18:31.06,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kukira kalian\Nhanya sekelompok pria tua ceroboh.
Dialogue: 0,0:18:32.94,0:18:35.74,Default,,0,0,0,,{\blur3}{\fad(00,40)}Sakata, jangan memprovokasi mereka!
Dialogue: 0,0:18:35.82,0:18:37.40,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jika terjadi sesuatu kepada para murid,
Dialogue: 0,0:18:37.49,0:18:39.32,Default,,0,0,0,,{\blur3}{\fad(00,40)}peringkat SMA populer tidak penting!
Dialogue: 0,0:18:39.41,0:18:41.45,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jadi, jangan memprovokasi mereka!
Dialogue: 0,0:18:42.28,0:18:45.29,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jangan memprovokasi perampok, Pak Hada?
Dialogue: 0,0:18:45.62,0:18:48.25,Default,,0,0,0,,{\blur3}{\fad(00,40)}Bukankah kau menghasut kami\Nuntuk menangkap mereka?
Dialogue: 0,0:18:48.33,0:18:51.00,Default,,0,0,0,,{\blur3}{\fad(00,40)}Sekarang, kau mengatakan\Nhal-hal yang keras kepala.
Dialogue: 0,0:18:52.59,0:18:53.84,Default,,0,0,0,,{\blur3}{\fad(00,40)}Para Perampok!
Dialogue: 0,0:18:53.92,0:18:56.51,Default,,0,0,0,,{\blur3}{\fad(00,40)}Apa permintaan kalian?
Dialogue: 0,0:18:56.59,0:18:59.18,Default,,0,0,0,,{\blur3}{\fad(00,40)}Siapkan mobil pelarian untuk kami.
Dialogue: 0,0:18:59.26,0:19:00.47,Default,,0,0,0,,{\blur3}{\fad(00,40)}Selain itu,
Dialogue: 0,0:19:00.55,0:19:03.06,Default,,0,0,0,,{\blur3}{\fad(00,40)}hilangkan penghalang aneh itu\Nsekarang juga!
Dialogue: 0,0:19:03.14,0:19:04.52,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tapi…
Dialogue: 0,0:19:06.02,0:19:08.39,Default,,0,0,0,,{\blur3}{\fad(00,40)}Turuti perintahku!
Dialogue: 0,0:19:08.69,0:19:10.98,Default,,0,0,0,,{\blur3}{\fad(00,40)}Maafkan aku…
Dialogue: 0,0:19:11.06,0:19:12.86,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tunggu, akan segera kutangani!
Dialogue: 0,0:19:12.94,0:19:14.07,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kulakukan sekarang. Sebentar.
Dialogue: 0,0:19:14.15,0:19:15.90,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tunggu, Pak Hada.
Dialogue: 0,0:19:19.61,0:19:22.12,Default,,0,0,0,,{\blur3}{\fad(00,40)}Tidak perlu menghilangkan penghalangnya.
Dialogue: 0,0:19:22.20,0:19:24.79,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kita tidak bisa biarkan\Nmereka meninggalkan sekolah.
Dialogue: 0,0:19:24.87,0:19:26.16,Default,,0,0,0,,{\blur3}{\fad(00,40)}Namun…
Dialogue: 0,0:19:26.25,0:19:27.91,Default,,0,0,0,,{\blur3}{\fad(00,40)}Para murid dijadikan perisai manusia.
Dialogue: 0,0:19:28.00,0:19:30.17,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jika kita menyiapkan mobil\Nuntuk mereka kabur,
Dialogue: 0,0:19:30.25,0:19:34.34,Default,,0,0,0,,{\blur3}{\fad(00,40)}kita akan mendapat peringkat pertama\Ndi daftar "SMA Pecundang".
Dialogue: 0,0:19:36.84,0:19:39.05,Default,,0,0,0,,{\blur3}{\fad(00,40)}Ini akhir bagi kalian.
Dialogue: 0,0:19:40.59,0:19:41.84,Default,,0,0,0,,{\blur3}{\fad(00,40)}Karena aku pahlawannya.
Dialogue: 0,0:19:41.93,0:19:45.97,Default,,0,0,0,,{\blur3}{\fad(00,40)}Pahlawan semua orang,\NGinpachi, ada di sini!
Dialogue: 0,0:19:54.15,0:19:55.57,Default,,0,0,0,,{\blur3}{\fad(00,40)}Konyol sekali.
Dialogue: 0,0:19:55.65,0:19:58.53,Default,,0,0,0,,{\blur3}{\fad(00,40)}Apa yang bisa kau lakukan\Ndalam situasi begini?
Dialogue: 0,0:19:58.61,0:20:00.03,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kami ada banyak.
Dialogue: 0,0:20:01.20,0:20:05.33,Default,,0,0,0,,{\blur3}{\fad(00,40)}Walau kutembak dengan mata terpejam,\Naku pasti akan mengenai kalian.
Dialogue: 0,0:20:08.25,0:20:09.71,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kau pikir akan kubiarkan lolos?
Dialogue: 0,0:20:12.75,0:20:13.96,Default,,0,0,0,,{\blur3}{\fad(00,40)}Pak Ginpachi…
Dialogue: 0,0:20:14.92,0:20:18.01,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kau tidak menggertak, bukan?
Dialogue: 0,0:20:18.30,0:20:19.76,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kagura.
Dialogue: 0,0:20:19.84,0:20:22.63,Default,,0,0,0,,{\blur3}{\fad(00,40)}Shinpachi diam-diam\Nmemakan kombu cuka buatanmu.
Dialogue: 0,0:20:23.39,0:20:24.72,Default,,0,0,0,,{\blur3}{\fad(00,40)}Shinpachi, dasar berengsek!
Dialogue: 0,0:20:24.80,0:20:26.72,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Kapan kau makan?\N- Jangan bergerak!
Dialogue: 0,0:20:49.33,0:20:51.62,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku tidak bisa melihat.
Dialogue: 0,0:21:01.67,0:21:04.89,Default,,0,0,0,,{\blur3}{\fad(00,40)}Jangan bergerak, Berandal.
Dialogue: 0,0:21:04.97,0:21:06.97,Default,,0,0,0,,{\blur3}{\fad(00,40)}Menyerahlah sekarang.
Dialogue: 0,0:21:09.39,0:21:10.39,Default,,0,0,0,,{\blur3}{\fad(00,40)}Peraturan Kedelapan.
Dialogue: 0,0:21:10.85,0:21:14.85,Default,,0,0,0,,{\blur3}{\fad(00,40)}Mereka yang gaya rambutnya kacau\Nakan melakukan seppuku!
Dialogue: 0,0:21:17.65,0:21:19.82,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Maaf…\N- Maaf…
Dialogue: 0,0:21:20.03,0:21:22.28,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Ginpachi, kau luar biasa!\N- Luar biasa!
Dialogue: 0,0:21:22.36,0:21:24.91,Default,,0,0,0,,{\blur3}{\fad(00,40)}- Bagus, Ginpachi!\N- Kelas Z juga keren!
Dialogue: 0,0:21:40.50,0:21:41.34,Default,,0,0,0,,{\blur3}{\fad(00,40)}Bukan apa-apa.
Dialogue: 0,0:21:41.42,0:21:46.09,Default,,0,0,0,,{\blur3}{\fad(00,40)}Aku terlalu bersemangat\Nkarena perampoknya tertangkap.
Dialogue: 0,0:21:46.18,0:21:47.85,Default,,0,0,0,,{\blur3}{\fad(00,40)}Kau menghabiskannya?
Dialogue: 0,0:21:47.93,0:21:50.47,Default,,0,0,0,,{\blur3}{\fad(00,40)}Satu juta? Semuanya?
Dialogue: 0,0:21:52.85,0:21:54.94,Default,,0,0,0,,{\blur3}{\fad(00,40)}Maafkan aku.
Dialogue: 0,0:21:56.06,0:21:57.73,Default,,0,0,0,,{\blur3}{\fad(00,40)}Yang benar saja!