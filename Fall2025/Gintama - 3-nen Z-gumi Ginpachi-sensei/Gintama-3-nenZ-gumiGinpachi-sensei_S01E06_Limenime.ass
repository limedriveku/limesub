Dialogue: 0,0:00:17.85,0:00:20.85,tanda,,0000,0000,0000,,INGATLAH UNTUK MENJAGA KECERAHAN\NDAN JARAK YANG TEPAT SAAT MENONTON
Dialogue: 0,0:02:03.24,0:02:09.53,tanda,,0000,0000,0000,,GINTAMA - MR.GINPACHI'S ZANY CLASS
Dialogue: 0,0:03:21.95,0:03:26.41,tanda,,0000,0000,0000,,HARI-HARI DI KELAS 3-Z\NTIBA-TIBA BERAKHIR
Dialogue: 0,0:03:26.49,0:03:31.92,tanda,,0000,0000,0000,,HARI-HARI DI KELAS 3-Z\NTIBA-TIBA BERAKHIR
Dialogue: 0,0:03:33.88,0:03:35.13,tanda,,0000,0000,0000,,EPISODE 6
Dialogue: 0,0:03:35.21,0:03:36.96,tanda,,0000,0000,0000,,PINDAH SEKOLAH KARENA PEKERJAAN AYAH\NDAN SEMACAMNYA TAMPAK CUKUP MENARIK
Dialogue: 0,0:03:52.02,0:03:53.77,tanda,,0000,0000,0000,,"PLAYBOY MINGGUAN"
Dialogue: 0,0:04:06.83,0:04:09.04,tanda,,0000,0000,0000,,"SEHARUSNYA ITU CUKUP"
Dialogue: 0,0:06:00.65,0:06:01.94,tanda,,0000,0000,0000,,"SAMPAI JUMPA DI PENGADILAN"
Dialogue: 0,0:07:48.05,0:07:48.96,tanda,,0000,0000,0000,,"BORRAGINOL"
Dialogue: 0,0:08:32.47,0:08:34.01,tanda,,0000,0000,0000,,"TAMA"
Dialogue: 0,0:10:04.56,0:10:05.39,tanda,,0000,0000,0000,,"BERPIKIR KERAS"
Dialogue: 0,0:10:21.70,0:10:24.29,tanda,,0000,0000,0000,,"100 PUISI OLEH 100 PENYAIR"
Dialogue: 0,0:10:28.00,0:10:29.00,tanda,,0000,0000,0000,,"AKU TIDAK BISA MELAKUKAN ITU"
Dialogue: 0,0:11:35.48,0:11:37.94,tanda,,0000,0000,0000,,GINTAMA - MR.GINPACHI'S ZANY CLASS
Dialogue: 0,0:11:48.45,0:11:49.75,tanda,,0000,0000,0000,,PESTA PERPISAHAN BESAR\NPELUK AKU ERAT SEPERTI OMBAK ITU
Dialogue: 0,0:11:49.83,0:11:51.17,tanda,,0000,0000,0000,,"TIDAK BISA"
Dialogue: 0,0:12:06.72,0:12:08.31,tanda,,0000,0000,0000,,"S-GO, NAMA SAMARAN"
Dialogue: 0,0:12:08.39,0:12:09.98,tanda,,0000,0000,0000,,"SUARA DIUBAH UNTUK MENJAGA PRIVASI"
Dialogue: 0,0:12:20.99,0:12:23.95,tanda,,0000,0000,0000,,"SHUKU, NAMA SAMARAN"
Dialogue: 0,0:12:34.83,0:12:39.05,tanda,,0000,0000,0000,,"T-JOU, NAMA SAMARAN"
Dialogue: 0,0:13:16.88,0:13:18.79,tanda,,0000,0000,0000,,KENANGAN
Dialogue: 0,0:13:18.88,0:13:21.46,tanda,,0000,0000,0000,,LARI, TAE
Dialogue: 0,0:13:21.55,0:13:23.67,tanda,,0000,0000,0000,,PUKUL, TAE
Dialogue: 0,0:13:23.76,0:13:24.88,tanda,,0000,0000,0000,,TAE YANG MENCOLOK MEMASAK KARI
Dialogue: 0,0:13:24.97,0:13:26.18,tanda,,0000,0000,0000,,MARI MAKAN BERSAMA, SANGAT LEZAT
Dialogue: 0,0:13:26.26,0:13:27.22,tanda,,0000,0000,0000,,TAE SAAT PINDAH KELAS
Dialogue: 0,0:13:27.30,0:13:28.14,tanda,,0000,0000,0000,,TAE MEMAKAI BAJU RENANG
Dialogue: 0,0:13:28.22,0:13:30.14,tanda,,0000,0000,0000,,"TAE DI AREA GANTI SEPATU"
Dialogue: 0,0:13:51.29,0:13:52.20,tanda,,0000,0000,0000,,KACAMATA SHINPACHI
Dialogue: 0,0:13:52.29,0:13:53.87,tanda,,0000,0000,0000,,"DI PERMAINAN BOLA"
Dialogue: 0,0:13:53.95,0:13:56.17,tanda,,0000,0000,0000,,KACAMATA SHINPACHI DI DEKAT API UNGGUN\NSAAT KARYAWISATA SEKOLAH
Dialogue: 0,0:13:56.25,0:13:58.71,tanda,,0000,0000,0000,,KACAMATA SHINPACHI\NSAAT PIDATO PEMILIHAN KETUA OSIS
Dialogue: 0,0:13:58.79,0:14:00.04,tanda,,0000,0000,0000,,SEBENARNYA, BUKAN,\NITU KACAMATA HITAM HASEGAWA
Dialogue: 0,0:17:36.64,0:17:39.26,tanda,,0000,0000,0000,,"KAIT HIDUNG HEMAT"
Dialogue: 0,0:17:43.77,0:17:44.60,tanda,,0000,0000,0000,,AKU BISA MEMBERIMU PAPAN TANDAKU
Dialogue: 0,0:20:26.76,0:20:28.31,tanda,,0000,0000,0000,,"SHINPACHI, TAE, JAGA DIRI KALIAN"
Dialogue: 0,0:21:57.73,0:22:00.90,tanda,,0000,0000,0000,,"APA KAU BERCANDA"
Dialogue: 0,0:23:31.45,0:23:34.08,tanda,,0000,0000,0000,,TAKASUGI PEKAN INI
Dialogue: 0,0:23:56.39,0:23:57.68,tanda,,0000,0000,0000,,EPISODE 7
Dialogue: 0,0:23:57.77,0:23:59.89,tanda,,0000,0000,0000,,SANGAT HANYUT DALAM LOMPATAN LAMA HINGGA\NTIDAK ADA BANYAK KEMAJUAN BERSIH-BERSIH
Dialogue: 0,0:23:59.98,0:24:05.77,tanda,,0000,0000,0000,,IBU KANTIN SEKOLAH,\NSELAMAT ATAS PERNIKAHANMU! BERBAHAGIALAH!
Dialogue: 0,0:24:05.86,0:24:09.90,tanda,,0000,0000,0000,,EPISODE SELANJUTNYA,\NPRIA TANGGUH DATANG KE SMA GINTAMA
Dialogue: 0,0:00:02.37,0:00:04.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}"Dia yang tahu kapan harus bicara,"
Dialogue: 0,0:00:04.54,0:00:06.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}"juga tahu kapan harus diam."
Dialogue: 0,0:00:06.71,0:00:08.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kutipan dan pepatah terkenal
Dialogue: 0,0:00:08.67,0:00:10.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}sangat berguna, bukan?
Dialogue: 0,0:00:10.30,0:00:12.09,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Benar.
Dialogue: 0,0:00:12.17,0:00:14.89,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau tidak perlu mengatakannya keras-keras…
Dialogue: 0,0:00:14.97,0:00:16.43,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lupakan saja.
Dialogue: 0,0:00:24.14,0:00:25.35,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Shin.
Dialogue: 0,0:00:26.81,0:00:28.82,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ayo ke sekolah bersama hari ini.
Dialogue: 0,0:00:30.03,0:00:31.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bersama?
Dialogue: 0,0:00:38.24,0:00:40.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau bersenang-senang di sekolah?
Dialogue: 0,0:00:41.87,0:00:43.37,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Biasa saja.
Dialogue: 0,0:00:45.37,0:00:49.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Shin, kita akan pindah pekan depan.
Dialogue: 0,0:00:50.13,0:00:51.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maafkan aku.
Dialogue: 0,0:00:51.26,0:00:53.09,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku ingin bilang lebih awal,
Dialogue: 0,0:00:53.17,0:00:55.26,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tapi aku tidak bisa\Nmenemukan waktu yang tepat.
Dialogue: 0,0:00:56.09,0:00:59.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sebenarnya, setelah ayah kita meninggal,
Dialogue: 0,0:00:59.14,0:01:03.27,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}seorang bibi telah mengurus kita.
Dialogue: 0,0:01:03.35,0:01:04.31,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bibi?
Dialogue: 0,0:01:05.01,0:01:05.94,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya.
Dialogue: 0,0:01:06.02,0:01:12.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sepupu teman minum bos istri ketiga\Nkeponakan kakek kita.
Dialogue: 0,0:01:12.48,0:01:14.99,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau begitu, dia orang asing, bukan?
Dialogue: 0,0:01:15.74,0:01:17.11,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bibi itu bilang
Dialogue: 0,0:01:17.20,0:01:19.37,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dia khawatir kami tinggal sendirian
Dialogue: 0,0:01:19.45,0:01:22.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dan menyarankan kami tinggal bersama.
Dialogue: 0,0:01:25.50,0:01:28.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tae, saat kau bilang "pindah"…
Dialogue: 0,0:01:28.88,0:01:30.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}itu artinya kita akan pindah sekolah?
Dialogue: 0,0:01:31.80,0:01:32.84,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya.
Dialogue: 0,0:01:34.59,0:01:36.47,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau sudah bilang guru soal ini?
Dialogue: 0,0:01:36.55,0:01:37.89,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudah.
Dialogue: 0,0:01:37.97,0:01:40.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi aku belum memberi tahu\Nsemua orang di kelas.
Dialogue: 0,0:01:44.77,0:01:47.48,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kelas 3-Z.
Dialogue: 0,0:01:48.06,0:01:51.32,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}- Pak Ginpachi.
Dialogue: 0,0:03:37.38,0:03:38.55,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku memakannya!
Dialogue: 0,0:03:38.63,0:03:40.22,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dasar jalang!
Dialogue: 0,0:03:40.30,0:03:45.51,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}- Beraninya kau makan sosis guritaku?\N- Belum ada yang tahu aku akan pindah.
Dialogue: 0,0:03:46.31,0:03:50.85,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}- Ini pemandangan biasa setiap hari.\N- Haruskah kutaruh di sana, Elizabeth?
Dialogue: 0,0:03:50.93,0:03:51.94,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hei, siapa itu?
Dialogue: 0,0:03:52.02,0:03:53.77,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Siapa yang melempar penghapus? Hei!
Dialogue: 0,0:03:53.85,0:03:55.98,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berikutnya giliranku meminjamnya!
Dialogue: 0,0:03:56.06,0:03:58.28,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak, ini milikku!
Dialogue: 0,0:03:58.36,0:04:00.03,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}- Hei, semuanya robek!\N- Tapi bagiku,
Dialogue: 0,0:04:00.11,0:04:01.65,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}- Kenapa kau tidak mengoleskan pelumas?\N- momen ini
Dialogue: 0,0:04:01.74,0:04:03.41,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}- Kenapa kau tidak mengoleskan pelumas?\N- tiba-tiba menyenangkan.
Dialogue: 0,0:04:06.83,0:04:09.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Karena pekan depan…
Dialogue: 0,0:04:09.12,0:04:11.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}aku tidak akan ada di sini.
Dialogue: 0,0:04:12.33,0:04:14.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jika semua orang tahu aku akan pindah…
Dialogue: 0,0:04:17.04,0:04:18.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}- Pindah?
Dialogue: 0,0:04:20.17,0:04:23.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}kita akan berpisah. Menyedihkan sekali!\NShinpachi, aku tidak percaya
Dialogue: 0,0:04:23.72,0:04:25.34,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kenapa kau pindah?
Dialogue: 0,0:04:25.89,0:04:29.64,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Selamat tinggal, semuanya. Terima kasih.
Dialogue: 0,0:04:31.77,0:04:34.35,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalian berisik sekali.
Dialogue: 0,0:04:34.44,0:04:37.56,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Seolah wali kelasnya tidak akan masuk,\NBerengsek.
Dialogue: 0,0:04:37.90,0:04:39.11,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak.
Dialogue: 0,0:04:39.19,0:04:42.19,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Persis seperti yang baru saja kau katakan!
Dialogue: 0,0:04:43.82,0:04:47.70,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berdasarkan ejekan buruk itu, sudah jelas
Dialogue: 0,0:04:48.24,0:04:51.45,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Shinpachi sedang syok.
Dialogue: 0,0:04:52.12,0:04:53.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mengenai alasannya,
Dialogue: 0,0:04:54.04,0:04:55.62,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}karena itu, bukan?
Dialogue: 0,0:04:55.71,0:04:57.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Benar.
Dialogue: 0,0:04:57.71,0:04:59.34,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau ingin mengumumkannya sendiri?
Dialogue: 0,0:05:00.75,0:05:01.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya.
Dialogue: 0,0:05:15.89,0:05:19.02,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Begini… sebenarnya…
Dialogue: 0,0:05:19.11,0:05:22.94,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku dan kakakku\Nakan pindah pekan depan, jadi…
Dialogue: 0,0:05:23.61,0:05:25.65,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi, kami akan pindah sekolah.
Dialogue: 0,0:05:25.74,0:05:26.99,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maafkan aku.
Dialogue: 0,0:05:28.74,0:05:29.95,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baiklah…
Dialogue: 0,0:05:30.41,0:05:32.87,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hei, berhenti bercanda, Nona Tae!
Dialogue: 0,0:05:32.95,0:05:35.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tolong jangan pindah, ya?
Dialogue: 0,0:05:35.66,0:05:38.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku akan pindah menggantikanmu!
Dialogue: 0,0:05:38.13,0:05:40.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini bukan sesuatu yang bisa\Ndiselesaikan dengan pengganti.
Dialogue: 0,0:05:40.79,0:05:42.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sayang sekali,
Dialogue: 0,0:05:43.01,0:05:46.09,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tapi Shimura 1 dan 2 akan datang\Nke sekolah untuk kali terakhir pekan ini.
Dialogue: 0,0:05:47.18,0:05:50.18,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Omong-omong, pria berambut panjang\Nmenyebalkan itu juga pindah.
Dialogue: 0,0:05:50.26,0:05:52.60,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak, aku tidak pindah.
Dialogue: 0,0:05:52.68,0:05:54.14,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau begitu, bisakah kau berhenti sekolah?
Dialogue: 0,0:05:54.22,0:05:55.89,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak, aku mungkin akan menuntutmu.
Dialogue: 0,0:05:55.98,0:05:58.27,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sebenarnya, aku sudah melakukannya.
Dialogue: 0,0:05:58.35,0:06:00.56,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau harus terima panggilan pengadilan
Dialogue: 0,0:06:00.65,0:06:01.94,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}pekan depan.
Dialogue: 0,0:06:02.36,0:06:05.82,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Omong-omong,\Nrapat kelas pagi ini sudah selesai.
Dialogue: 0,0:06:05.90,0:06:06.90,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Murid Piket, beri aba-aba.
Dialogue: 0,0:06:06.99,0:06:07.91,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak, begini…
Dialogue: 0,0:06:07.99,0:06:10.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berdiri! Beri hormat!
Dialogue: 0,0:06:10.32,0:06:12.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tinju Burung Biduk Selatan!
Dialogue: 0,0:06:12.62,0:06:15.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lelucon payah macam apa itu?
Dialogue: 0,0:06:15.16,0:06:16.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tunggu, Pak!
Dialogue: 0,0:06:24.30,0:06:27.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pekan depan,\Naku akan berada di sekolah lain.
Dialogue: 0,0:06:27.13,0:06:29.68,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan pergi, Nona Tae!
Dialogue: 0,0:06:32.18,0:06:34.01,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau menangis?
Dialogue: 0,0:06:35.68,0:06:37.14,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ibu kantin sekolah…
Dialogue: 0,0:06:37.81,0:06:39.23,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku tidak menangis.
Dialogue: 0,0:06:39.31,0:06:40.85,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau akan pindah?
Dialogue: 0,0:06:40.94,0:06:44.23,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Memang sulit meninggalkan kota\Nyang biasa kau tinggali.
Dialogue: 0,0:06:44.90,0:06:46.94,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maaf membuatmu khawatir.
Dialogue: 0,0:06:47.03,0:06:49.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan terlalu formal.
Dialogue: 0,0:06:49.32,0:06:52.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan malu. Kau bisa mengandalkanku.
Dialogue: 0,0:06:52.32,0:06:55.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Karena aku ibu semua orang!
Dialogue: 0,0:06:55.12,0:06:58.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini, makanlah sup labu dan bersemangatlah.
Dialogue: 0,0:06:58.29,0:06:59.12,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terima kasih banyak.
Dialogue: 0,0:06:59.21,0:07:01.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dan ini untuk tetangga\Nyang dulu mengurusmu.
Dialogue: 0,0:07:01.88,0:07:04.84,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini untuk petugas pindahan.
Dialogue: 0,0:07:04.92,0:07:05.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dan ini…
Dialogue: 0,0:07:05.84,0:07:06.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini cukup.
Dialogue: 0,0:07:07.05,0:07:10.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lagi pula, petugas pindahan\Ntidak mau semur setelah bekerja fisik.
Dialogue: 0,0:07:11.64,0:07:14.39,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Menurutmu begitu?
Dialogue: 0,0:07:15.72,0:07:16.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak… bukan begitu.
Dialogue: 0,0:07:17.06,0:07:19.39,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Beberapa petugas pindahan\Nmungkin ingin makan apa pun.
Dialogue: 0,0:07:19.48,0:07:22.48,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mereka mungkin ingin makan banyak sup labu.
Dialogue: 0,0:07:22.56,0:07:23.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Begitukah?
Dialogue: 0,0:07:23.48,0:07:24.31,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya.
Dialogue: 0,0:07:24.40,0:07:27.82,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku yakin beberapa petugas pindahan\Nmenunggu untuk makan semurmu!
Dialogue: 0,0:07:28.24,0:07:29.44,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terima kasih.
Dialogue: 0,0:07:29.53,0:07:32.11,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berkatmu, aku kembali percaya diri.
Dialogue: 0,0:07:32.66,0:07:34.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Makanlah.
Dialogue: 0,0:07:36.62,0:07:38.91,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu juga kali terakhir aku melihatnya.
Dialogue: 0,0:07:40.50,0:07:42.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kudengar kau akan pindah sekolah.
Dialogue: 0,0:07:42.17,0:07:43.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak Hattori.
Dialogue: 0,0:07:43.50,0:07:46.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini hadiah perpisahan untukmu. Ambillah.
Dialogue: 0,0:07:46.30,0:07:47.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini…
Dialogue: 0,0:07:48.05,0:07:48.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Obat wasir.
Dialogue: 0,0:07:49.05,0:07:49.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak, aku tidak perlu.
Dialogue: 0,0:07:50.05,0:07:51.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau bisa berikan ke tetanggamu.
Dialogue: 0,0:07:51.68,0:07:54.39,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Siapa yang memberikan obat wasir\Nsaat pindah?
Dialogue: 0,0:07:54.97,0:07:56.51,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau begitu, jaga dirimu.
Dialogue: 0,0:07:57.56,0:07:59.06,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ada apa sebenarnya?
Dialogue: 0,0:07:59.73,0:08:02.06,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kudengar kau pindah sekolah.
Dialogue: 0,0:08:02.14,0:08:03.44,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak Matsudaira.
Dialogue: 0,0:08:03.81,0:08:07.36,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini hadiah perpisahan untukmu. Ambillah.
Dialogue: 0,0:08:07.73,0:08:09.19,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Boleh aku tahu apa ini…
Dialogue: 0,0:08:09.28,0:08:10.82,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pistol setrum.
Dialogue: 0,0:08:10.90,0:08:12.95,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jika penjaga datang berpatroli,
Dialogue: 0,0:08:13.03,0:08:14.57,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tidurkan dia dengan ini.
Dialogue: 0,0:08:14.66,0:08:15.87,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak, aku tidak membutuhkannya.
Dialogue: 0,0:08:15.95,0:08:17.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lagi pula, kenapa ada penjaga?
Dialogue: 0,0:08:17.91,0:08:21.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku akan memberimu nasihat terakhir.
Dialogue: 0,0:08:21.58,0:08:23.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan mati.
Dialogue: 0,0:08:23.46,0:08:24.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu terlalu berat!
Dialogue: 0,0:08:24.37,0:08:27.09,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau pikir aku akan pindah ke mana?
Dialogue: 0,0:08:27.71,0:08:29.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku tidak butuh ini.
Dialogue: 0,0:08:29.67,0:08:30.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tuan Shinpachi.
Dialogue: 0,0:08:32.47,0:08:34.01,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tama…
Dialogue: 0,0:08:34.64,0:08:36.60,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku sangat kesepian.
Dialogue: 0,0:08:36.67,0:08:39.01,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi aku belum bicara banyak\Ndi episode utama.
Dialogue: 0,0:08:39.10,0:08:41.14,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau tidak perlu mengatakan itu.
Dialogue: 0,0:08:43.64,0:08:47.73,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak, aku juga sangat kesepian.
Dialogue: 0,0:08:48.23,0:08:50.90,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kudengar ada sesuatu\Nyang disebut hadiah perpisahan.
Dialogue: 0,0:08:50.98,0:08:53.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini untuk seseorang yang akan pergi.
Dialogue: 0,0:08:53.32,0:08:55.28,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak Gengai mengajariku itu.
Dialogue: 0,0:08:55.86,0:08:57.03,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini.
Dialogue: 0,0:08:57.41,0:08:58.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini sekrup yang baru dikeluarkan.
Dialogue: 0,0:08:58.91,0:09:01.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau membuatnya terdengar\Nseperti persik yang baru dipetik.
Dialogue: 0,0:09:01.41,0:09:03.16,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Simpanlah sebagai suvenir.
Dialogue: 0,0:09:03.25,0:09:05.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi ambil ini bukan ide bagus.
Dialogue: 0,0:09:05.25,0:09:07.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak perlu khawatir.
Dialogue: 0,0:09:07.29,0:09:08.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hanya karena sekrupnya dikeluarkan,
Dialogue: 0,0:09:09.04,0:09:11.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tidak akan langsung rusak…
Dialogue: 0,0:09:11.09,0:09:14.72,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudah rusak! Kerusakan serius!
Dialogue: 0,0:09:15.26,0:09:19.60,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tuan Shinpachi… jaga dirimu.
Dialogue: 0,0:09:19.68,0:09:21.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Seberapa penting sekrup\Nyang kau keluarkan ini?
Dialogue: 0,0:09:22.06,0:09:23.10,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tunggu, Tama!
Dialogue: 0,0:09:23.60,0:09:26.48,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tolong berikan… stimulasi listrik…
Dialogue: 0,0:09:26.56,0:09:28.52,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}komputer pusat internalku…
Dialogue: 0,0:09:28.61,0:09:30.73,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak mungkin. Bahkan jika kau menyuruhku\Nmemakai stimulasi listrik,
Dialogue: 0,0:09:30.82,0:09:32.07,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}kita tidak punya alat seperti itu di sini!
Dialogue: 0,0:09:32.15,0:09:33.19,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sebenarnya ada!
Dialogue: 0,0:09:33.90,0:09:36.70,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terima kasih, Pak Matsudaira!
Dialogue: 0,0:09:38.20,0:09:39.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berkumpul!
Dialogue: 0,0:09:42.16,0:09:45.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi, hari terakhir telah tiba.
Dialogue: 0,0:09:52.80,0:09:54.84,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan!
Dialogue: 0,0:09:55.34,0:09:58.72,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Perasaan sebelum guru memasuki kelas…
Dialogue: 0,0:09:58.93,0:10:00.43,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}10.000 yen per jam… Kau yakin?
Dialogue: 0,0:10:00.51,0:10:02.26,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lalu beberapa saat lalu, tetanggaku…
Dialogue: 0,0:10:02.35,0:10:04.47,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa yang akan kau lakukan?\NSekarang giliranmu.
Dialogue: 0,0:10:04.56,0:10:05.39,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa yang akan kau lakukan?
Dialogue: 0,0:10:06.31,0:10:08.52,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}- Kembalikan…\N- Ini hari terakhir.
Dialogue: 0,0:10:10.61,0:10:13.23,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalian berisik sekali.
Dialogue: 0,0:10:13.86,0:10:15.90,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Duduklah sekarang, Berengsek.
Dialogue: 0,0:10:16.70,0:10:18.11,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku akan mengabsen.
Dialogue: 0,0:10:21.70,0:10:24.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baik, kita akan kuis dadakan sekarang.
Dialogue: 0,0:10:24.83,0:10:26.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak, kau tidak mengatakan apa pun\Ntentang kuis.
Dialogue: 0,0:10:26.58,0:10:27.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jika sesuatu harus dadakan,
Dialogue: 0,0:10:28.00,0:10:29.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dadakan yang lain.
Dialogue: 0,0:10:29.42,0:10:31.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berlarilah seolah hidupmu\Nbergantung padanya!
Dialogue: 0,0:10:32.25,0:10:35.51,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tahun ini,\NPerry mengemudikan kapal ke sini…
Dialogue: 0,0:10:35.59,0:10:37.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}- Baiklah, sampai kapan kau akan menangis?\N- Nona Tae…
Dialogue: 0,0:10:37.47,0:10:39.80,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}- Ayo ke koperasi, Kondou.\N- Nona Tae…
Dialogue: 0,0:10:40.55,0:10:41.55,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baik,
Dialogue: 0,0:10:41.64,0:10:44.39,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}sekian rapat kelas hari ini.
Dialogue: 0,0:10:44.47,0:10:45.56,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hati-hati saat pulang.
Dialogue: 0,0:10:46.27,0:10:47.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak mungkin. Benarkah?
Dialogue: 0,0:10:47.89,0:10:49.23,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Murid Piket, beri aba-aba.
Dialogue: 0,0:10:49.31,0:10:50.23,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berdiri!
Dialogue: 0,0:10:50.31,0:10:51.69,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Beri hormat… Rahasianya adalah…
Dialogue: 0,0:10:54.11,0:10:55.28,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kecantikan Putih Membubung!
Dialogue: 0,0:10:55.36,0:10:57.95,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lelucon itu lagi? Cukup!
Dialogue: 0,0:11:03.16,0:11:04.95,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu saja?
Dialogue: 0,0:11:05.70,0:11:10.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hari terakhirku di Kelas 3-Z SMA Gintama
Dialogue: 0,0:11:10.37,0:11:14.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}benar-benar berakhir\Ndengan ejekan asal-asalan dan terburuk?
Dialogue: 0,0:11:19.97,0:11:22.43,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau mau mengulang, Shinpachi?
Dialogue: 0,0:11:23.39,0:11:25.22,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak Ginpachi…
Dialogue: 0,0:11:25.85,0:11:27.14,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kita akan ulang dari awal?
Dialogue: 0,0:11:29.23,0:11:30.44,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Semuanya…
Dialogue: 0,0:11:38.78,0:11:41.36,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kelas 3-Z.
Dialogue: 0,0:11:41.99,0:11:43.95,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}- Pak Ginpachi.
Dialogue: 0,0:11:44.03,0:11:45.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukan itu arti "mengulang"!
Dialogue: 0,0:11:45.91,0:11:48.37,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kenapa mengulang dari awal?
Dialogue: 0,0:11:49.83,0:11:51.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baiklah,
Dialogue: 0,0:11:51.25,0:11:52.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}pesta perpisahan dimulai sekarang!
Dialogue: 0,0:11:54.59,0:11:56.84,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pembawa acara kalian\Nadalah Sakata Silver Eight.
Dialogue: 0,0:11:56.92,0:11:57.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dan…
Dialogue: 0,0:11:57.84,0:11:58.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Halo, semuanya.
Dialogue: 0,0:11:58.76,0:12:00.76,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku pembawa acara kalian, Kanna Hashimoto.
Dialogue: 0,0:12:02.55,0:12:03.47,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tanpa basa-basi lagi,
Dialogue: 0,0:12:03.55,0:12:05.64,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}mari kita dengar ucapan berkat\Ndari teman-teman sekelas kalian.
Dialogue: 0,0:12:06.72,0:12:08.31,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Shinpachi, bukan?
Dialogue: 0,0:12:08.39,0:12:09.98,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Akan kuberi tahu sebuah rahasia.
Dialogue: 0,0:12:10.06,0:12:13.19,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dan yang berwenang adalah ini dan itu.\NIni dibuat oleh perusahaan tertentu,
Dialogue: 0,0:12:13.27,0:12:14.69,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukankah itu aneh sejak awal?
Dialogue: 0,0:12:14.77,0:12:16.23,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi, lensanya juga begini dan begitu.
Dialogue: 0,0:12:16.32,0:12:17.86,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bingkainya juga begini.
Dialogue: 0,0:12:17.94,0:12:20.49,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau membahas kacamataku selama ini?
Dialogue: 0,0:12:20.99,0:12:23.95,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Perpisahan tiba-tiba selalu menyedihkan.
Dialogue: 0,0:12:24.03,0:12:24.95,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hasegawa…
Dialogue: 0,0:12:25.03,0:12:26.28,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kami mengobrol, tertawa,
Dialogue: 0,0:12:26.37,0:12:28.45,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dan berpamitan dengan "Sampai jumpa lagi".
Dialogue: 0,0:12:28.58,0:12:30.87,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku menunggu lama, tapi tidak ada balasan.
Dialogue: 0,0:12:30.95,0:12:33.12,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ada apa dengan pekerjaan paruh waktu\Nbelakangan ini?
Dialogue: 0,0:12:33.21,0:12:34.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia membicarakan pekerjaan paruh waktu!
Dialogue: 0,0:12:34.83,0:12:39.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Banyak jalan yang berbeda dalam hidup.
Dialogue: 0,0:12:39.13,0:12:42.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}- Akhirnya, ucapan berkat yang normal.\N- Tapi memilih jalan yang harus ditempuh
Dialogue: 0,0:12:42.26,0:12:44.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}selalu sulit.
Dialogue: 0,0:12:44.13,0:12:45.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Haruskah aku fokus pada layanan mandi busa
Dialogue: 0,0:12:46.05,0:12:48.22,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}atau memilih layanan becak?
Dialogue: 0,0:12:48.31,0:12:49.39,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tunggu, pilihan macam apa itu?
Dialogue: 0,0:12:49.47,0:12:50.47,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sia-sia saja aku berharap.
Dialogue: 0,0:12:51.18,0:12:53.77,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baik, mereka bertiga memberikan\Nkata-kata yang mengharukan.
Dialogue: 0,0:12:53.85,0:12:55.10,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tunggu sebentar!
Dialogue: 0,0:12:56.65,0:12:59.73,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berikutnya\N"Pertunjukkan Salindia Kenangan".
Dialogue: 0,0:12:59.82,0:13:00.98,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dalam segmen ini,
Dialogue: 0,0:13:01.78,0:13:04.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}kita akan melihat kilas balik\Nmasa muda di sekolah
Dialogue: 0,0:13:04.32,0:13:06.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dari Shinpachi Shimura dan Tae Shimura.
Dialogue: 0,0:13:06.32,0:13:09.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu rencananya.
Dialogue: 0,0:13:10.08,0:13:12.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Akhirnya mulai terasa\Nseperti pesta perpisahan.
Dialogue: 0,0:13:12.83,0:13:13.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baik,
Dialogue: 0,0:13:13.83,0:13:16.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}mulai "Pertunjukkan Salindia Kenangan".
Dialogue: 0,0:13:28.22,0:13:30.14,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Di mana aku?
Dialogue: 0,0:13:30.51,0:13:32.39,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Setiap klipnya adalah kakakku!
Dialogue: 0,0:13:32.47,0:13:35.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini berubah menjadi video gambar idola!
Dialogue: 0,0:13:35.89,0:13:37.23,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan khawatir.
Dialogue: 0,0:13:37.31,0:13:39.77,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kami juga akan memberimu\Nkilas balik masa mudamu.
Dialogue: 0,0:13:39.86,0:13:40.73,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Benar.
Dialogue: 0,0:13:40.82,0:13:42.82,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pachi adalah bintang sejati\Ndi salindia ini,
Dialogue: 0,0:13:42.90,0:13:44.32,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}jadi, dia diletakkan di ujung.
Dialogue: 0,0:13:44.61,0:13:46.07,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baiklah, kalau begitu.
Dialogue: 0,0:13:46.53,0:13:47.78,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berikutnya adalah
Dialogue: 0,0:13:47.87,0:13:51.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}kilas balik masa muda Shinpachi Shimura.\NSaksikan.
Dialogue: 0,0:13:52.29,0:13:53.87,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kacamata lagi!
Dialogue: 0,0:14:00.13,0:14:01.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tunggu sebentar!
Dialogue: 0,0:14:01.88,0:14:03.34,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Setiap adegannya hanya kacamata!
Dialogue: 0,0:14:03.42,0:14:05.34,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dan yang terakhir adalah Hasegawa!
Dialogue: 0,0:14:05.76,0:14:07.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kacamata Shinpachi berteriak begitu keras.
Dialogue: 0,0:14:07.68,0:14:08.51,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan rekam aku!
Dialogue: 0,0:14:08.59,0:14:10.10,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan marah.
Dialogue: 0,0:14:10.97,0:14:13.10,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Masih ada banyak di program ini.
Dialogue: 0,0:14:13.89,0:14:15.98,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ayolah, lebih sulit untuk tidak marah.
Dialogue: 0,0:14:16.06,0:14:16.98,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau bilang aku tokoh utamanya,
Dialogue: 0,0:14:17.06,0:14:18.56,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}- jadi, aku berharap.\N- Berikutnya adalah konser!
Dialogue: 0,0:14:18.65,0:14:19.65,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hei!
Dialogue: 0,0:14:19.90,0:14:22.77,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tampil megah di konser perpisahan hari ini
Dialogue: 0,0:14:22.86,0:14:25.74,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}adalah pria terseksi\Ndi dunia hiphop sekarang,
Dialogue: 0,0:14:25.82,0:14:27.86,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}MC Zura! Ayo!
Dialogue: 0,0:14:28.74,0:14:29.82,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hei, hei
Dialogue: 0,0:14:29.91,0:14:31.78,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku bukan Zura, yo
Dialogue: 0,0:14:31.87,0:14:33.62,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Untuk kalian berdua hari ini
Dialogue: 0,0:14:33.70,0:14:36.12,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kuberikan lirik perpisahan
Dialogue: 0,0:14:36.21,0:14:38.62,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Biar kutunjukkan\NKekuatan meledak-ledak ini, yo
Dialogue: 0,0:14:38.71,0:14:40.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yo, katakan hoo
Dialogue: 0,0:14:40.96,0:14:42.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini agak berat, jadi, akan kutaruh.
Dialogue: 0,0:14:42.79,0:14:44.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kenapa kau tidak menaruhnya di sana\Nsejak awal?
Dialogue: 0,0:14:45.17,0:14:47.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hei, pindah sekolah
Dialogue: 0,0:14:47.51,0:14:49.72,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pria itu terlalu sentimental
Dialogue: 0,0:14:49.80,0:14:52.14,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hei, pindah sekolah, Tenko Hikita
Dialogue: 0,0:14:52.22,0:14:54.39,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pria itu terlalu tampan
Dialogue: 0,0:14:54.47,0:14:55.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yo yo yo
Dialogue: 0,0:14:55.89,0:14:57.35,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini ilusi, yo
Dialogue: 0,0:14:57.43,0:14:59.10,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dililit rantai beberapa kali
Dialogue: 0,0:14:59.73,0:15:01.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lalu dimasukkan ke kotak
Dialogue: 0,0:15:01.48,0:15:02.31,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bom
Dialogue: 0,0:15:02.40,0:15:04.36,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi Tenko, namun Tenko
Dialogue: 0,0:15:04.44,0:15:05.57,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kembali tanpa terluka
Dialogue: 0,0:15:05.65,0:15:07.07,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Katakan hoo
Dialogue: 0,0:15:07.15,0:15:08.49,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tenko
Dialogue: 0,0:15:08.57,0:15:10.16,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tenko Hikita
Dialogue: 0,0:15:10.24,0:15:12.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hideaki, Hideaki Tokunaga
Dialogue: 0,0:15:12.28,0:15:13.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya
Dialogue: 0,0:15:13.37,0:15:15.12,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Cukup!
Dialogue: 0,0:15:15.20,0:15:16.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ada apa dengan rap ini?
Dialogue: 0,0:15:16.37,0:15:18.62,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sebagian besar tentang Tenko Hikita!
Dialogue: 0,0:15:18.71,0:15:21.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hei, aku suka Hideaki Tokunaga
Dialogue: 0,0:15:21.79,0:15:23.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Nintendo Switch rusak
Dialogue: 0,0:15:23.67,0:15:25.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kenapa tepatnya? Perbaiki!
Dialogue: 0,0:15:25.88,0:15:28.09,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Perbaiki otakmu juga!
Dialogue: 0,0:15:28.76,0:15:29.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak.
Dialogue: 0,0:15:29.72,0:15:32.55,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jika kita mengadakan pesta perpisahan,\Nlakukan dengan benar!
Dialogue: 0,0:15:32.64,0:15:33.55,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kumohon!
Dialogue: 0,0:15:34.39,0:15:36.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}"Waktu Pengakuan Isao Kondou!"
Dialogue: 0,0:15:37.89,0:15:42.27,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Cinta gorila ceroboh\Nuntuk Tae Shimura tumbuh setiap hari.
Dialogue: 0,0:15:43.27,0:15:44.77,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia dengan berani menghadapi penolakan.
Dialogue: 0,0:15:44.86,0:15:49.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Perjalanan pengakuan Isao Kondou\Nadalah serangkaian penolakan.
Dialogue: 0,0:15:49.53,0:15:51.82,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Namun, hari ini adalah hari terakhir.
Dialogue: 0,0:15:51.91,0:15:55.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tae Shimura meninggalkan 3-Z\Ndan memulai perjalanan baru.
Dialogue: 0,0:15:55.28,0:15:58.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mari kita ukir\Naksi heroik terakhir Isao Kondou
Dialogue: 0,0:15:58.16,0:15:59.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}ke dalam mata kita.
Dialogue: 0,0:16:00.04,0:16:00.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak, begini…
Dialogue: 0,0:16:01.04,0:16:02.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tunggu, Shimura.
Dialogue: 0,0:16:02.92,0:16:04.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kondou sudah ditolak
Dialogue: 0,0:16:04.46,0:16:06.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}berkali-kali sampai sekarang.
Dialogue: 0,0:16:06.67,0:16:09.26,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi, hasilnya pasti akan sama hari ini…
Dialogue: 0,0:16:09.34,0:16:11.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kita semua tahu itu.
Dialogue: 0,0:16:11.97,0:16:14.34,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi ini hari terakhir.
Dialogue: 0,0:16:14.43,0:16:16.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kumohon, biarkan kami menyaksikannya.
Dialogue: 0,0:16:18.93,0:16:20.23,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Begitulah adanya.
Dialogue: 0,0:16:20.31,0:16:22.10,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bisa bantu kami
Dialogue: 0,0:16:22.19,0:16:23.48,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dan dengarkan pengakuan terakhirnya?
Dialogue: 0,0:16:23.56,0:16:24.77,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baik.
Dialogue: 0,0:16:28.90,0:16:30.28,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Nona Tae.
Dialogue: 0,0:16:30.36,0:16:32.32,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau memiliki\Nkemampuan observasi yang tajam.
Dialogue: 0,0:16:32.40,0:16:35.28,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mungkin kau sudah menyadari dengan samar
Dialogue: 0,0:16:35.37,0:16:37.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}bahwa aku menyukaimu.
Dialogue: 0,0:16:37.95,0:16:40.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku bersumpah demi dewa langit dan bumi.
Dialogue: 0,0:16:40.66,0:16:46.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku, Isao Kondou,\Nberjanji untuk membuatmu bahagia.
Dialogue: 0,0:16:46.21,0:16:49.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terimalah perasaanku!
Dialogue: 0,0:16:50.05,0:16:52.01,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu sangat berbeda.
Dialogue: 0,0:16:52.76,0:16:54.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pria yang mencuri seruling kakakku,
Dialogue: 0,0:16:55.05,0:16:57.14,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}menggosokkan pipinya ke kursi kakakku,
Dialogue: 0,0:16:57.47,0:16:59.64,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}mencuri pakaian olahraga kakakku,
Dialogue: 0,0:16:59.72,0:17:01.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dan bahkan diam-diam memakainya
Dialogue: 0,0:17:02.89,0:17:06.27,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}hingga membuatmu melupakan kejahatan itu…\Nsangat berbeda
Dialogue: 0,0:17:07.69,0:17:09.78,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maaf, aku tak menyimak.
Dialogue: 0,0:17:09.85,0:17:11.44,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bisa katakan sekali lagi,
Dialogue: 0,0:17:11.53,0:17:12.44,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak Gondou?
Dialogue: 0,0:17:12.65,0:17:16.61,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak Gondou…
Dialogue: 0,0:17:20.29,0:17:22.11,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak Kondou!
Dialogue: 0,0:17:23.66,0:17:26.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berikutnya adalah segmen pemberian hadiah.
Dialogue: 0,0:17:26.11,0:17:28.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tunggu, transisi itu terlalu mendadak!
Dialogue: 0,0:17:28.46,0:17:30.34,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini hadiah perpisahanku untukmu.
Dialogue: 0,0:17:30.42,0:17:32.30,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini kait berbentuk S yang kubeli di Lofuto.
Dialogue: 0,0:17:32.67,0:17:35.01,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ambil kemasan isi 30 ini, Pencuri!
Dialogue: 0,0:17:35.09,0:17:36.39,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Siapa yang butuh 30?
Dialogue: 0,0:17:36.64,0:17:39.26,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bagaimana dengan ini? Kait hidung!
Dialogue: 0,0:17:39.35,0:17:41.06,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ambil kemasan isi 30 ini, Pencuri.
Dialogue: 0,0:17:41.14,0:17:42.18,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku bahkan tidak butuh satu!
Dialogue: 0,0:17:42.27,0:17:43.68,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalian sangat suka kait, ya?
Dialogue: 0,0:17:44.69,0:17:47.19,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tanpa itu, karakteristikmu akan hilang.\NKau yang akan mendapat masalah.
Dialogue: 0,0:17:47.65,0:17:50.15,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tunggu sebentar, semuanya. Tenang dulu!
Dialogue: 0,0:17:50.23,0:17:51.61,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jika kau hanya akan mengeluh,
Dialogue: 0,0:17:51.69,0:17:53.49,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}beri aku hadiah perpisahan.
Dialogue: 0,0:17:53.57,0:17:54.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kenapa aku?
Dialogue: 0,0:17:54.49,0:17:55.78,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pindah, Tenko Hikita.
Dialogue: 0,0:17:55.86,0:17:57.28,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan muncul!
Dialogue: 0,0:17:57.36,0:17:59.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hei, semuanya. Tunggu.
Dialogue: 0,0:18:01.16,0:18:04.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Cukup, Labrador!
Dialogue: 0,0:18:04.62,0:18:06.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Labrador…
Dialogue: 0,0:18:08.92,0:18:11.55,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maaf karena aku tiba-tiba berteriak.
Dialogue: 0,0:18:12.84,0:18:15.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Semuanya, terima kasih banyak.
Dialogue: 0,0:18:16.38,0:18:19.43,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kukira tidak akan ada\Npesta perpisahan sama sekali.
Dialogue: 0,0:18:19.51,0:18:21.56,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku tidak menduga\Nkalian akan mengadakan pesta untuk kami.
Dialogue: 0,0:18:21.64,0:18:25.02,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi aku terus berteriak\Nsampai pembuluh darahku hampir pecah.
Dialogue: 0,0:18:25.56,0:18:27.31,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maafkan aku.
Dialogue: 0,0:18:28.65,0:18:31.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lelucon Tenko Hikita Zura sangat kuno.
Dialogue: 0,0:18:31.48,0:18:33.86,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku bukan marah soal itu!
Dialogue: 0,0:18:33.94,0:18:35.32,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi Shinpachi,
Dialogue: 0,0:18:36.11,0:18:38.03,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}kami hanya ingin melihat
Dialogue: 0,0:18:38.57,0:18:40.66,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}ejekanmu yang biasa.
Dialogue: 0,0:18:44.49,0:18:49.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Entah itu lelucon klise atau konyol,\Nkami ingin melihat ejekan tajammu.
Dialogue: 0,0:18:49.67,0:18:51.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hari ini juga kali terakhir.
Dialogue: 0,0:18:52.04,0:18:53.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak…
Dialogue: 0,0:18:53.88,0:18:56.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ejekan Shinpachi\Njuga yang terbaik hari ini.
Dialogue: 0,0:18:57.05,0:18:58.01,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kagura…
Dialogue: 0,0:18:58.34,0:19:01.30,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ejekan terhadap rapku juga luar biasa.
Dialogue: 0,0:19:01.39,0:19:04.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terutama kekuatan suaramu\Nsaat berteriak, "Kenapa terbalik?"
Dialogue: 0,0:19:04.89,0:19:06.85,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku tidak ingat mengatakan itu.
Dialogue: 0,0:19:06.93,0:19:09.56,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku bahkan tidak melakukan\Nbanyak hal konyol.
Dialogue: 0,0:19:09.65,0:19:12.27,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau marah saat aku hendak membuat lelucon.
Dialogue: 0,0:19:12.36,0:19:14.53,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Okita… Maafkan aku.
Dialogue: 0,0:19:14.61,0:19:15.86,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu cukup bagus.
Dialogue: 0,0:19:15.94,0:19:16.86,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu bagus.
Dialogue: 0,0:19:16.94,0:19:18.49,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ejekan yang bagus.
Dialogue: 0,0:19:18.57,0:19:19.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Semuanya…
Dialogue: 0,0:19:19.49,0:19:20.49,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Shinpachi…
Dialogue: 0,0:19:21.16,0:19:23.70,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku senang bisa mendengar\Nejekan familiermu untuk kali terakhir.
Dialogue: 0,0:19:23.78,0:19:25.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu sungguh luar biasa.
Dialogue: 0,0:19:25.29,0:19:26.45,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kondou…
Dialogue: 0,0:19:26.87,0:19:28.66,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dan Nona Tae.
Dialogue: 0,0:19:29.16,0:19:32.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pada akhirnya, aku tidak bisa\Nmenyampaikan perasaanku kepadamu.
Dialogue: 0,0:19:33.42,0:19:35.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi aku tidak menyesal.
Dialogue: 0,0:19:35.50,0:19:37.26,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kudoakan yang terbaik untukmu\Ndi sekolah barumu.
Dialogue: 0,0:19:37.51,0:19:39.34,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maaf, tapi kau siapa?
Dialogue: 0,0:19:39.42,0:19:40.55,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tunggu, itu kejam.
Dialogue: 0,0:19:40.63,0:19:42.68,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak ada kebaikan sedikit pun.
Dialogue: 0,0:19:42.76,0:19:44.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya, ejekan semacam itu.
Dialogue: 0,0:19:44.89,0:19:46.93,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terima kasih banyak.
Dialogue: 0,0:19:47.02,0:19:48.64,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi jika kau memujiku seperti ini,
Dialogue: 0,0:19:48.73,0:19:50.69,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}akan lebih sulit bagiku untuk mengejek.
Dialogue: 0,0:19:51.10,0:19:53.15,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Akhirnya kau tertawa.
Dialogue: 0,0:19:53.23,0:19:56.11,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sekarang, aku bisa melepas kalian\Ndengan tenang.
Dialogue: 0,0:19:57.32,0:19:58.74,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Shinpachi,
Dialogue: 0,0:19:58.82,0:20:00.49,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dan kakak Shinpachi,
Dialogue: 0,0:20:02.49,0:20:03.70,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}jaga diri kalian.
Dialogue: 0,0:20:05.03,0:20:06.74,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi sejujurnya,
Dialogue: 0,0:20:06.83,0:20:10.66,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}akan terlalu menyakitkan bagi Kelas 3-Z\Ntanpa ejekanmu.
Dialogue: 0,0:20:10.75,0:20:12.12,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi apa boleh buat.
Dialogue: 0,0:20:12.75,0:20:14.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kami juga akan melakukan yang terbaik.
Dialogue: 0,0:20:14.88,0:20:17.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Meski kau pergi, kami akan terus maju.
Dialogue: 0,0:20:17.59,0:20:19.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi, bagaimana mengatakannya, ya?
Dialogue: 0,0:20:20.67,0:20:22.47,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terima kasih atas semuanya selama ini.
Dialogue: 0,0:20:23.76,0:20:26.01,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dan ini dari semua orang.
Dialogue: 0,0:20:26.76,0:20:28.31,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini…
Dialogue: 0,0:20:28.56,0:20:29.56,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku akan mengirimimu pesan.
Dialogue: 0,0:20:29.64,0:20:31.31,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan lupakan kami.
Dialogue: 0,0:20:31.39,0:20:33.44,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berhenti bicara\Ndan berikan hadiah perpisahanku.
Dialogue: 0,0:20:33.52,0:20:34.35,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jaga dirimu.
Dialogue: 0,0:20:34.44,0:20:37.02,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jika kau tahu pekerjaan paruh waktu\Nyang bagus, beri tahu aku.
Dialogue: 0,0:20:37.11,0:20:38.78,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jaga diri kalian.
Dialogue: 0,0:20:41.03,0:20:43.91,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Benar… aku masih tidak mau…
Dialogue: 0,0:20:44.91,0:20:47.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku tidak mau berpisah dengan semua orang.
Dialogue: 0,0:20:47.83,0:20:49.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Shin…
Dialogue: 0,0:20:49.62,0:20:52.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku tidak mau…
Dialogue: 0,0:20:53.29,0:20:55.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}pindah sama sekali!
Dialogue: 0,0:20:57.54,0:20:59.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Soal itu…
Dialogue: 0,0:21:00.05,0:21:01.09,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu ibu kantin…
Dialogue: 0,0:21:01.17,0:21:02.30,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bibi.
Dialogue: 0,0:21:03.05,0:21:03.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bibi?
Dialogue: 0,0:21:03.97,0:21:04.80,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya.
Dialogue: 0,0:21:04.89,0:21:11.64,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sepupu teman minum bos istri ketiga\Nkeponakan kakek kita.
Dialogue: 0,0:21:11.73,0:21:14.52,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi, kita akan tinggal bersamanya?
Dialogue: 0,0:21:14.85,0:21:16.15,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maafkan aku.
Dialogue: 0,0:21:16.23,0:21:19.53,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bisakah kita lupakan aku mengatakan itu?
Dialogue: 0,0:21:21.19,0:21:24.53,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sebenarnya, aku punya pacar\Nyang bekerja sebagai petugas pindahan.
Dialogue: 0,0:21:24.74,0:21:26.45,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Karena kau menyemangatiku
Dialogue: 0,0:21:26.53,0:21:29.49,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}akhirnya aku memutuskan untuk menikahinya.
Dialogue: 0,0:21:30.70,0:21:31.62,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Begitukah?
Dialogue: 0,0:21:31.70,0:21:32.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya.
Dialogue: 0,0:21:32.62,0:21:35.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku yakin beberapa petugas pindahan\Nmenunggu untuk makan semurmu!
Dialogue: 0,0:21:36.04,0:21:37.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terima kasih.
Dialogue: 0,0:21:37.29,0:21:39.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berkatmu, aku kembali percaya diri.
Dialogue: 0,0:21:40.30,0:21:42.51,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terima kasih sudah menyemangatiku!
Dialogue: 0,0:21:42.59,0:21:44.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak, aku tidak mengerti.
Dialogue: 0,0:21:44.72,0:21:45.76,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ibu semua orang
Dialogue: 0,0:21:45.84,0:21:48.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}hanya miliknya sekarang.
Dialogue: 0,0:21:49.05,0:21:50.47,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Selamat.
Dialogue: 0,0:21:50.56,0:21:51.64,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terima kasih, kalian berdua.
Dialogue: 0,0:21:51.72,0:21:54.27,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi, kalian tidak perlu pindah.
Dialogue: 0,0:21:54.35,0:21:55.85,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sampai jumpa!
Dialogue: 0,0:21:56.81,0:21:57.65,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa…
Dialogue: 0,0:21:57.73,0:22:00.90,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa-apaan ini?
