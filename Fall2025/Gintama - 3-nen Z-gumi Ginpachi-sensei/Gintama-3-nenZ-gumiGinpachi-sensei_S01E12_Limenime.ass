Dialogue: 0,0:01:55.19,0:02:01.53,tanda,,0000,0000,0000,,GINTAMA - KELAS 3-Z GINPACHI-SENSEI
Dialogue: 0,0:03:13.94,0:03:19.24,tanda,,0000,0000,0000,,SHIGERU MORI, 38 TAHUN, 169 CM, 55 KG\NISTRI 12 TAHUN LEBIH MUDA, AYAH MOSUKE
Dialogue: 0,0:03:19.32,0:03:23.95,tanda,,0000,0000,0000,,DARI TOKYO, TINGGAL DI ASAGAYA\NSLOGAN: "TETAP JADI DIRI SENDIRI"
Dialogue: 0,0:03:25.95,0:03:26.87,tanda,,0000,0000,0000,,EPISODE 12
Dialogue: 0,0:03:26.95,0:03:28.95,tanda,,0000,0000,0000,,REUNI KELAS DAN REKAP ANIME\NHANYA BANTU MEMPERPANJANG AKHIR CERITANYA
Dialogue: 0,0:03:28.95,0:03:30.12,tanda,,0000,0000,0000,,JALAN KABUKICHO ICHIBAN
Dialogue: 0,0:07:22.65,0:07:28.57,tanda,,0000,0000,0000,,SATU KEBAIKAN SEHARI\NOKITA, SEJAK ZAMAN KUNO
Dialogue: 0,0:08:58.62,0:08:59.54,tanda,,0000,0000,0000,,KAKEK, MINUM OBAT TIGA KALI SEHARI
Dialogue: 0,0:10:45.68,0:10:47.35,tanda,,0000,0000,0000,,HARI OLAHRAGA SMA GINTAMA
Dialogue: 0,0:12:50.81,0:12:52.02,tanda,,0000,0000,0000,,KAGURA, WANITA, LEVEL 99
Dialogue: 0,0:13:11.54,0:13:15.80,tanda,,0000,0000,0000,,GINTAMA - KELAS 3-Z GINPACHI-SENSEI
Dialogue: 0,0:13:13.45,0:13:15.80,tanda,,0000,0000,0000,,SEMUANYA BAGUS!
Dialogue: 0,0:14:47.76,0:14:49.22,tanda,,0000,0000,0000,,PULANGLAH SEKARANG
Dialogue: 0,0:15:31.59,0:15:38.06,tanda,,0000,0000,0000,,GINTAMA - KELAS 3-Z GINPACHI-SENSEI
Dialogue: 0,0:16:50.59,0:16:52.47,tanda,,0000,0000,0000,,SMA GINTAMA
Dialogue: 0,0:17:16.03,0:17:19.86,tanda,,0000,0000,0000,,GINPACHI SAKATA\NGURU BAHASA, KELAS 3-Z
Dialogue: 0,0:20:27.68,0:20:28.64,tanda,,0000,0000,0000,,TAKASUGI
Dialogue: 0,0:22:24.01,0:22:25.63,tanda,,0000,0000,0000,,KELAS 3-Z GINPACHI-SENSEI KEMBALI
Dialogue: 0,0:22:25.72,0:22:27.05,tanda,,0000,0000,0000,,BELUM DIPUTUSKAN
Dialogue: 0,0:23:59.94,0:24:04.65,tanda,,0000,0000,0000,,TERIMA KASIH DUKUNGANNYA!
Dialogue: 0,0:24:04.73,0:24:09.90,tanda,,0000,0000,0000,,UNTUK MENEBUS EPISODE LALU,\NKAMI PUTAR LAGU PEMBUKA LEBIH SERING!
Dialogue: 0,0:00:02.00,0:00:03.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Negeri Samurai.
Dialogue: 0,0:00:03.75,0:00:06.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudah lama negara kita
Dialogue: 0,0:00:06.25,0:00:07.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}disebut begitu.
Dialogue: 0,0:00:08.84,0:00:13.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Langit Edo yang dulu penuh impian samurai,
Dialogue: 0,0:00:13.47,0:00:15.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}kini dipenuhi kapal udara asing.
Dialogue: 0,0:00:17.05,0:00:20.64,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jalanan tempat samurai\Ndulu berpacu membelah angin,
Dialogue: 0,0:00:20.72,0:00:23.56,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}kini sesak oleh orang asing.
Dialogue: 0,0:00:24.06,0:00:25.69,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bodoh!
Dialogue: 0,0:00:25.77,0:00:27.90,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudah kubilang bukan begitu!
Dialogue: 0,0:00:28.40,0:00:30.69,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudah berapa tahun kau kerja?
Dialogue: 0,0:00:30.78,0:00:31.74,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Delapan tahun.
Dialogue: 0,0:00:31.82,0:00:35.41,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Delapan tahun masih tak becus\Nmelayani turis asing?
Dialogue: 0,0:00:35.49,0:00:37.03,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}"How much" itu bahasa Inggris dasar.
Dialogue: 0,0:00:37.03,0:00:39.37,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Anak SMP pun tahu tanpa menyontek.
Dialogue: 0,0:00:39.44,0:00:40.52,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lihat dirimu.
Dialogue: 0,0:00:40.62,0:00:42.14,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bisa-bisanya salah dengar "hamachi"
Dialogue: 0,0:00:42.16,0:00:43.66,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}jadi "how much"?
Dialogue: 0,0:00:43.75,0:00:44.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maafkan aku.
Dialogue: 0,0:00:44.92,0:00:47.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pakai saja sontekan.
Dialogue: 0,0:00:47.84,0:00:51.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tak bisakah kau lebih berguna sedikit?
Dialogue: 0,0:00:52.17,0:00:53.34,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maaf…
Dialogue: 0,0:00:53.42,0:00:54.84,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terima kasih…
Dialogue: 0,0:01:10.98,0:01:12.69,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku pulang.
Dialogue: 0,0:01:12.99,0:01:14.44,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Selamat datang.
Dialogue: 0,0:01:17.20,0:01:18.91,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tae, apa isi kardus itu?
Dialogue: 0,0:01:19.45,0:01:22.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kutemukan saat membereskan gudang.
Dialogue: 0,0:01:26.00,0:01:27.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Nostalgia, ya?
Dialogue: 0,0:01:37.05,0:01:38.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tae.
Dialogue: 0,0:01:38.47,0:01:39.64,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Shin?
Dialogue: 0,0:01:40.10,0:01:43.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bagaimana kalau kita reuni?
Dialogue: 0,0:03:30.21,0:03:31.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}- Bersulang!
Dialogue: 0,0:03:35.29,0:03:36.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Astaga.
Dialogue: 0,0:03:36.34,0:03:38.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudah lama sekali, ya.
Dialogue: 0,0:03:38.51,0:03:39.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Benar.
Dialogue: 0,0:03:39.67,0:03:40.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Betul.
Dialogue: 0,0:03:40.76,0:03:42.26,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Semua datang hari ini?
Dialogue: 0,0:03:42.34,0:03:43.18,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak.
Dialogue: 0,0:03:43.26,0:03:45.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Beberapa orang akan terlambat.
Dialogue: 0,0:03:45.43,0:03:46.35,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Shinpachi.
Dialogue: 0,0:03:46.43,0:03:48.18,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau sudah hubungi Takasugi dan yang lain?
Dialogue: 0,0:03:48.72,0:03:50.35,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya, tentu saja.
Dialogue: 0,0:03:50.43,0:03:52.44,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi tak ada balasan.
Dialogue: 0,0:03:54.23,0:03:57.07,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Omong-omong, apa pekerjaan kalian sekarang?
Dialogue: 0,0:03:57.69,0:03:59.28,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ketua Kelas membuka usaha,
Dialogue: 0,0:03:59.36,0:04:03.07,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}mengelola perusahaan pembongkaran\Ndan konstruksi, 'kan?
Dialogue: 0,0:04:03.16,0:04:05.41,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pemilik bisnis? Hebat!
Dialogue: 0,0:04:05.49,0:04:07.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukan hal besar, kok.
Dialogue: 0,0:04:07.91,0:04:10.41,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Cuma bisnis kecil.
Dialogue: 0,0:04:10.50,0:04:11.62,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mengejutkan.
Dialogue: 0,0:04:11.71,0:04:13.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tak ada data yang menunjukkan
Dialogue: 0,0:04:13.79,0:04:15.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}minat Pak Kondou
Dialogue: 0,0:04:15.04,0:04:16.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}di arsitektur.
Dialogue: 0,0:04:17.00,0:04:18.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Titik baliknya...
Dialogue: 0,0:04:19.55,0:04:21.72,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berkat invasi terus-menerus ke rumah Otae,
Dialogue: 0,0:04:21.80,0:04:25.80,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}aku menguasai sistem keamanan\Ndan cara melucuti jebakan.
Dialogue: 0,0:04:25.89,0:04:28.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Melucuti? Maksudmu mematikan\Nkeamanan rumahku!
Dialogue: 0,0:04:29.60,0:04:31.73,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bahkan dirakit ulang
Dialogue: 0,0:04:31.81,0:04:34.69,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dengan sempurna\Nsampai para ahli pun tak tahu.
Dialogue: 0,0:04:35.35,0:04:38.02,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kami "hancurkan yang lama"\Ndan "bangun yang baru".
Dialogue: 0,0:04:38.11,0:04:39.94,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu aturan perusahaan kami.
Dialogue: 0,0:04:40.03,0:04:42.95,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ikuti aturan manusia dulu, dong.
Dialogue: 0,0:04:43.32,0:04:45.49,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukan titik balik,
Dialogue: 0,0:04:45.57,0:04:48.66,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}hidupmu itu yang hancur.
Dialogue: 0,0:04:48.91,0:04:52.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau masih saja menguntit.
Dialogue: 0,0:04:52.41,0:04:55.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tae, kau tak bisa tenang bekerja, 'kan?
Dialogue: 0,0:04:56.04,0:04:57.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sama sekali tidak.
Dialogue: 0,0:04:57.71,0:04:58.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Benar, 'kan, Kyu?
Dialogue: 0,0:04:58.84,0:05:01.51,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Benar. Aku berinvestasi,
Dialogue: 0,0:05:01.59,0:05:04.80,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dan kami buka\Nperusahaan konstruksi kecil bersama.
Dialogue: 0,0:05:06.05,0:05:10.10,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Selain Kyube, aku tak tahu\NTae berbakat manajemen.
Dialogue: 0,0:05:10.18,0:05:13.35,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku orangnya pragmatis.
Dialogue: 0,0:05:14.02,0:05:17.35,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sekarang, aku bertanggung jawab\Nmengawasi lokasi pembongkaran.
Dialogue: 0,0:05:18.19,0:05:22.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tunggu, itu bukan mengawasi konstruksi,\Ntapi autopsi Gorila!
Dialogue: 0,0:05:22.99,0:05:24.86,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bahkan dirakit ulang
Dialogue: 0,0:05:24.95,0:05:28.12,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dengan sempurna\Nsampai para ahli pun tak tahu.
Dialogue: 0,0:05:28.20,0:05:30.66,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kami "hancurkan yang lama"\Ndan "bangun yang baru".
Dialogue: 0,0:05:30.74,0:05:32.37,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu aturan perusahaan kami.
Dialogue: 0,0:05:32.45,0:05:34.87,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu tak dirakit kembali jadi Gorila.
Dialogue: 0,0:05:34.87,0:05:36.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukan Gorila, tapi COBRA!
Dialogue: 0,0:05:36.33,0:05:39.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kita sudah lulus, tapi masih\Nberbagi pekerjaan.
Dialogue: 0,0:05:39.21,0:05:40.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dunia memang sempit.
Dialogue: 0,0:05:41.25,0:05:42.55,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi, Pak Hijikata,
Dialogue: 0,0:05:42.63,0:05:45.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}apa pekerjaanmu sekarang?
Dialogue: 0,0:05:46.09,0:05:49.55,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bisa dibilang,\Naku pebisnis elite internasional.
Dialogue: 0,0:05:50.85,0:05:52.85,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku keliling dunia
Dialogue: 0,0:05:52.93,0:05:55.52,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}untuk mengumpulkan berbagai material bekas.
Dialogue: 0,0:05:55.89,0:05:58.35,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tunggu, itu botol mayones,\Nbukan material bekas!
Dialogue: 0,0:05:58.65,0:06:03.23,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Klien utamaku perusahaan konstruksi\Nseperti milik Kondou atau Shimura.
Dialogue: 0,0:06:03.73,0:06:06.99,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bisnis konstruksi macam apa\Nyang membangun rumah
Dialogue: 0,0:06:07.07,0:06:08.16,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dari kardus?
Dialogue: 0,0:06:08.41,0:06:11.53,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tolong… Berikan benda itu padaku.
Dialogue: 0,0:06:11.62,0:06:13.74,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Omong-omong, kenapa Hasegawa di sini?
Dialogue: 0,0:06:13.83,0:06:16.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Meski tak berguna bagi sebagian orang,
Dialogue: 0,0:06:16.33,0:06:20.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tetap berguna bagi orang lain.
Dialogue: 0,0:06:20.25,0:06:23.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berguna?\NItu tak ada gizinya!
Dialogue: 0,0:06:23.21,0:06:27.09,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lagi pula, itu bukan hanya\Npenjualan satu arah.
Dialogue: 0,0:06:27.17,0:06:29.80,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terkadang, kami beli barang\Nyang tak diinginkan klien
Dialogue: 0,0:06:29.89,0:06:31.89,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}lalu dijual ke orang lain.
Dialogue: 0,0:06:31.97,0:06:33.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini simbiosis mutualisme.
Dialogue: 0,0:06:33.89,0:06:35.02,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hubungan macam apa itu?
Dialogue: 0,0:06:35.10,0:06:36.77,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ada gorila di situ.
Dialogue: 0,0:06:36.85,0:06:39.56,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bagian tubuh Gorila akan jadi tak berguna!
Dialogue: 0,0:06:39.81,0:06:42.15,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baik Kondou atau Hijikata,
Dialogue: 0,0:06:42.23,0:06:45.61,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}masih saling kontak meski sudah lulus.
Dialogue: 0,0:06:45.69,0:06:48.03,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi, apa pekerjaanmu sekarang?
Dialogue: 0,0:06:48.11,0:06:49.53,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau bertanya padaku?
Dialogue: 0,0:06:49.61,0:06:51.62,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku seniman rakugo.
Dialogue: 0,0:06:51.70,0:06:54.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau pasti akan segera promosi\Nke tingkat Shinuchi.
Dialogue: 0,0:06:54.91,0:06:56.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Shinuchi di usiamu ini?
Dialogue: 0,0:06:56.66,0:06:58.91,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Cukup cepat untuk industri ini.
Dialogue: 0,0:06:59.00,0:07:01.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa dianggap terlalu dini?
Dialogue: 0,0:07:02.25,0:07:04.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku temukan aib guruku\Nlalu menginterogasinya.
Dialogue: 0,0:07:04.50,0:07:06.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia langsung setuju.
Dialogue: 0,0:07:06.67,0:07:08.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia bukan seniman rakugo,
Dialogue: 0,0:07:08.05,0:07:09.76,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tapi pemeras.
Dialogue: 0,0:07:09.76,0:07:12.64,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi seniman rakugo itu menarik.
Dialogue: 0,0:07:12.72,0:07:14.72,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baru kali ini aku bertemu langsung\Nseniman rakugo.
Dialogue: 0,0:07:14.72,0:07:16.60,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ceritakan lelucon lucu
Dialogue: 0,0:07:16.68,0:07:18.52,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dan buat semua tertawa.
Dialogue: 0,0:07:18.60,0:07:20.14,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sepertinya aku tak punya pilihan.
Dialogue: 0,0:07:20.23,0:07:22.56,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baiklah, akan kuceritakan satu kisah.
Dialogue: 0,0:07:28.65,0:07:30.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini lelucon untukmu.
Dialogue: 0,0:07:31.20,0:07:34.91,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kabarnya rumah sebelah punya dinding baru.
Dialogue: 0,0:07:36.95,0:07:38.37,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa-apaan ini?
Dialogue: 0,0:07:38.45,0:07:41.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kenapa si tak berguna Kondou\Ndan aku terjebak di dalamnya?
Dialogue: 0,0:07:41.75,0:07:43.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Meski tak berguna bagi sebagian orang,
Dialogue: 0,0:07:44.00,0:07:48.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tetap jadi sumber daya berguna\Nbagi orang lain.
Dialogue: 0,0:07:48.30,0:07:49.76,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Akhirnya, semua bahagia.
Dialogue: 0,0:07:50.26,0:07:51.47,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yang benar saja!
Dialogue: 0,0:07:51.55,0:07:53.68,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Siapa yang kau sebut tak berguna?
Dialogue: 0,0:07:55.97,0:07:56.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku iri padamu.
Dialogue: 0,0:07:56.89,0:07:59.89,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Masih berhubungan dengan teman kelas\Nsetelah lulus.
Dialogue: 0,0:08:00.39,0:08:04.31,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Nona Sarutobi, apa pekerjaanmu sekarang?
Dialogue: 0,0:08:04.40,0:08:05.69,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku akuntan.
Dialogue: 0,0:08:05.77,0:08:08.53,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Akuntan di perusahaan kecil milik kerabat.
Dialogue: 0,0:08:08.61,0:08:10.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Luar biasa.
Dialogue: 0,0:08:10.49,0:08:12.07,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bagi pemilik bisnis,
Dialogue: 0,0:08:12.15,0:08:13.86,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}akuntan itu sangat penting.
Dialogue: 0,0:08:14.32,0:08:15.32,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Benar.
Dialogue: 0,0:08:15.41,0:08:20.16,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dibandingkan dulu yang vulgar\Ndan mesum, dia banyak berubah. Hebat!
Dialogue: 0,0:08:20.25,0:08:22.16,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tae, itu bukan pujian.
Dialogue: 0,0:08:22.25,0:08:23.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa yang kau lalui
Dialogue: 0,0:08:23.62,0:08:26.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}demi mendapatkan pekerjaan layak itu?
Dialogue: 0,0:08:27.21,0:08:29.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukan masalah besar, kok.
Dialogue: 0,0:08:29.67,0:08:31.47,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Setelah lulus SMA,
Dialogue: 0,0:08:32.26,0:08:34.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}aku cari kerja di mana-mana.
Dialogue: 0,0:08:34.51,0:08:37.14,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi tak ada perusahaan yang cocok.
Dialogue: 0,0:08:37.39,0:08:40.35,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Akhirnya aku bantu bisnis keluarga
Dialogue: 0,0:08:40.42,0:08:42.30,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dan tinggal di kampung halaman.
Dialogue: 0,0:08:42.39,0:08:43.80,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi…
Dialogue: 0,0:08:47.36,0:08:49.48,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Makin lama di rumah,
Dialogue: 0,0:08:49.57,0:08:52.36,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}makin terpikir orang itu dan terasa hampa.
Dialogue: 0,0:08:53.07,0:08:55.74,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi, aku putuskan kerja di perusahaan
Dialogue: 0,0:08:55.82,0:08:58.53,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}agar orang tuaku tenang.
Dialogue: 0,0:08:59.54,0:09:01.37,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yang lebih penting,
Dialogue: 0,0:09:01.37,0:09:05.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}aku harus lanjut hidup demi melupakan dia.
Dialogue: 0,0:09:06.79,0:09:09.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kugunakan koneksiku
Dialogue: 0,0:09:09.25,0:09:11.76,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}untuk kerja di perusahaan kerabat
Dialogue: 0,0:09:21.97,0:09:23.85,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dan membalas hubungan masa laluku.
Dialogue: 0,0:09:23.93,0:09:25.48,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi itu maksudmu dengan akuntansi?
Dialogue: 0,0:09:25.56,0:09:27.65,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bagaimana bisa disebut akuntan?
Dialogue: 0,0:09:27.73,0:09:29.61,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ada kata sandi di industri kami.
Dialogue: 0,0:09:29.69,0:09:31.78,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kami sebut pengawasan lokasi\Nsebagai "akuntansi".
Dialogue: 0,0:09:31.82,0:09:34.74,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lokasinya di TKP pembunuhan, paham?
Dialogue: 0,0:09:34.82,0:09:35.74,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa ini?
Dialogue: 0,0:09:35.82,0:09:38.49,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku cuma sampah pungutan, ya?
Dialogue: 0,0:09:39.41,0:09:41.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Meski tak berguna bagi sebagian orang,
Dialogue: 0,0:09:41.62,0:09:45.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tetap sumber daya berguna bagi orang lain.
Dialogue: 0,0:09:45.41,0:09:46.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sumber daya apa?
Dialogue: 0,0:09:46.67,0:09:47.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lalu aku mayat?
Dialogue: 0,0:09:47.96,0:09:49.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku sama sekali tak ingat ini!
Dialogue: 0,0:09:51.34,0:09:52.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dengan begitu,
Dialogue: 0,0:09:52.96,0:09:54.80,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}kami berkeliling dunia
Dialogue: 0,0:09:54.88,0:09:57.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}mengumpulkan berbagai material bekas.
Dialogue: 0,0:09:57.68,0:09:59.80,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi mereka juga di sini.
Dialogue: 0,0:10:02.01,0:10:04.10,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bahkan dirakit ulang
Dialogue: 0,0:10:04.18,0:10:07.19,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dengan sempurna\Nsampai para ahli pun tak tahu.
Dialogue: 0,0:10:08.27,0:10:11.02,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kami "hancurkan yang lama"\Ndan "bangun yang baru".
Dialogue: 0,0:10:11.11,0:10:12.32,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu aturan perusahaan kami.
Dialogue: 0,0:10:12.32,0:10:14.32,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini video promosi perusahaan mana?
Dialogue: 0,0:10:14.40,0:10:17.45,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pantas ingatanku buram belakangan ini.
Dialogue: 0,0:10:17.53,0:10:21.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pantas saja USB\Ndi dekat selangkanganku tegak.
Dialogue: 0,0:10:21.91,0:10:24.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu cuma fenomena fisiologis.
Dialogue: 0,0:10:24.62,0:10:26.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Omong-omong, apa aku siborg?
Dialogue: 0,0:10:29.37,0:10:33.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mau kuperbaiki agar ingatanmu kembali?
Dialogue: 0,0:10:33.50,0:10:34.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau begitu, lupakan reuni.
Dialogue: 0,0:10:34.71,0:10:36.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini jadi pesta perpisahan.
Dialogue: 0,0:10:36.51,0:10:37.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tunggu, Tama!
Dialogue: 0,0:10:37.67,0:10:41.35,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Omong-omong,\Naku ingat semua begitu menutup mata.
Dialogue: 0,0:10:41.43,0:10:44.85,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Karyawisata, festival budaya,\Nhari olahraga.
Dialogue: 0,0:10:47.43,0:10:49.48,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Karena tak tayang di anime utama,
Dialogue: 0,0:10:49.56,0:10:51.31,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}kalian pasti bingung.
Dialogue: 0,0:10:51.40,0:10:54.02,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Benar. Lagi pula, ini SMA.
Dialogue: 0,0:10:54.11,0:10:56.07,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tentu saja ada hari olahraga.
Dialogue: 0,0:10:57.78,0:11:00.28,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Chappa sulit dihadapi.
Dialogue: 0,0:11:00.36,0:11:01.87,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini bukan Hari Olahraga.
Dialogue: 0,0:11:01.95,0:11:04.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini turnamen bela diri!
Dialogue: 0,0:11:08.37,0:11:10.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ujian Tengah Semester juga melelahkan.
Dialogue: 0,0:11:11.00,0:11:12.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini Ujian Chunin, bukan?
Dialogue: 0,0:11:12.75,0:11:14.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi anime lain.
Dialogue: 0,0:11:15.05,0:11:17.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kita jarang berkumpul begini.
Dialogue: 0,0:11:17.21,0:11:20.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bagaimana jika petinggi memarahi kita\Nsampai anime ini dihentikan?
Dialogue: 0,0:11:21.05,0:11:24.51,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berhenti membicarakan\Nmahakarya terbesar Jump!
Dialogue: 0,0:11:24.60,0:11:26.52,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau benar, Shinpachi.
Dialogue: 0,0:11:27.06,0:11:28.18,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Suara itu…
Dialogue: 0,0:11:29.02,0:11:30.27,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Reuni...
Dialogue: 0,0:11:30.35,0:11:34.23,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}adalah tempat teman lama berkumpul\Nuntuk saling menyemangati
Dialogue: 0,0:11:34.31,0:11:38.86,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}sebagai asupan batin\Nuntuk petualangan baru esok.
Dialogue: 0,0:11:39.70,0:11:41.57,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukan untuk mengenang masa lalu
Dialogue: 0,0:11:41.66,0:11:43.95,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}atau seperti kompilasi anime
Dialogue: 0,0:11:44.03,0:11:47.62,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}yang dibuat untuk lari dari kenyataan\Nkarena terjebak masa lalu.
Dialogue: 0,0:11:48.41,0:11:49.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Belum lagi…
Dialogue: 0,0:11:50.16,0:11:52.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}anime lain bahkan tak boleh dibahas.
Dialogue: 0,0:11:52.54,0:11:56.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lupakan anime. Kau malah bahas gim!
Dialogue: 0,0:11:57.05,0:11:58.34,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yang benar saja.
Dialogue: 0,0:11:58.42,0:12:01.30,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa yang kau lakukan setelah dewasa?
Dialogue: 0,0:12:01.63,0:12:03.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku sering ganti pekerjaan.
Dialogue: 0,0:12:03.59,0:12:05.22,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sekarang, aku Pahlawan.
Dialogue: 0,0:12:05.30,0:12:06.14,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Siapa peduli?
Dialogue: 0,0:12:06.56,0:12:09.89,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Otakmu masih Level 1\Ndi kota awal Ariahan, Zura!
Dialogue: 0,0:12:09.98,0:12:11.94,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukan Zura, tapi Zurako!
Dialogue: 0,0:12:12.02,0:12:14.06,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu maksudmu ganti pekerjaan?
Dialogue: 0,0:12:14.23,0:12:17.11,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku juga sudah ganti pekerjaan\Nbeberapa kali.
Dialogue: 0,0:12:17.19,0:12:19.99,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Akhirnya, aku putuskan jadi Orang Bijak.
Dialogue: 0,0:12:20.07,0:12:22.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi kau masih buaya darat.
Dialogue: 0,0:12:22.20,0:12:24.62,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudah level maksimal,\Ntapi belum berubah sama sekali?
Dialogue: 0,0:12:24.70,0:12:26.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan salah paham, Tuan Muda.
Dialogue: 0,0:12:26.33,0:12:28.62,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku sadar demi mencapai\NMode Bijak pada akhirnya,
Dialogue: 0,0:12:28.70,0:12:30.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}aku harus bersenang-senang
Dialogue: 0,0:12:30.29,0:12:32.87,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}- sampai saat terakhir, Tuan Muda.\N- Tae, ada yang bicara denganku?
Dialogue: 0,0:12:32.87,0:12:33.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Benarkah?
Dialogue: 0,0:12:34.33,0:12:36.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa aku salah dengar?
Dialogue: 0,0:12:37.04,0:12:40.01,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku juga, sudah beberapa kali\Nganti pekerjaan.
Dialogue: 0,0:12:40.09,0:12:43.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tanpa sadar, aku jadi pengangguran hibrida.
Dialogue: 0,0:12:43.22,0:12:44.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukan, perusahaan pembongkaran mencurigakan
Dialogue: 0,0:12:44.13,0:12:45.09,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}merampok
Dialogue: 0,0:12:45.09,0:12:46.89,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tubuh
Dialogue: 0,0:12:46.97,0:12:47.80,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dan ingatan aslimu.
Dialogue: 0,0:12:48.56,0:12:50.64,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kenapa ada Chappa?
Dialogue: 0,0:12:52.10,0:12:54.06,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Shinpachi, lama tak jumpa.
Dialogue: 0,0:12:54.35,0:12:57.31,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kagura, apa yang kau lakukan?
Dialogue: 0,0:12:57.69,0:12:59.57,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku ganti pekerjaan berkali-kali.
Dialogue: 0,0:12:59.57,0:13:01.69,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sekarang aku Chappa.
Dialogue: 0,0:13:02.95,0:13:05.53,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kubawakan handuk basah ekstra.
Dialogue: 0,0:13:06.82,0:13:10.95,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Memangnya ada Chappa profesional?
Dialogue: 0,0:13:16.67,0:13:18.09,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pada akhirnya,
Dialogue: 0,0:13:18.17,0:13:20.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Takasugi dan yang lain tak datang.
Dialogue: 0,0:13:20.63,0:13:24.09,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tak kusangka semua pergi secepat itu.
Dialogue: 0,0:13:24.17,0:13:26.64,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yah, semua orang punya kehidupan baru.
Dialogue: 0,0:13:27.72,0:13:29.56,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukankah Katsura juga bilang?
Dialogue: 0,0:13:29.64,0:13:34.48,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Reuni adalah asupan batin
Dialogue: 0,0:13:34.56,0:13:36.98,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}untuk petualangan baru esok.
Dialogue: 0,0:13:37.69,0:13:39.19,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau benar.
Dialogue: 0,0:13:39.27,0:13:44.57,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi aku masih ingin mengobrol\Nsoal masa lalu.
Dialogue: 0,0:13:44.65,0:13:48.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Shinpachi, apa kenanganmu\Nyang paling berkesan?
Dialogue: 0,0:13:51.12,0:13:53.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Banyak yang tak terlupakan,
Dialogue: 0,0:13:53.16,0:13:54.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tapi pastinya
Dialogue: 0,0:13:54.25,0:13:56.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Operasi Menyontek.
Dialogue: 0,0:13:56.92,0:13:59.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Menyontek bukan hal yang membanggakan,
Dialogue: 0,0:14:00.54,0:14:03.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tapi saat ingat kita serius\Nmelakukan hal bodoh itu,
Dialogue: 0,0:14:04.05,0:14:05.51,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}aku jadi nostalgia.
Dialogue: 0,0:14:05.97,0:14:07.43,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Shinpachi…
Dialogue: 0,0:14:08.22,0:14:11.51,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hanya saja sebagai orang dewasa, aku sadar
Dialogue: 0,0:14:11.60,0:14:15.43,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tak ada jawaban hidup\Nyang didapat dari menyontek.
Dialogue: 0,0:14:16.19,0:14:17.56,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Shin…
Dialogue: 0,0:14:19.27,0:14:21.73,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maaf, aku tak sengaja bikin suasana suram.
Dialogue: 0,0:14:21.82,0:14:22.98,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maafkan aku.
Dialogue: 0,0:14:23.07,0:14:25.57,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ayo ulang dengan suasana lebih ringan.
Dialogue: 0,0:14:28.20,0:14:29.36,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bodoh!
Dialogue: 0,0:14:29.45,0:14:31.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudah kubilang bukan begitu!
Dialogue: 0,0:14:31.91,0:14:33.16,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maafkan aku.
Dialogue: 0,0:14:33.24,0:14:34.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hei, kau.
Dialogue: 0,0:14:34.16,0:14:36.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Entah pakai sontekan atau apa pun,
Dialogue: 0,0:14:36.54,0:14:39.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tak bisakah kau lebih berguna sedikit?
Dialogue: 0,0:14:41.54,0:14:44.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mengoceh terus.
Dialogue: 0,0:14:44.30,0:14:46.55,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalian lagi berahi, ya?
Dialogue: 0,0:14:49.47,0:14:52.18,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak Ginpachi, kenapa?
Dialogue: 0,0:14:52.26,0:14:54.89,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Shinpachi, kau sendiri bilang
Dialogue: 0,0:14:54.97,0:14:57.73,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}harus diulang dengan suasana ringan?
Dialogue: 0,0:14:59.65,0:15:02.73,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak Ginpachi… Teman-teman…
Dialogue: 0,0:15:03.32,0:15:04.23,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lagi pula,
Dialogue: 0,0:15:04.32,0:15:07.03,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}ada beberapa yang tidak datang.
Dialogue: 0,0:15:11.78,0:15:13.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mau bagaimana lagi.
Dialogue: 0,0:15:13.66,0:15:15.41,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kami ikut saja.
Dialogue: 0,0:15:17.37,0:15:20.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baiklah, ayo kita ulangi.
Dialogue: 0,0:16:52.55,0:16:54.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku akan
Dialogue: 0,0:16:54.47,0:16:56.80,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}langsung ke intinya, Pak Sakata.
Dialogue: 0,0:16:56.89,0:16:59.26,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Murid-muridmu
Dialogue: 0,0:16:59.35,0:17:00.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dari Kelas 3-Z
Dialogue: 0,0:17:01.02,0:17:03.31,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}punya
Dialogue: 0,0:17:03.39,0:17:05.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}nilai yang buruk.
Dialogue: 0,0:17:05.56,0:17:07.27,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa-apaan nilai ujian ini?
Dialogue: 0,0:17:07.35,0:17:09.48,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yakin ini bukan usia rata-rata
Dialogue: 0,0:17:09.48,0:17:10.31,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}pelawak muda?
Dialogue: 0,0:17:10.40,0:17:13.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bagaimana bisa dapat nilai begini?
Dialogue: 0,0:17:13.28,0:17:15.10,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Selalu begini kalau ujian.
Dialogue: 0,0:17:20.29,0:17:22.41,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tentu saja aku tahu.
Dialogue: 0,0:17:22.50,0:17:23.86,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku wali kelasnya.
Dialogue: 0,0:17:23.96,0:17:26.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak Sakata, dengarkan baik-baik.
Dialogue: 0,0:17:26.32,0:17:27.57,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baiklah.
Dialogue: 0,0:17:29.84,0:17:32.26,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau mau aku lakukan apa?
Dialogue: 0,0:17:32.34,0:17:33.72,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia dianggap kepala sekolah.
Dialogue: 0,0:17:33.80,0:17:35.55,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tak usah pakai "dianggap".
Dialogue: 0,0:17:35.63,0:17:39.22,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau begitu, aku ambil tindakan tegas.
Dialogue: 0,0:17:39.43,0:17:40.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tindakan tegas?
Dialogue: 0,0:17:41.06,0:17:43.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Di ujian pascaistirahat pekan depan,
Dialogue: 0,0:17:43.89,0:17:46.14,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tak masalah walau hanya satu pelajaran,
Dialogue: 0,0:17:46.23,0:17:48.06,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}nilai rata-rata kelas minimal 80.
Dialogue: 0,0:17:48.15,0:17:50.44,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jika mereka tak mampu,
Dialogue: 0,0:17:51.28,0:17:54.07,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kelas 3-Z harus ikut\Nkelas remedial di akhir pekan.
Dialogue: 0,0:17:54.15,0:17:55.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Selain itu,
Dialogue: 0,0:17:55.28,0:17:58.07,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}gajimu dipotong 20 persen!
Dialogue: 0,0:17:58.16,0:18:00.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Potong gaji…
Dialogue: 0,0:18:01.62,0:18:04.12,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kupotong 20 persen darimu duluan!
Dialogue: 0,0:18:04.20,0:18:05.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aduh…
Dialogue: 0,0:18:05.58,0:18:07.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sungguh? Kau bercanda, ya?
Dialogue: 0,0:18:07.54,0:18:08.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sakit.
Dialogue: 0,0:18:08.58,0:18:10.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu mauku, Kepsek sialan.
Dialogue: 0,0:18:10.84,0:18:12.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak, kau yang bikin aku berdarah.
Dialogue: 0,0:18:13.01,0:18:15.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baiklah, tantangan diterima.
Dialogue: 0,0:18:15.13,0:18:16.68,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Akan kubuat mereka melakukannya.
Dialogue: 0,0:18:17.26,0:18:20.64,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukan cuma akhir pekan,\Ntapi juga remedial di hari biasa.
Dialogue: 0,0:18:21.14,0:18:25.27,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Potong gajiku sepuluh persen\Njuga tak masalah.
Dialogue: 0,0:18:25.35,0:18:28.48,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau jelas-jelas kurangi hukumanmu.
Dialogue: 0,0:18:28.56,0:18:29.77,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Omong-omong, Pak Hata,
Dialogue: 0,0:18:29.86,0:18:32.65,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}karena Pak Sakata menyarankannya,
Dialogue: 0,0:18:32.73,0:18:34.99,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}kenapa tak biarkan mereka\Nikut remedial hari biasa?
Dialogue: 0,0:18:35.07,0:18:36.99,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pakai gelar hormat, Wakil Kepsek.
Dialogue: 0,0:18:37.07,0:18:39.41,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maafkan aku. Tidak, maaf.
Dialogue: 0,0:18:39.49,0:18:41.70,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yakin tak akan ada masalah?
Dialogue: 0,0:18:41.78,0:18:43.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak apa-apa.
Dialogue: 0,0:18:43.29,0:18:46.37,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi gajiku\Nhanya boleh dipotong sepuluh persen.
Dialogue: 0,0:18:46.46,0:18:47.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau masih tak mau mengalah?
Dialogue: 0,0:18:47.83,0:18:49.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tentu saja.
Dialogue: 0,0:18:49.33,0:18:51.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baiklah, Pak Berdarah.
Dialogue: 0,0:18:51.29,0:18:54.51,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudah kubilang,\Nkau yang bikin aku berdarah!
Dialogue: 0,0:18:55.34,0:18:59.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku nantikan hasilnya,
Dialogue: 0,0:18:59.47,0:19:03.51,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak Ginpachi dari Kelas 3-Z.
Dialogue: 0,0:19:08.94,0:19:10.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi, begitu ceritanya.
Dialogue: 0,0:19:10.81,0:19:15.98,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Di ujian nanti, meski cuma satu pelajaran,\Nnilai kalian harus 80.
Dialogue: 0,0:19:16.28,0:19:17.28,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa?
Dialogue: 0,0:19:17.65,0:19:20.99,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kenapa ulang dari episode satu\Nanime Ginpachi?
Dialogue: 0,0:19:21.07,0:19:23.91,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau bilang kita harus lakukan lagi\Ndengan suasana ringan.
Dialogue: 0,0:19:23.99,0:19:26.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukan reka ulang dari awal!
Dialogue: 0,0:19:26.16,0:19:28.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maksudku, kita harus reuni lagi, paham?
Dialogue: 0,0:19:28.87,0:19:30.37,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Shinpachi.
Dialogue: 0,0:19:30.62,0:19:32.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Omong-omong, kenapa Kagura\Nmasih jadi Chappa
Dialogue: 0,0:19:32.71,0:19:34.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}di klimaks episode terakhir?
Dialogue: 0,0:19:39.13,0:19:42.18,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Katsura malah berubah jadi makhluk lain!
Dialogue: 0,0:19:42.26,0:19:44.56,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku bukan Katsura. Aku Mama-san Zurako.
Dialogue: 0,0:19:44.89,0:19:48.52,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Waktu hanya berlalu di ruang waktu mental.
Dialogue: 0,0:19:49.27,0:19:50.73,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa hubungannya?
Dialogue: 0,0:19:53.73,0:19:57.36,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kita harus mulai Arc Festival Olahraga,\Natau durasinya habis.
Dialogue: 0,0:19:58.32,0:20:00.61,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kenapa tiba-tiba jadi turnamen bela diri?
Dialogue: 0,0:20:00.70,0:20:02.91,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Chappa menang diam-diam!
Dialogue: 0,0:20:03.16,0:20:06.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kami juga siap\Nuntuk Arc Ujian Tengah Semester!
Dialogue: 0,0:20:07.41,0:20:09.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tak boleh tayangkan ini!
Dialogue: 0,0:20:09.12,0:20:11.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini benar-benar "Naruto"!
Dialogue: 0,0:20:11.25,0:20:13.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku semangat sekali!
Dialogue: 0,0:20:13.13,0:20:13.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hei.
Dialogue: 0,0:20:14.04,0:20:16.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku tak paham lagi pandangan duniamu!
Dialogue: 0,0:20:16.46,0:20:18.51,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukankah mereka berdua ikut tes lain?
Dialogue: 0,0:20:18.59,0:20:21.01,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau tak bisa mengakhirinya sekacau ini.
Dialogue: 0,0:20:21.09,0:20:23.34,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Omong-omong,\Nayo cepat kembali ke topik tadi.
Dialogue: 0,0:20:23.43,0:20:24.76,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kembali ke yang tadi?
Dialogue: 0,0:20:25.39,0:20:26.85,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bagaimana caranya?
Dialogue: 0,0:20:29.60,0:20:31.77,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Takasugi?
Dialogue: 0,0:20:32.31,0:20:34.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saksikan aku hancurkan segalanya.
Dialogue: 0,0:20:34.48,0:20:37.61,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kuhancurkan jeruk busuk\Ndan makan siang sekolah ini.
Dialogue: 0,0:20:38.11,0:20:40.69,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kenapa kalian kembali ke SD?
Dialogue: 0,0:20:40.78,0:20:42.91,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukan itu maksudku!
Dialogue: 0,0:20:43.28,0:20:44.74,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku Manusia Mayones!
Dialogue: 0,0:20:44.95,0:20:46.41,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku Manusia Kacamata Hitam!
Dialogue: 0,0:20:46.49,0:20:48.12,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku ereksi!
Dialogue: 0,0:20:48.20,0:20:51.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadilah siborg lagi kalau kalian mau!
Dialogue: 0,0:20:52.12,0:20:55.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kenapa kalian masih tak berguna\Nsampai akhir?
Dialogue: 0,0:20:55.25,0:20:57.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tak berguna? Bukankah itu sempurna?
Dialogue: 0,0:20:59.13,0:21:01.30,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Makhluk seperti manusia
Dialogue: 0,0:21:01.63,0:21:03.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}memang semuanya tak berguna.
Dialogue: 0,0:21:04.22,0:21:06.43,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi setiap orang
Dialogue: 0,0:21:06.51,0:21:08.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}punya nilai masing-masing.
Dialogue: 0,0:21:08.43,0:21:10.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukankah mereka juga bilang?
Dialogue: 0,0:21:10.89,0:21:13.19,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Meski sampah bagi sebagian orang,
Dialogue: 0,0:21:13.27,0:21:18.19,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tetap berguna bagi orang lain.
Dialogue: 0,0:21:19.23,0:21:20.61,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dengan kata lain,
Dialogue: 0,0:21:21.11,0:21:25.07,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}"hancurkan yang lama" dan\N"bangun yang baru" untuk umat manusia.
Dialogue: 0,0:21:25.16,0:21:26.99,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu aturan sekolah kami.
Dialogue: 0,0:21:27.07,0:21:29.99,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ternyata video promosi lagi!
Dialogue: 0,0:21:34.12,0:21:35.37,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lumayan.
Dialogue: 0,0:21:35.46,0:21:37.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bagi kalian,\Nitu pekerjaan yang cukup bagus.
Dialogue: 0,0:21:38.46,0:21:39.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi kenapa
Dialogue: 0,0:21:39.38,0:21:40.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tiba-tiba
Dialogue: 0,0:21:40.59,0:21:41.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}lakukan ini?
Dialogue: 0,0:21:41.51,0:21:43.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau ingin buat video humas
Dialogue: 0,0:21:43.26,0:21:45.55,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}berisi mereka bersenang-senang.
Dialogue: 0,0:21:45.63,0:21:46.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jelas sekali, 'kan?
Dialogue: 0,0:21:47.05,0:21:48.93,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Untuk yakinkan atasan,
Dialogue: 0,0:21:49.01,0:21:52.52,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}kita butuh sesuatu yang nyata.
Dialogue: 0,0:21:53.35,0:21:54.60,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maksudmu…
Dialogue: 0,0:21:54.69,0:21:56.10,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kita akan melanjutkannya?
Dialogue: 0,0:21:57.35,0:21:59.77,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Anime Ginpachi Musim Kedua…
Dialogue: 0,0:22:00.94,0:22:04.36,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Masih banyak materi drama sekolah\Nyang belum terpakai.
Dialogue: 0,0:22:04.44,0:22:08.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sayang kalau tamat satu musim saja.
Dialogue: 0,0:22:08.95,0:22:11.66,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Astaga, merepotkan sekali.
Dialogue: 0,0:22:12.33,0:22:15.91,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mau bagaimana lagi.\NKami ikut saja.
Dialogue: 0,0:22:17.79,0:22:20.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kelas 3-Z.
Dialogue: 0,0:22:21.09,0:22:23.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}- Pak Ginpachi.
Dialogue: 0,0:22:27.13,0:22:29.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi belum putus, ya!
