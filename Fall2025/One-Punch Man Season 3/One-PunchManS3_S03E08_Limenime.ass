Dialogue: 0,0:00:22.29,0:00:24.67,tanda,,0000,0000,0000,,RAJA KENTANG
Dialogue: 0,0:02:57.03,0:03:00.95,tanda,,0000,0000,0000,,BABAK SHINOBI
Dialogue: 0,0:05:35.79,0:05:38.03,tanda,,0000,0000,0000,,"LEVEL BENCANA: IBLIS, ANGIN RIBUT"
Dialogue: 0,0:05:39.74,0:05:41.86,tanda,,0000,0000,0000,,"LEVEL BENCANA: IBLIS, API NERAKA"
Dialogue: 0,0:08:54.51,0:08:58.02,tanda,,0000,0000,0000,,"NAGA, API NERAKA - NAGA, ANGIN RIBUT"
Dialogue: 0,0:11:53.85,0:11:55.57,tanda,,0000,0000,0000,,TENDANGAN KILAT
Dialogue: 0,0:12:11.20,0:12:11.63,tanda,,0000,0000,0000,,TEBASAN CAHAYA KILAT
Dialogue: 0,0:13:57.61,0:13:58.86,tanda,,0000,0000,0000,,SRUK SRUK
Dialogue: 0,0:13:59.90,0:14:00.86,tanda,,0000,0000,0000,,BET BET
Dialogue: 0,0:14:03.74,0:14:04.95,tanda,,0000,0000,0000,,HYUUSH
Dialogue: 0,0:14:46.07,0:14:47.44,tanda,,0000,0000,0000,,TANK GURITA MINI - NOMOR 8
Dialogue: 0,0:17:25.66,0:17:28.02,tanda,,0000,0000,0000,,"LEVEL BENCANA: IBLIS, MANUSIA FENIKS"
