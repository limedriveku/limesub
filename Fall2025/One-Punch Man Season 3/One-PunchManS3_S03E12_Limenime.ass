Dialogue: 0,0:01:11.76,0:01:14.01,tanda,,0000,0000,0000,,"MENDING JANGAN" SUDAH KUPERINGATKAN, LO
Dialogue: 0,0:01:14.01,0:01:15.97,tanda,,0000,0000,0000,,AKU SUDAH TERLALU KUAT? TENTU TIDAK
Dialogue: 0,0:01:36.07,0:01:41.00,tanda,,0000,0000,0000,,HWAA HWAA HWAA
Dialogue: 0,0:03:11.05,0:03:11.99,tanda,,0000,0000,0000,,MAKHLUK SEMPURNA
Dialogue: 0,0:03:32.44,0:03:34.32,tanda,,0000,0000,0000,,LEVEL BENCANA: NAGA\NHAGUKI
