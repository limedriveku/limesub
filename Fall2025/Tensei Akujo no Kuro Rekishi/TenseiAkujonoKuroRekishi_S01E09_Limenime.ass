Dialogue: 0,0:04:13.55,0:04:17.29,tanda,,0000,0000,0000,,EPISODE 9: PENILAIAN DIRI SENDIRI ITU\NTIDAK BISA DIJADIKAN PEGANGAN
Dialogue: 0,0:04:57.91,0:04:58.51,tanda,,0000,0000,0000,,BUGH
Dialogue: 0,0:04:58.54,0:04:59.05,tanda,,0000,0000,0000,,HOEK
Dialogue: 0,0:06:06.92,0:06:07.36,tanda,,0000,0000,0000,,TAMENG UNTUK MELINDUNGI AURA JAHAT
Dialogue: 0,0:06:21.49,0:06:21.89,tanda,,0000,0000,0000,,LAYAR IMAJINASI MILIK IANA
Dialogue: 0,0:06:23.60,0:06:24.23,tanda,,0000,0000,0000,,TES IANA
Dialogue: 0,0:06:40.54,0:06:41.11,tanda,,0000,0000,0000,,LAYAR IMAJINASI
Dialogue: 0,0:06:42.22,0:06:42.87,tanda,,0000,0000,0000,,SATOU
Dialogue: 0,0:06:45.26,0:06:46.15,tanda,,0000,0000,0000,,ISTILAH ROTI TAWAR
Dialogue: 0,0:07:05.50,0:07:05.84,tanda,,0000,0000,0000,,WANITA LAINNYA
Dialogue: 0,0:07:13.55,0:07:15.18,tanda,,0000,0000,0000,,"KONOHA"
Dialogue: 0,0:08:47.14,0:08:47.87,tanda,,0000,0000,0000,,BUKAN UNTUK PERGI, TAPI PERGI UNTUK MATI!
Dialogue: 0,0:09:52.17,0:09:53.84,tanda,,0000,0000,0000,,"KEESOKAN HARINYA"
Dialogue: 0,0:10:59.32,0:10:59.60,tanda,,0000,0000,0000,,BERTARUNG, MELARIKAN DIRI, ITEM
Dialogue: 0,0:11:28.20,0:11:31.01,tanda,,0000,0000,0000,,THE DARK HISTORY OF THE REINCARNATED VILLAINESS
Dialogue: 0,0:11:31.04,0:11:32.61,tanda,,0000,0000,0000,,KEDIAMAN COUNT MAGNOLIA
Dialogue: 0,0:11:47.36,0:11:48.36,tanda,,0000,0000,0000,,KIRIMAN CORDELIA
Dialogue: 0,0:12:01.74,0:12:02.36,tanda,,0000,0000,0000,,UWOOH!
Dialogue: 0,0:12:16.32,0:12:17.30,tanda,,0000,0000,0000,,BEKU
Dialogue: 0,0:12:17.82,0:12:21.60,tanda,,0000,0000,0000,,GUNUNG BERSALJU LOKAL
Dialogue: 0,0:14:50.89,0:14:51.39,tanda,,0000,0000,0000,,SI IANA YANG MALAH KAGET
Dialogue: 0,0:16:00.41,0:16:01.12,tanda,,0000,0000,0000,,TOLONG SALING GAMBAR WAJAH KALIAN
Dialogue: 0,0:16:35.22,0:16:38.04,tanda,,0000,0000,0000,,BANTUAN
