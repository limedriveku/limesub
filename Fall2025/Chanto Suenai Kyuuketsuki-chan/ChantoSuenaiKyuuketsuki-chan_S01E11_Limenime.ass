Dialogue: 0,0:00:02.61,0:00:05.23,tanda,,0000,0000,0000,,KASIR
Dialogue: 0,0:00:05.23,0:00:07.90,tanda,,0000,0000,0000,,EIKO: KUMPUL DI SINI, PESTA PENUTUPAN!\NOKE!
Dialogue: 0,0:00:53.19,0:00:58.03,tanda,,0000,0000,0000,,LI'L MISS VAMPIRE CAN'T SUCK RIGHT
Dialogue: 0,0:02:02.96,0:02:08.00,tanda,,0000,0000,0000,,NONA VAMPIR KECIL DAN ULASAN
Dialogue: 0,0:04:04.81,0:04:08.81,tanda,,0000,0000,0000,,PAMERAN KLUB SENI
