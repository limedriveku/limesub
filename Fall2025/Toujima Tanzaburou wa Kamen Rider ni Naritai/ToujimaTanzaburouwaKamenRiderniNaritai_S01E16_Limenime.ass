Dialogue: 0,0:04:50.13,0:04:52.93,tanda,,0000,0000,0000,,TOJIMA TANZABURO WANTS TO BE A MASKED RIDER
Dialogue: 0,0:09:42.96,0:09:44.29,tanda,,0000,0000,0000,,TOLONG!
Dialogue: 0,0:09:44.29,0:09:46.89,tanda,,0000,0000,0000,,TOLONG! DI TEPI SUNGAI BIASA
Dialogue: 0,0:09:45.33,0:09:46.89,tanda,,0000,0000,0000,,DI BAWAH JALAN LAYANG
Dialogue: 0,0:10:29.83,0:10:40.47,tanda,,0000,0000,0000,,TOJIMA TANZABURO WANTS TO BE A MASKED RIDER
Dialogue: 0,0:20:42.37,0:20:49.78,tanda,,0000,0000,0000,,YANG PALSU TIDAK BISA MENGALAHKAN YANG ASLI
Dialogue: 0,0:22:23.31,0:22:25.83,tanda,,0000,0000,0000,,"RESTORAN KELUARGA"
Dialogue: 0,0:23:24.83,0:23:27.51,tanda,,0000,0000,0000,,EPISODE SELANJUTNYA
