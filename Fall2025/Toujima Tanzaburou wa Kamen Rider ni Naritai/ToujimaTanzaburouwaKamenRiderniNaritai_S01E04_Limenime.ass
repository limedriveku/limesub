Dialogue: 0,0:03:02.47,0:03:03.38,tanda,,0,0,0,,OKADA YURIKO\NAKU INGIN MINTA TOLONG
Dialogue: 0,0:04:37.19,0:04:39.82,tanda,,0,0,0,,TOJIMA TANZABURO WANTS\NTO BE A MASKED RIDER
Dialogue: 0,0:07:47.69,0:07:48.38,tanda,,0,0,0,,KASUS PERAMPOKAN MOBIL PENGANGKUT UANG TUNAI
Dialogue: 0,0:12:51.87,0:12:53.18,tanda,,0,0,0,,SIAPA KAMEN RIDER TERKUAT?
Dialogue: 0,0:12:56.96,0:13:06.97,tanda,,0,0,0,,TOJIMA TANZABURO WANTS\NTO BE A MASKED RIDER
Dialogue: 0,0:13:30.98,0:13:32.73,tanda,,0,0,0,,V3!
Dialogue: 0,0:17:26.38,0:17:27.92,tanda,,0,0,0,,V3!
Dialogue: 0,0:17:39.81,0:17:40.73,Default,,0,0,0,,A...
Dialogue: 0,0:21:04.09,0:21:06.30,tanda,,0,0,0,,RAMEN
Dialogue: 0,0:23:24.95,0:23:26.87,tanda,,0,0,0,,EPISODE SELANJUTNYA
Dialogue: 0,0:23:30.28,0:23:32.11,tanda,,0,0,0,,BIARKAN AKU
Dialogue: 0,0:23:32.87,0:23:35.26,tanda,,0,0,0,,JADI
