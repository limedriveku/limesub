Dialogue: 0,0:00:56.40,0:00:57.88,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Wahai Air, datanglah.{\b\fn\fs}
Dialogue: 0,0:02:46.45,0:02:47.88,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Di mana ini?{\b\fn\fs}
Dialogue: 0,0:02:49.50,0:02:51.69,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ini bukan alam baka.{\b\fn\fs}
Dialogue: 0,0:02:56.37,0:02:58.17,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Anda Mihara Ryo, bukan?{\b\fn\fs}
Dialogue: 0,0:02:58.69,0:03:00.85,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Benar, itu saya.{\b\fn\fs}
Dialogue: 0,0:03:01.72,0:03:02.97,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Syukurlah.{\b\fn\fs}
Dialogue: 0,0:03:03.05,0:03:05.93,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sebenarnya, Anda tamu pertama\Nsejak sekian lama.{\b\fn\fs}
Dialogue: 0,0:03:09.17,0:03:13.80,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Mihara Ryo, Anda meninggal di\Nkecelakaan lalu lintas.{\b\fn\fs}
Dialogue: 0,0:03:14.18,0:03:16.40,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ya, aku ingat.{\b\fn\fs}
Dialogue: 0,0:03:18.01,0:03:19.68,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Anda Tuhan?{\b\fn\fs}
Dialogue: 0,0:03:20.10,0:03:22.96,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Bukan, saya bukan Tuhan.{\b\fn\fs}
Dialogue: 0,0:03:23.26,0:03:26.34,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kalau saya andaikan dari apa\Nyang Anda tahu,{\b\fn\fs}
Dialogue: 0,0:03:26.80,0:03:29.22,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}saya bisa disebut "Malaikat".{\b\fn\fs}
Dialogue: 0,0:03:29.46,0:03:32.73,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Mari panggil dia\N"Michael (Sementara)" saja.{\b\fn\fs}
Dialogue: 0,0:03:34.44,0:03:39.94,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Saya tahu arti reinkarnasi, tapi apa\Nyang harus saya lakukan di sana?{\b\fn\fs}
Dialogue: 0,0:03:40.25,0:03:42.37,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Hiduplah sesuka Anda.{\b\fn\fs}
Dialogue: 0,0:03:42.66,0:03:49.77,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tak ada perintah untuk melakukan\Nsesuatu atau ditugasi sesuatu.{\b\fn\fs}
Dialogue: 0,0:03:50.30,0:03:51.80,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sesukaku...{\b\fn\fs}
Dialogue: 0,0:03:56.48,0:04:00.14,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kalau begitu, saya ingin\Nhidup santai.{\b\fn\fs}
Dialogue: 0,0:04:04.09,0:04:07.50,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tempat reinkarnasi Anda adalah\Ndunia dengan sihir.{\b\fn\fs}
Dialogue: 0,0:04:07.58,0:04:09.97,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}- Abel!\N- Dunia itu...{\b\fn\fs}
Dialogue: 0,0:04:10.05,0:04:11.73,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kami sebut "Phi".{\b\fn\fs}
Dialogue: 0,0:04:12.13,0:04:17.06,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Di Phi, umumnya satu dari lima orang\Nbisa sihir.{\b\fn\fs}
Dialogue: 0,0:04:17.60,0:04:19.80,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Elemen yang Anda miliki adalah...{\b\fn\fs}
Dialogue: 0,0:04:20.34,0:04:21.77,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Elemen air.{\b\fn\fs}
Dialogue: 0,0:04:22.88,0:04:25.33,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Karena Anda ingin hidup santai,{\b\fn\fs}
Dialogue: 0,0:04:25.41,0:04:28.36,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}saya pilihkan Hutan Rondo\Nsebagai tujuan reinkarnasi.{\b\fn\fs}
Dialogue: 0,0:04:30.09,0:04:34.41,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Saya sudah siapkan rumah dan makanan\Nyang cukup untuk dua bulan.{\b\fn\fs}
Dialogue: 0,0:04:35.02,0:04:36.52,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ruang penyimpanan es...{\b\fn\fs}
Dialogue: 0,0:04:37.12,0:04:39.21,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Apa ini juga sihir?{\b\fn\fs}
Dialogue: 0,0:04:40.28,0:04:44.50,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Saya atur agar monster menjauhi\Ndaerah sekitar rumah Anda.{\b\fn\fs}
Dialogue: 0,0:04:45.12,0:04:47.78,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Anggap saja semacam pelindung.{\b\fn\fs}
Dialogue: 0,0:04:48.50,0:04:50.73,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ensiklopedi monster level pemula.{\b\fn\fs}
Dialogue: 0,0:04:51.32,0:04:53.90,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Alfabet ini asing tapi\Nbisa kubaca.{\b\fn\fs}
Dialogue: 0,0:04:54.94,0:04:57.04,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Yang ini ensiklopedi tanaman.{\b\fn\fs}
Dialogue: 0,0:04:58.80,0:05:00.22,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Wahai Air, datanglah.{\b\fn\fs}
Dialogue: 0,0:05:03.37,0:05:04.33,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Air.{\b\fn\fs}
Dialogue: 0,0:05:04.69,0:05:05.73,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Water.{\b\fn\fs}
Dialogue: 0,0:05:09.41,0:05:13.42,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Jadi aku tak perlu melafalkan\Nasal aku bisa bayangkan.{\b\fn\fs}
Dialogue: 0,0:05:14.14,0:05:17.16,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Padahal aku sedikit terpikat\Ndengan mantra keren.{\b\fn\fs}
Dialogue: 0,0:05:20.09,0:05:21.97,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sudah kuduga, cuma bisa air dingin.{\b\fn\fs}
Dialogue: 0,0:05:23.46,0:05:27.21,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku ingin berendam air panas\Nkarena ada bak mandi bagus.{\b\fn\fs}
Dialogue: 0,0:05:27.32,0:05:29.21,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tapi sepertinya sulit.{\b\fn\fs}
Dialogue: 0,0:05:29.78,0:05:31.21,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Apa boleh buat.{\b\fn\fs}
Dialogue: 0,0:05:40.17,0:05:41.48,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku menyerah!{\b\fn\fs}
Dialogue: 0,0:05:43.01,0:05:45.06,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Menyerah juga penting.{\b\fn\fs}
Dialogue: 0,0:05:52.29,0:05:53.62,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Matahari...{\b\fn\fs}
Dialogue: 0,0:05:55.53,0:05:56.74,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Membekulah!{\b\fn\fs}
Dialogue: 0,0:05:59.76,0:06:01.13,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sulit, ya.{\b\fn\fs}
Dialogue: 0,0:06:01.78,0:06:04.56,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tapi aku harus bisa membuat es.{\b\fn\fs}
Dialogue: 0,0:06:04.86,0:06:08.45,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Habis, aku ingin coba pakai\Nsihir tombak es.{\b\fn\fs}
Dialogue: 0,0:06:08.76,0:06:11.56,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Padatkan air lalu eliminasi panas.{\b\fn\fs}
Dialogue: 0,0:06:13.26,0:06:16.60,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Bayangkan molekul H2O di pusat air.{\b\fn\fs}
Dialogue: 0,0:06:17.24,0:06:19.41,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kalau kugabungkan oksigen dari H2O{\b\fn\fs}
Dialogue: 0,0:06:19.53,0:06:23.56,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}dengan hidrogen dari molekul H20\Ndi sebelahnya...{\b\fn\fs}
Dialogue: 0,0:06:29.08,0:06:30.60,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku berhasil!{\b\fn\fs}
Dialogue: 0,0:06:30.88,0:06:33.58,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kunci sukses adalah ikatan hidrogen!{\b\fn\fs}
Dialogue: 0,0:06:33.84,0:06:36.06,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Lalu tinggal kubentuk!{\b\fn\fs}
Dialogue: 0,0:06:43.04,0:06:44.42,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Berhasil!{\b\fn\fs}
Dialogue: 0,0:06:47.36,0:06:50.80,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ini hasil lumayan untuk\Nhari pertama.{\b\fn\fs}
Dialogue: 0,0:06:52.44,0:06:56.21,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sepertinya aku bisa buat air panas\Ndengan memodifikasi cara buat es.{\b\fn\fs}
Dialogue: 0,0:06:56.77,0:06:58.86,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sihir itu praktis sekali, ya.{\b\fn\fs}
Dialogue: 0,0:06:59.93,0:07:03.32,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Nah, mulai besok pun\Npekerjaanku segunung.{\b\fn\fs}
Dialogue: 0,0:07:07.76,0:07:11.33,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Makanan yang diberi Michael cukup\Nuntuk dua bulan.{\b\fn\fs}
Dialogue: 0,0:07:11.72,0:07:16.38,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Jadi, lewat dari itu aku harus\Nberburu dan latihan masak.{\b\fn\fs}
Dialogue: 0,0:07:17.94,0:07:20.64,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Hutan ada di luar pembatas\Njadi pasti ada monster.{\b\fn\fs}
Dialogue: 0,0:07:21.00,0:07:23.66,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku tak akan merasa aman\Ndengan satu pisau.{\b\fn\fs}
Dialogue: 0,0:07:23.77,0:07:25.74,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku pasti butuh sihir menyerang.{\b\fn\fs}
Dialogue: 0,0:07:26.76,0:07:31.52,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Demi hidup santai yang aman tentram,\Naku harus latihan.{\b\fn\fs}
Dialogue: 0,0:07:33.42,0:07:34.78,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Water Ball.{\b\fn\fs}
Dialogue: 0,0:07:37.98,0:07:40.13,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sihir menyerang pertama sukses!{\b\fn\fs}
Dialogue: 0,0:07:40.62,0:07:43.50,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tapi sepertinya serangannya lemah.{\b\fn\fs}
Dialogue: 0,0:07:43.89,0:07:46.16,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sudah kuduga, sih.{\b\fn\fs}
Dialogue: 0,0:07:46.56,0:07:48.92,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tapi aku ada kartu rahasia!{\b\fn\fs}
Dialogue: 0,0:07:52.04,0:07:56.46,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kalau Water Ball tidak mempan,\Nbuat saja Water Jet!{\b\fn\fs}
Dialogue: 0,0:07:56.54,0:08:00.68,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Bayangkan semburan air tajam dan\Ncepat yang melukai target!{\b\fn\fs}
Dialogue: 0,0:08:01.04,0:08:02.06,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Maju!{\b\fn\fs}
Dialogue: 0,0:08:07.78,0:08:09.02,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku gagal.{\b\fn\fs}
Dialogue: 0,0:08:11.08,0:08:14.90,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Pertanyaan terakhir, bagaimana\Ncara pakai sihir?{\b\fn\fs}
Dialogue: 0,0:08:15.44,0:08:17.88,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Inti sihir adalah gambaran.{\b\fn\fs}
Dialogue: 0,0:08:18.24,0:08:19.64,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Water Jet!{\b\fn\fs}
Dialogue: 0,0:08:19.72,0:08:21.56,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}- Bayangkan detail.\N- Water Jet!{\b\fn\fs}
Dialogue: 0,0:08:21.65,0:08:24.28,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}- Lalu pupuk pengalaman.\N- Water Jet!{\b\fn\fs}
Dialogue: 0,0:08:25.57,0:08:27.89,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Hasilnya mulai menjanjikan.{\b\fn\fs}
Dialogue: 0,0:08:29.50,0:08:30.69,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Asik!{\b\fn\fs}
Dialogue: 0,0:08:30.88,0:08:34.69,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sudah kuduga, dengan membalikkan\Nproses membayangkan es,{\b\fn\fs}
Dialogue: 0,0:08:34.78,0:08:36.69,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}aku juga bisa buat air panas.{\b\fn\fs}
Dialogue: 0,0:08:37.56,0:08:39.04,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aduh, panasnya!{\b\fn\fs}
Dialogue: 0,0:08:41.25,0:08:43.78,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Buat es langsung di udara.{\b\fn\fs}
Dialogue: 0,0:08:49.32,0:08:54.60,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Memang memakan waktu, tapi\Naku bisa buat es di udara.{\b\fn\fs}
Dialogue: 0,0:08:55.28,0:08:58.56,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ini dia tujuan utama sihir es!{\b\fn\fs}
Dialogue: 0,0:08:58.97,0:09:00.50,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Icicle Lance!{\b\fn\fs}
Dialogue: 0,0:09:00.77,0:09:02.22,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Terbanglah!{\b\fn\fs}
Dialogue: 0,0:09:04.36,0:09:07.57,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tidak seperti air,\Ndia sulit terbang, ya.{\b\fn\fs}
Dialogue: 0,0:09:08.26,0:09:11.04,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Waktu pembuatan dan jarak terbangnya\Nmasih jelek.{\b\fn\fs}
Dialogue: 0,0:09:11.16,0:09:14.40,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kalau begini, masih tak bisa dipakai\Ndi pertarungan.{\b\fn\fs}
Dialogue: 0,0:09:15.54,0:09:17.98,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Memang tak ada cara lain\Nselain latihan.{\b\fn\fs}
Dialogue: 0,0:09:42.76,0:09:44.50,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Serasa mau pingsan.{\b\fn\fs}
Dialogue: 0,0:09:44.74,0:09:47.40,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Apa ini artinya aku kehabisan sihir?{\b\fn\fs}
Dialogue: 0,0:09:50.58,0:09:51.93,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Oh, iya.{\b\fn\fs}
Dialogue: 0,0:09:52.22,0:09:55.16,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku harus buat api sebelum\Nmatahari tenggelam.{\b\fn\fs}
Dialogue: 0,0:10:07.53,0:10:08.96,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Icicle Lance!{\b\fn\fs}
Dialogue: 0,0:10:11.36,0:10:14.21,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku sudah bisa buat\Ndengan cepat.{\b\fn\fs}
Dialogue: 0,0:10:14.73,0:10:17.24,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tapi seperti biasa, dia\Nsulit terbang.{\b\fn\fs}
Dialogue: 0,0:10:18.37,0:10:20.80,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kalau begitu, mari coba pergi!{\b\fn\fs}
Dialogue: 0,0:10:21.65,0:10:23.26,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ke luar pelindung!{\b\fn\fs}
Dialogue: 0,0:10:39.16,0:10:40.13,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Bingo!{\b\fn\fs}
Dialogue: 0,0:10:41.54,0:10:42.66,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kuarsa!{\b\fn\fs}
Dialogue: 0,0:10:42.74,0:10:45.50,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sudah kuduga bisa ditemukan\Ndi bantaran sungai!{\b\fn\fs}
Dialogue: 0,0:10:45.58,0:10:46.98,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Dengan ini...{\b\fn\fs}
Dialogue: 0,0:10:47.10,0:10:50.80,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Oke! Tanpa matahari pun\Naku bisa buat api.{\b\fn\fs}
Dialogue: 0,0:10:51.44,0:10:53.64,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sekarang tinggal temukan\Ndaun penawar.{\b\fn\fs}
Dialogue: 0,0:10:53.72,0:10:56.54,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Lalu petualangan hari ini\Nsukses besar.{\b\fn\fs}
Dialogue: 0,0:11:03.58,0:11:06.41,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Lesser Boar.{\b\fn\fs}
Dialogue: 0,0:11:06.98,0:11:08.02,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Gawat.{\b\fn\fs}
Dialogue: 0,0:11:13.89,0:11:14.96,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ayo, gerak!{\b\fn\fs}
Dialogue: 0,0:11:15.04,0:11:17.40,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Gerak! Gerak! Gerak! Bergeraklah!{\b\fn\fs}
Dialogue: 0,0:11:33.73,0:11:34.97,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku tak bisa bergerak.{\b\fn\fs}
Dialogue: 0,0:11:35.05,0:11:37.76,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tapi di dunia ini ada sihir!{\b\fn\fs}
Dialogue: 0,0:11:37.98,0:11:39.52,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ice Rink!{\b\fn\fs}
Dialogue: 0,0:11:41.89,0:11:44.36,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Icicle Lance 16!{\b\fn\fs}
Dialogue: 0,0:12:00.37,0:12:01.74,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku menang...{\b\fn\fs}
Dialogue: 0,0:12:03.09,0:12:04.84,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Lesser Boar...{\b\fn\fs}
Dialogue: 0,0:12:05.08,0:12:07.16,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Seperti dugaanku,\Ndia level terendah.{\b\fn\fs}
Dialogue: 0,0:12:07.58,0:12:09.17,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Itu terendah?{\b\fn\fs}
Dialogue: 0,0:12:10.17,0:12:12.72,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ensiklopedi monster ini\Nuntuk pemula,{\b\fn\fs}
Dialogue: 0,0:12:12.80,0:12:15.77,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}tapi dua halaman terakhir\Nada bagian spesial?{\b\fn\fs}
Dialogue: 0,0:12:15.97,0:12:17.08,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Naga.{\b\fn\fs}
Dialogue: 0,0:12:17.21,0:12:21.00,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Satu saja bisa dengan mudah\Nmenghancurkan satu kota.{\b\fn\fs}
Dialogue: 0,0:12:21.08,0:12:22.24,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Seramnya.{\b\fn\fs}
Dialogue: 0,0:12:22.45,0:12:25.01,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Catatan: Bila bertemu, kaburlah.{\b\fn\fs}
Dialogue: 0,0:12:25.57,0:12:28.38,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tapi kemungkinan besar\Ntak bisa kabur.{\b\fn\fs}
Dialogue: 0,0:12:28.46,0:12:31.40,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Jadi, kalau ketemu tamat sudah.{\b\fn\fs}
Dialogue: 0,0:12:32.41,0:12:36.36,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Iblis. Bukan malaikat jatuh.{\b\fn\fs}
Dialogue: 0,0:12:36.50,0:12:38.40,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Asal tidak diketahui.{\b\fn\fs}
Dialogue: 0,0:12:38.58,0:12:41.46,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Catatan: Berdoalah tak akan\Npernah bertemu.{\b\fn\fs}
Dialogue: 0,0:12:41.54,0:12:43.34,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Berdoa pun percuma, 'kan?{\b\fn\fs}
Dialogue: 0,0:12:45.74,0:12:47.90,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Dunia ini luas, ya.{\b\fn\fs}
Dialogue: 0,0:12:57.12,0:13:00.12,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Rasa manis dan asamnya pas sekali!{\b\fn\fs}
Dialogue: 0,0:13:05.30,0:13:06.38,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Burung?{\b\fn\fs}
Dialogue: 0,0:13:11.78,0:13:14.04,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kok tiba-tiba level tinggi?!{\b\fn\fs}
Dialogue: 0,0:13:17.28,0:13:18.78,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Assassin Hawk.{\b\fn\fs}
Dialogue: 0,0:13:18.88,0:13:21.72,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Senjata utamanya sihir angin\Ntak kasat mata Air Slash{\b\fn\fs}
Dialogue: 0,0:13:21.80,0:13:25.28,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}dan terjangan udara dengan\Nparuh dan cakar.{\b\fn\fs}
Dialogue: 0,0:13:25.69,0:13:27.30,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sepertinya aku tak bisa lari.{\b\fn\fs}
Dialogue: 0,0:13:28.37,0:13:29.85,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku harus bertarung!{\b\fn\fs}
Dialogue: 0,0:14:07.60,0:14:09.53,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Padahal cuma ingin hidup santai...{\b\fn\fs}
Dialogue: 0,0:14:09.61,0:14:11.56,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}kok susah sekali?{\b\fn\fs}
Dialogue: 0,0:14:12.53,0:14:15.10,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Karena sambil lari dari\NAssassin Hawk,{\b\fn\fs}
Dialogue: 0,0:14:15.32,0:14:18.10,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}aku jadi tak tahu arah pulang.{\b\fn\fs}
Dialogue: 0,0:14:19.44,0:14:22.22,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Harusnya aku sudah dekat.{\b\fn\fs}
Dialogue: 0,0:15:21.13,0:15:22.86,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Abrasive Jet!{\b\fn\fs}
Dialogue: 0,0:15:27.12,0:15:29.29,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Oke, sukses!{\b\fn\fs}
Dialogue: 0,0:15:30.53,0:15:35.50,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Es yang kukeraskan dengan sihir\Nbisa jadi pengganti pengikis garnet.{\b\fn\fs}
Dialogue: 0,0:15:36.46,0:15:39.70,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Mungkin aku mulai terbiasa\Nmengontrol sihir.{\b\fn\fs}
Dialogue: 0,0:15:55.05,0:15:58.84,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Dullahan tidak ada di ensiklopedi\Nmonster pemula.{\b\fn\fs}
Dialogue: 0,0:15:59.36,0:16:03.73,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Antara dia bukan monster atau\Ndia bukan level pemula.{\b\fn\fs}
Dialogue: 0,0:16:03.96,0:16:05.34,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Apa pun alasannya...{\b\fn\fs}
Dialogue: 0,0:16:22.94,0:16:24.25,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Masuk!{\b\fn\fs}
Dialogue: 0,0:16:26.18,0:16:27.25,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Apa?{\b\fn\fs}
Dialogue: 0,0:16:34.30,0:16:37.65,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Di levelku sekarang, tak ada\Nharapan menang.{\b\fn\fs}
Dialogue: 0,0:16:38.26,0:16:41.92,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tapi pasti ada alasan dia datang\Nke sini setiap malam.{\b\fn\fs}
Dialogue: 0,0:16:43.06,0:16:44.74,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Dia seperti ingin melatihku.{\b\fn\fs}
Dialogue: 0,0:16:45.18,0:16:47.25,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kurasa itu maksudnya.{\b\fn\fs}
Dialogue: 0,0:16:53.94,0:16:55.80,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Pedang air.{\b\fn\fs}
Dialogue: 0,0:17:06.01,0:17:08.25,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kamu ingin aku menguasai ini?{\b\fn\fs}
Dialogue: 0,0:17:17.80,0:17:20.14,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Fantasi yang luar biasa.{\b\fn\fs}
Dialogue: 0,0:17:32.38,0:17:34.37,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Assassin Hawk yang dulu?{\b\fn\fs}
Dialogue: 0,0:17:34.45,0:17:37.22,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tapi warnanya kok hitam legam?{\b\fn\fs}
Dialogue: 0,0:17:41.58,0:17:43.37,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Dia berevolusi.{\b\fn\fs}
Dialogue: 0,0:17:44.24,0:17:46.06,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ice Wall Lima Lapis!{\b\fn\fs}
Dialogue: 0,0:17:56.40,0:17:58.04,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ice Wall Sepuluh Lapis!{\b\fn\fs}
Dialogue: 0,0:18:03.50,0:18:04.54,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ice Wall!{\b\fn\fs}
Dialogue: 0,0:18:07.37,0:18:08.54,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Icicle Lance.{\b\fn\fs}
Dialogue: 0,0:18:13.32,0:18:14.56,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Dihapus?!{\b\fn\fs}
Dialogue: 0,0:18:15.54,0:18:17.92,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Jangan bilang dia bisa\Nmembatalkan sihir?!{\b\fn\fs}
Dialogue: 0,0:18:18.00,0:18:20.30,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Evolusi yang menyulitkan!{\b\fn\fs}
Dialogue: 0,0:18:21.82,0:18:24.25,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Dia akan melakukan sesuatu!{\b\fn\fs}
Dialogue: 0,0:19:21.40,0:19:22.65,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Benar juga.{\b\fn\fs}
Dialogue: 0,0:19:23.28,0:19:25.72,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kamu melawan orang yang melukai\Nsatu matanya.{\b\fn\fs}
Dialogue: 0,0:19:26.42,0:19:28.94,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Walau kalah setelah bertarung\Nsekuat tenaga,{\b\fn\fs}
Dialogue: 0,0:19:29.44,0:19:31.25,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}kamu pasti tidak terima.{\b\fn\fs}
Dialogue: 0,0:19:36.20,0:19:40.40,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Yang pasti, aku bisa tumbuh setelah\Nbertemu denganmu.{\b\fn\fs}
Dialogue: 0,0:19:41.20,0:19:45.53,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Dengan bertarung dua kali denganmu,\Naku merasa aku jadi lebih kuat.{\b\fn\fs}
Dialogue: 0,0:19:46.68,0:19:50.41,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Terima kasih sudah jadi pemicuku\Nmenjadi lebih kuat.{\b\fn\fs}
Dialogue: 0,0:19:55.08,0:19:58.76,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kamu sampai berevolusi\Ndemi membalasku.{\b\fn\fs}
Dialogue: 0,0:19:59.49,0:20:02.44,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Dengan sosok gagahmu di benakku,\Ndengan hormat...{\b\fn\fs}
Dialogue: 0,0:20:03.10,0:20:04.61,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}akan kuakhiri hidupmu.{\b\fn\fs}
Dialogue: 0,0:20:19.60,0:20:21.12,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tak mungkin...{\b\fn\fs}
Dialogue: 0,0:20:22.69,0:20:23.73,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Naga?{\b\fn\fs}
Dialogue: 0,0:20:27.16,0:20:29.49,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Hey, manusia di sana!{\b\fn\fs}
Dialogue: 0,0:20:29.73,0:20:30.89,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Dia bicara?{\b\fn\fs}
Dialogue: 0,0:20:31.16,0:20:33.41,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Syukurlah! Bagus!{\b\fn\fs}
Dialogue: 0,0:20:33.54,0:20:38.08,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku sudah lama tidak telepati,\Ntakutnya tak terdengar.{\b\fn\fs}
Dialogue: 0,0:20:38.22,0:20:39.25,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Telepati?{\b\fn\fs}
Dialogue: 0,0:20:39.78,0:20:43.20,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ya! Ada yang ingin kutanyakan.{\b\fn\fs}
Dialogue: 0,0:20:43.33,0:20:49.06,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Apa kamu lihat evolusi burung\NAssassin Hawk di sekitar sini?{\b\fn\fs}
Dialogue: 0,0:20:50.04,0:20:52.12,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sa-saya lihat.{\b\fn\fs}
Dialogue: 0,0:20:52.29,0:20:55.86,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Jangan-jangan dia semacam\Npengikut Anda?{\b\fn\fs}
Dialogue: 0,0:20:56.21,0:20:59.50,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Bila iya, saya harus meminta maaf.{\b\fn\fs}
Dialogue: 0,0:21:00.02,0:21:02.16,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Begitu, ya!{\b\fn\fs}
Dialogue: 0,0:21:02.24,0:21:06.89,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Kamu yang membunuhnya, ya!\NHebat juga!{\b\fn\fs}
Dialogue: 0,0:21:07.13,0:21:11.94,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Terus, kamu juga punya benda langka\Ndi pinggangmu, ya.{\b\fn\fs}
Dialogue: 0,0:21:16.13,0:21:18.57,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sudah kuduga, itu Pedang Raja Peri.{\b\fn\fs}
Dialogue: 0,0:21:18.65,0:21:24.12,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Raja Peri pasti lumayan menyukaimu\Nkalau sampai memberimu pedang itu.{\b\fn\fs}
Dialogue: 0,0:21:24.20,0:21:26.53,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Saya tak begitu tahu soal itu.{\b\fn\fs}
Dialogue: 0,0:21:26.93,0:21:30.38,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Maaf, saya ingin bertanya\Nbeberapa hal.{\b\fn\fs}
Dialogue: 0,0:21:35.60,0:21:37.85,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Maaf, maaf!{\b\fn\fs}
Dialogue: 0,0:21:37.93,0:21:40.12,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sepertinya aku dipanggil.{\b\fn\fs}
Dialogue: 0,0:21:40.20,0:21:42.66,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Padahal masih ingin mengobrol\Nsedikit lagi.{\b\fn\fs}
Dialogue: 0,0:21:42.80,0:21:46.73,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Tak apa. Terima kasih sudah banyak\Nmengajarkan saya.{\b\fn\fs}
Dialogue: 0,0:21:47.08,0:21:49.14,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Maaf, boleh tahu nama Anda?{\b\fn\fs}
Dialogue: 0,0:21:49.53,0:21:51.14,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Nama saya Ryo.{\b\fn\fs}
Dialogue: 0,0:21:51.48,0:21:54.12,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Ryo, ya. Aku Lewin!{\b\fn\fs}
Dialogue: 0,0:21:54.21,0:21:57.18,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Mari bertemu lagi, Ryo!{\b\fn\fs}
Dialogue: 0,0:22:03.61,0:22:05.54,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Energi yang luar biasa.{\b\fn\fs}
Dialogue: 0,0:22:06.25,0:22:09.49,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aku tak percaya datang hari\Naku bicara dengan naga.{\b\fn\fs}
Dialogue: 0,0:22:09.90,0:22:12.98,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Dunia lain itu penuh dengan\Nberagam hal, ya.{\b\fn\fs}
Dialogue: 0,0:22:13.96,0:22:17.08,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Aduh, cerobohnya aku!\NAda yang terlewat.{\b\fn\fs}
Dialogue: 0,0:22:18.89,0:22:22.04,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Atribut tersembunyi? Kenapa bisa?{\b\fn\fs}
Dialogue: 0,0:22:22.60,0:22:24.26,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Atribut tersembunyi...{\b\fn\fs}
Dialogue: 0,0:22:24.34,0:22:27.68,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Terakhir kulihat di gadis pada\Ntugas reinkarnasi pertamaku.{\b\fn\fs}
Dialogue: 0,0:22:28.76,0:22:30.58,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Sudah 10.000 tahun lalu.{\b\fn\fs}
Dialogue: 0,0:22:31.64,0:22:33.81,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Atribut tersembunyi Ryo adalah...{\b\fn\fs}
Dialogue: 0,0:22:34.46,0:22:35.73,Default,,0,0,0,,{\fnBasic Comical NC\fs70\b1}Awet muda, ya.{\b\fn\fs}
