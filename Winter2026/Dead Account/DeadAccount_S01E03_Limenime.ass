Dialogue: 0,0:02:39.95,0:02:44.37,tanda,,0000,0000,0000,,DEAD ACCOUNT
Dialogue: 0,0:02:49.04,0:02:51.00,tanda,,0000,0000,0000,,LIMA JAM USAI PERTARUNGAN\NMELAWAN KASUBATA
Dialogue: 0,0:03:46.89,0:03:48.68,tanda,,0000,0000,0000,,AKADEMI MIDEN, KELAS 1-B
Dialogue: 0,0:05:58.42,0:05:59.15,tanda,,0000,0000,0000,,KELUAR
Dialogue: 0,0:07:37.08,0:07:38.45,tanda,,0000,0000,0000,,"KANTIN, YOYO-TEI"
Dialogue: 0,0:11:05.33,0:11:07.62,tanda,,0000,0000,0000,,HARI BERIKUTNYA
