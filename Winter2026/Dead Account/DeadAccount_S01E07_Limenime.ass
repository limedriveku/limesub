Dialogue: 0,0:00:30.13,0:00:31.01,tanda,,0000,0000,0000,,AUTOPHOBIA K: CERITAKAN MASALAHMU
Dialogue: 0,0:02:40.32,0:02:42.03,tanda,,0000,0000,0000,,KELAS 1-A, DEI SURUGI
Dialogue: 0,0:02:43.33,0:02:45.41,tanda,,0000,0000,0000,,"KELAS 1-A, MINORU WAGARI"
Dialogue: 0,0:03:01.43,0:03:03.85,tanda,,0000,0000,0000,,"KELAS 1-A, BAN ASHINA"
Dialogue: 0,0:03:04.35,0:03:06.43,tanda,,0000,0000,0000,,"KELAS 1-A, KIRI NAMEKAWA"
Dialogue: 0,0:03:11.98,0:03:13.98,tanda,,0000,0000,0000,,"KELAS 1-A, IKKO OWADA"
Dialogue: 0,0:03:20.57,0:03:22.99,tanda,,0000,0000,0000,,"KELAS 1-A, ERU KUSABA"
Dialogue: 0,0:03:51.23,0:03:53.48,tanda,,0000,0000,0000,,"FLAMES OF LOATHING"
Dialogue: 0,0:06:33.33,0:06:34.16,tanda,,0000,0000,0000,,UNIT KESEHATAN
Dialogue: 0,0:08:05.77,0:08:06.69,tanda,,0000,0000,0000,,AUTOPHOBIA K
Dialogue: 0,0:08:06.77,0:08:09.53,tanda,,0000,0000,0000,,- KEINGINANMU AKAN SEGERA TERWUJUD\N- TERIMA KASIH, AKU MENANTIKANNYA
Dialogue: 0,0:10:09.27,0:10:12.44,tanda,,0000,0000,0000,,"PERTARUNGAN ANTARKELAS"
Dialogue: 0,0:10:17.28,0:10:19.20,tanda,,0000,0000,0000,,"UKS"
Dialogue: 0,0:10:19.28,0:10:21.20,tanda,,0000,0000,0000,,"SUDAH TERISI PENUH DAN MENUNGGUMU"
Dialogue: 0,0:14:44.42,0:14:47.22,tanda,,0000,0000,0000,,"MILLENNIUM SUSPENSION"
Dialogue: 0,0:16:17.01,0:16:18.98,tanda,,0000,0000,0000,,"PUSAT PINDAHAN PIGGY"
Dialogue: 0,0:17:03.56,0:17:06.19,tanda,,0000,0000,0000,,"MEMANGGIL MONSTER SIBER"
Dialogue: 0,0:18:04.00,0:18:05.62,tanda,,0000,0000,0000,,MELESET! TERTAWA!
Dialogue: 0,0:00:01.33,0:00:03.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku sangat kesepian.
Dialogue: 0,0:00:03.62,0:00:05.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tuan K-ku yang malang.
Dialogue: 0,0:00:06.04,0:00:08.30,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau tersiksa karena kesepian lagi.
Dialogue: 0,0:00:08.38,0:00:11.34,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Entah apa yang dia lakukan sekarang,\Nmengendalikan Api Hantu.
Dialogue: 0,0:00:11.42,0:00:13.26,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku yakin kita pasti bertemu lagi.
Dialogue: 0,0:00:13.34,0:00:16.64,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi ada pengganggu di sekelilingnya...
Dialogue: 0,0:00:16.72,0:00:18.18,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku sangat kesepian.
Dialogue: 0,0:00:18.26,0:00:21.06,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kenapa harus kesepian?
Dialogue: 0,0:00:22.77,0:00:27.48,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Orang berbakat berkumpul\Ndemi mewujudkan cita-cita mereka.
Dialogue: 0,0:00:27.56,0:00:30.11,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudah hampir waktunya\Nkeinginan itu terwujud.
Dialogue: 0,0:00:31.03,0:00:33.99,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mari wujudkan negeri akun gaib kita.
Dialogue: 0,0:02:09.53,0:02:12.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kasubata, jangan salah paham...
Dialogue: 0,0:02:14.34,0:02:15.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia memantul pakai otot perut.
Dialogue: 0,0:02:16.01,0:02:17.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mirip udang.
Dialogue: 0,0:02:18.76,0:02:20.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Urusan otot perut, aku juga sehebat dia.
Dialogue: 0,0:02:21.05,0:02:22.30,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pakai bajumu yang benar.
Dialogue: 0,0:02:33.77,0:02:35.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Minoru, maafkan aku.
Dialogue: 0,0:02:35.48,0:02:37.44,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku tak sanggup menahannya.
Dialogue: 0,0:02:42.12,0:02:43.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Astaga, tipe pembakar
Dialogue: 0,0:02:43.33,0:02:45.41,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}benar-benar di luar kendali.
Dialogue: 0,0:02:45.83,0:02:48.62,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kasubata, sepertinya kita berdua\Nsama-sama menderita.
Dialogue: 0,0:02:48.71,0:02:53.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Setuju. Para bedebah pembakar itu\Nmemang dasarnya sampah.
Dialogue: 0,0:02:53.21,0:02:56.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi setidaknya teman sekelas kami itu\Nsampah yang layak. Maaf saja.
Dialogue: 0,0:02:56.96,0:03:00.30,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Judulnya "Pertarungan Tipe Pembakar".
Dialogue: 0,0:03:01.43,0:03:03.85,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak boleh.
Dialogue: 0,0:03:04.35,0:03:06.43,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Warna kuning mengingatkanku pada pisang.
Dialogue: 0,0:03:06.52,0:03:08.64,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pisang mengingatkanku pada warna kuning.
Dialogue: 0,0:03:08.73,0:03:10.94,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Warna kuning mengingatkanku pada pisang.
Dialogue: 0,0:03:11.98,0:03:13.98,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pembakar berengsek dari Kelas 1-B.
Dialogue: 0,0:03:14.06,0:03:16.57,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dipeluk oleh tubuh sempurna Kusaba.
Dialogue: 0,0:03:16.65,0:03:17.73,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku iri banget...
Dialogue: 0,0:03:17.82,0:03:19.49,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak, aku takkan pernah memaafkannya.
Dialogue: 0,0:03:20.57,0:03:22.99,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Urusugawa, bagaimana kabarmu belakangan ini?
Dialogue: 0,0:03:23.28,0:03:26.62,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan sok akrab denganku, Gadis Berbahaya.
Dialogue: 0,0:03:26.70,0:03:29.45,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Masih merayu cowok\Nbuat cari nafkah, seperti biasa?
Dialogue: 0,0:03:29.54,0:03:32.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau sama sekali belum berubah, ya.
Dialogue: 0,0:03:32.12,0:03:35.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Selera fesyenmu\Nmasih tidak laku di kalangan cowok.
Dialogue: 0,0:03:35.13,0:03:36.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Norak banget.
Dialogue: 0,0:03:38.92,0:03:41.68,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Api Hantu, tipe Pembakar!
Dialogue: 0,0:03:41.76,0:03:45.01,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau kau kena itu, urusannya takkan selesai\Nhanya dengan patah tulang.
Dialogue: 0,0:03:45.10,0:03:48.02,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kemarahan itu cuma milikku...
Dialogue: 0,0:03:48.10,0:03:49.93,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aktifkan siber.
Dialogue: 0,0:03:51.23,0:03:53.48,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Flames of Loathing!
Dialogue: 0,0:03:53.73,0:03:56.44,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Orang ini tak pakai ponselnya...
Dialogue: 0,0:03:58.65,0:04:01.11,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Panas! Ini api sungguhan!
Dialogue: 0,0:04:01.49,0:04:04.66,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa yang kau takutkan?\NBakarlah kemarahanmu lebih terang, Enishiro!
Dialogue: 0,0:04:04.74,0:04:08.74,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Oh ya, biar kuberi tahu\Nsoal pertarungan antarkelas semester lalu.
Dialogue: 0,0:04:09.58,0:04:11.41,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kelas kami menang telak.
Dialogue: 0,0:04:11.50,0:04:13.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Alasannya sederhana.
Dialogue: 0,0:04:13.12,0:04:15.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Perbedaan kemampuan kita.
Dialogue: 0,0:04:15.67,0:04:18.55,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dasar bajingan tukang intip\Nberwajah sok ramah!
Dialogue: 0,0:04:18.63,0:04:20.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dasar pengecut!
Dialogue: 0,0:04:20.76,0:04:22.93,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dasar boneka kecil!
Dialogue: 0,0:04:23.01,0:04:25.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Siapa yang kau bicarakan?\NCoba katakan lagi!
Dialogue: 0,0:04:25.14,0:04:28.26,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lalu wali kelas yang kuno itu!
Dialogue: 0,0:04:29.18,0:04:32.27,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kelas sampah yang isinya cuma pecundang!
Dialogue: 0,0:04:33.19,0:04:35.15,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sebenarnya seberapa...
Dialogue: 0,0:04:36.52,0:04:39.48,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tidak pedulinya kau\Nsama perasaan orang lain?
Dialogue: 0,0:04:41.69,0:04:44.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan berani mencemari kenangan Akari!
Dialogue: 0,0:04:47.12,0:04:50.41,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan remehkan Kelas 1-B!
Dialogue: 0,0:04:52.50,0:04:56.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia merebut apiku dengan tangan kosong!
Dialogue: 0,0:04:57.54,0:04:59.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hebat banget!
Dialogue: 0,0:05:11.27,0:05:12.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Cukup!
Dialogue: 0,0:05:13.18,0:05:14.43,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Enishiro!
Dialogue: 0,0:05:14.52,0:05:15.94,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dan semua anak Kelas 1-B!
Dialogue: 0,0:05:16.02,0:05:18.94,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku minta maaf\Natas kelancangan Surugi tadi.
Dialogue: 0,0:05:19.02,0:05:21.82,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi ada yang ingin\Nkukatakan juga pada kalian.
Dialogue: 0,0:05:23.15,0:05:24.61,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalian itu...
Dialogue: 0,0:05:25.49,0:05:26.91,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tahi.
Dialogue: 0,0:05:28.12,0:05:29.12,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa?
Dialogue: 0,0:05:29.37,0:05:31.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kenapa sikapmu malah begitu\Nhabis minta maaf?
Dialogue: 0,0:05:31.62,0:05:33.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau bukan cuma kasar, tapi juga bodoh, ya?
Dialogue: 0,0:05:33.62,0:05:35.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan salah paham.
Dialogue: 0,0:05:35.79,0:05:37.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku bilang begini\Nbukan karena kalian lemah.
Dialogue: 0,0:05:37.54,0:05:40.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berbeda dari Dei, aku tak membeda-bedakan\Norang karena alasan remeh begitu.
Dialogue: 0,0:05:41.00,0:05:43.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Cowok ini sama sekali tak mau dengar, deh.
Dialogue: 0,0:05:45.72,0:05:48.26,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Karena kalian menerima pengguna Api Hantu,
Dialogue: 0,0:05:48.34,0:05:50.93,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kelas 1-B sudah ternoda.
Dialogue: 0,0:05:53.14,0:05:54.89,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pada akhir bulan,
Dialogue: 0,0:05:54.98,0:05:57.10,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kelas 1-A akan kembali menang\Ndi pertarungan antarkelas.
Dialogue: 0,0:05:57.56,0:06:00.36,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kami takkan pernah sudi kalah\Ndari kelas seperti kalian.
Dialogue: 0,0:06:02.57,0:06:04.94,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau salah, Wagari.
Dialogue: 0,0:06:05.64,0:06:08.77,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kami bakal kalah karena dia?
Dialogue: 0,0:06:14.24,0:06:17.16,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Justru karena dialah, Kelas 1-B\Nakan menang di pertarungan berikutnya.
Dialogue: 0,0:06:18.75,0:06:20.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Enishiro!
Dialogue: 0,0:06:20.96,0:06:23.30,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku akan mengawasimu.
Dialogue: 0,0:06:25.80,0:06:29.18,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mari simpan rasa frustrasi ini\Nbuat pertandingan sungguhan nanti.
Dialogue: 0,0:06:29.26,0:06:31.72,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ayo selesaikan ini\Ndi pertarungan antarkelas!
Dialogue: 0,0:06:34.18,0:06:35.39,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yang benar saja.
Dialogue: 0,0:06:35.47,0:06:37.48,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kan sudah kubilang jangan berlebihan.
Dialogue: 0,0:06:37.56,0:06:39.69,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalian anak muda gampang banget terluka.
Dialogue: 0,0:06:39.77,0:06:41.15,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maafkan aku.
Dialogue: 0,0:06:43.19,0:06:44.90,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terima kasih atas pengobatannya.
Dialogue: 0,0:06:44.98,0:06:45.98,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hei.
Dialogue: 0,0:06:47.07,0:06:48.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kerja bagus.
Dialogue: 0,0:06:50.24,0:06:51.57,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak Azaki.
Dialogue: 0,0:06:51.66,0:06:52.70,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Di mana yang lainnya?
Dialogue: 0,0:06:52.78,0:06:55.70,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mereka sedang bersiap\Nuntuk pertarungan dua pekan lagi.
Dialogue: 0,0:06:55.79,0:06:57.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baiklah, aku juga tak boleh ketinggalan.
Dialogue: 0,0:07:00.37,0:07:01.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak Azaki.
Dialogue: 0,0:07:01.54,0:07:02.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya?
Dialogue: 0,0:07:02.67,0:07:04.55,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ada yang ingin kutanyakan.
Dialogue: 0,0:07:38.04,0:07:41.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pertarungan antarkelas semester kedua\Nakhirnya segera dimulai.
Dialogue: 0,0:07:41.92,0:07:44.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Semester lalu,\NKelas 1-B benar-benar dihabisi.
Dialogue: 0,0:07:45.00,0:07:48.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saat itu, hanya Kasubata\Nyang berjuang sendirian.
Dialogue: 0,0:07:48.13,0:07:54.47,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kekalahan telak Kelas 1-B terjadi karena\Nsiberkinesis pertahanan yang tak efektif.
Dialogue: 0,0:07:55.14,0:07:57.72,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Setelah Soji Enishiro bergabung,
Dialogue: 0,0:07:57.81,0:08:01.39,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}apakah ini akan jadi berkat atau kutukan?
Dialogue: 0,0:08:16.20,0:08:18.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku semangat banget, nih!
Dialogue: 0,0:08:18.66,0:08:20.62,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mari kita bahas soal cinta!
Dialogue: 0,0:08:20.70,0:08:23.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hatiku cuma buat Kureha.
Dialogue: 0,0:08:24.46,0:08:25.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku mabuk darat.
Dialogue: 0,0:08:25.71,0:08:27.84,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mari minum teh. Teh boleh juga.
Dialogue: 0,0:08:27.92,0:08:32.26,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pertarungan antarkelas sudah dekat.\NKenapa kau bisa begitu santai?
Dialogue: 0,0:08:32.34,0:08:35.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukannya ada yang ketakutan di sini?
Dialogue: 0,0:08:35.67,0:08:39.89,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Wajar kalau takut\Nsetelah bencana di semester lalu.
Dialogue: 0,0:08:43.02,0:08:43.89,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Teh.
Dialogue: 0,0:08:44.39,0:08:47.77,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pertarungan antarkelas terakhir\Ndiadakan di bukit belakang, 'kan?
Dialogue: 0,0:08:48.94,0:08:50.32,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Perkelahian di bukit belakang itu,
Dialogue: 0,0:08:50.40,0:08:51.78,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}aku bahkan tak mau memikirkannya lagi.
Dialogue: 0,0:08:52.86,0:08:55.95,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mereka semua\Npengguna siberkinesis tipe serangan.
Dialogue: 0,0:08:56.03,0:08:58.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kita takkan bisa lolos dari mereka,\Nsekeras apa pun kita berusaha.
Dialogue: 0,0:08:58.78,0:09:01.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Singkatnya, mereka itu\Nsegerombolan orang kejam.
Dialogue: 0,0:09:01.41,0:09:02.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Emoto, kau ini...
Dialogue: 0,0:09:02.91,0:09:06.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau setakut itu, kenapa kau\Nmasih jadi medium roh?
Dialogue: 0,0:09:06.63,0:09:08.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tega banget kau menabur garam\Ndi lukaku dengan wajah polos begitu!
Dialogue: 0,0:09:10.50,0:09:11.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Memangnya kenapa?
Dialogue: 0,0:09:12.21,0:09:13.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu karena...
Dialogue: 0,0:09:13.13,0:09:16.01,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudahlah, setiap orang pasti punya alasan.
Dialogue: 0,0:09:17.14,0:09:18.89,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Master Haijima.
Dialogue: 0,0:09:18.97,0:09:22.02,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sepertinya sosok aslimu\Nmasih belum bisa hadir, ya.
Dialogue: 0,0:09:22.10,0:09:23.77,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa maksudmu?
Dialogue: 0,0:09:24.64,0:09:26.02,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tentu saja sosok asliku ada di sini.
Dialogue: 0,0:09:28.02,0:09:30.02,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Begini...
Dialogue: 0,0:09:30.11,0:09:35.07,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Makin dekat dengan boneka,\Nkekuatan spiritualmu akan makin terkuras.
Dialogue: 0,0:09:35.74,0:09:39.53,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudah lama aku tak masuk akademi...
Dialogue: 0,0:09:39.62,0:09:43.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi kali ini,\Naku ingin menang apa pun yang terjadi.
Dialogue: 0,0:09:43.62,0:09:47.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bu Itabashi, kami takkan bersantai.
Dialogue: 0,0:09:48.42,0:09:51.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kami sudah bersiap untuk pertarungan ini\Nselama dua pekan terakhir.
Dialogue: 0,0:09:53.63,0:09:56.76,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku tak sabar mencoba "itu"\Ndi pertarungan sungguhan.
Dialogue: 0,0:09:56.84,0:09:58.76,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bersiaplah. Kita hampir sampai.
Dialogue: 0,0:10:02.64,0:10:04.35,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukannya ini Kusatsu?
Dialogue: 0,0:10:04.43,0:10:07.94,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bu Itabashi, kita kembali ke akademi.
Dialogue: 0,0:10:09.27,0:10:12.44,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kali ini, pertarungan jalanan Kusatsu.
Dialogue: 0,0:10:12.52,0:10:16.70,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau menang jika berhasil melumpuhkan\Natau memaksa lawanmu menyerah.
Dialogue: 0,0:10:17.28,0:10:19.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dokter Kochi, dokter sekolah, ikut hadir.
Dialogue: 0,0:10:19.28,0:10:21.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi tak masalah jika kalian terluka.
Dialogue: 0,0:10:21.28,0:10:25.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi kalian akan didiskualifikasi\Njika membunuh.
Dialogue: 0,0:10:26.00,0:10:28.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Inilah Kusatsu Virtual,
Dialogue: 0,0:10:28.42,0:10:31.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}reka ulang penuh dari lanskap Kota Kusatsu\Ndi area publik.
Dialogue: 0,0:10:31.79,0:10:33.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini sih, berlebihan banget.
Dialogue: 0,0:10:34.59,0:10:38.43,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Reka ulang secara penuh. Apa itu mungkin?
Dialogue: 0,0:10:38.51,0:10:42.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ada pengguna siberkinesis salin-tempel\Ndi antara para penasihat akademi.
Dialogue: 0,0:10:42.14,0:10:44.85,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kekuatan spiritualnya memang luar biasa.
Dialogue: 0,0:10:45.43,0:10:48.52,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku sudah terhubung dengan Pak Azaki.
Dialogue: 0,0:10:49.19,0:10:51.02,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa? Apa ini sudah merekam?
Dialogue: 0,0:10:51.48,0:10:52.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Halo?
Dialogue: 0,0:10:52.90,0:10:55.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak Azaki, layarnya gelap.
Dialogue: 0,0:10:56.24,0:10:57.74,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini lubang hidung, 'kan?
Dialogue: 0,0:10:58.40,0:11:01.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak Azaki, terlalu dekat.\NAgak menjauh dari kamera.
Dialogue: 0,0:11:01.99,0:11:03.62,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa begini sudah bagus?
Dialogue: 0,0:11:03.70,0:11:05.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sekarang malah kejauhan!
Dialogue: 0,0:11:05.91,0:11:07.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kenapa tidak pakai celana, sih?
Dialogue: 0,0:11:07.37,0:11:09.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kesalahan klasik\Nsaat panggilan video jarak jauh.
Dialogue: 0,0:11:09.29,0:11:11.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sayang banget kejadian ini tidak terekam.
Dialogue: 0,0:11:11.46,0:11:13.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak Azaki, tolong serius sedikit.
Dialogue: 0,0:11:14.00,0:11:18.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku serius, tahu!\NTablet pinjaman ini susah banget dipakai.
Dialogue: 0,0:11:18.42,0:11:20.51,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baiklah, semoga kalian berhasil.
Dialogue: 0,0:11:20.80,0:11:25.47,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Para guru dan senior kalian\Nmemantau pertandingan ini dengan saksama.
Dialogue: 0,0:11:25.56,0:11:29.60,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Meski ini acara rutin akademi,\Nkali ini rasanya berbeda.
Dialogue: 0,0:11:29.69,0:11:31.35,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalian sedang dievaluasi.
Dialogue: 0,0:11:31.44,0:11:34.06,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Untuk melihat apa kalian memenuhi syarat\Nikut Operasi Pembasmian K.
Dialogue: 0,0:11:35.32,0:11:37.15,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pak Guru, nantikan saja aksi kami.
Dialogue: 0,0:11:37.23,0:11:39.61,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku takkan mempermalukanmu kali ini.
Dialogue: 0,0:11:41.03,0:11:42.74,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sejujurnya,
Dialogue: 0,0:11:42.82,0:11:46.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}awalnya, kupikir tak masalah\Nkalau kalian kalah kali ini.
Dialogue: 0,0:11:49.33,0:11:52.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Karena aku menyayangi kalian.
Dialogue: 0,0:11:52.79,0:11:55.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau terlalu cepat dikirim\Nke misi pembasmian roh ekstrem,
Dialogue: 0,0:11:55.75,0:11:57.80,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}kemungkinan besar kalian bisa mati.
Dialogue: 0,0:11:57.88,0:12:03.51,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalian sudah bekerja keras dua pekan ini\Ndan kemampuan kalian telah meningkat.
Dialogue: 0,0:12:04.09,0:12:06.56,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sebagai wali kelas kalian,\Npesanku cuma satu.
Dialogue: 0,0:12:06.64,0:12:09.02,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Menangkan pertarungan ini. Itu saja.
Dialogue: 0,0:12:09.10,0:12:09.93,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baik, Pak Guru!
Dialogue: 0,0:12:10.35,0:12:12.90,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Menang, ya. Itu sudah pasti.
Dialogue: 0,0:12:13.48,0:12:17.32,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mari kita tunjukkan\Nsiapa medium roh yang sesungguhnya.
Dialogue: 0,0:12:17.40,0:12:19.07,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dengar, kita kalahkan mereka\Nsatu lawan satu...
Dialogue: 0,0:12:19.15,0:12:20.94,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Satu lawan satu.
Dialogue: 0,0:12:21.53,0:12:23.91,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Soal kekuatan tempur individu,\Nkita punya keuntungan besar.
Dialogue: 0,0:12:23.99,0:12:26.99,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Temukan mereka secepatnya\Ndan hancurkan semuanya sekaligus.
Dialogue: 0,0:12:27.08,0:12:29.12,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukan begitu, Pak Nanzo?
Dialogue: 0,0:12:29.20,0:12:30.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Benar sekali.
Dialogue: 0,0:12:30.37,0:12:33.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Enishiro itu milikku.
Dialogue: 0,0:12:33.50,0:12:35.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku juga tertarik pada Enishiro.
Dialogue: 0,0:12:36.63,0:12:39.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Biru langit adalah warna favoritku.
Dialogue: 0,0:12:39.30,0:12:42.09,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa? Jangan terlalu gegabah.
Dialogue: 0,0:12:42.72,0:12:47.68,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku dan Enishiro lagi mesra-mesranya.
Dialogue: 0,0:12:50.89,0:12:53.94,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sekarang mari kita\Nselesaikan semua ini dengan cepat.
Dialogue: 0,0:12:54.02,0:12:58.02,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Wagari, siapa yang harus kulawan?
Dialogue: 0,0:12:58.32,0:13:00.57,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Orang pertama yang kau temui,\Ndialah lawan yang ditakdirkan untukmu.
Dialogue: 0,0:13:03.28,0:13:04.28,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ayo.
Dialogue: 0,0:13:04.99,0:13:07.12,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saatnya berburu kelinci.
Dialogue: 0,0:13:08.03,0:13:09.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saat ini sudah masuk tengah hari.
Dialogue: 0,0:13:09.91,0:13:13.12,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kelas 1-A dan 1-B,\Nsilakan ambil posisi kalian.
Dialogue: 0,0:13:17.04,0:13:19.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saat ini, kita tidak punya pilihan\Nselain berusaha semaksimal mungkin.
Dialogue: 0,0:13:19.75,0:13:21.76,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ayo maju!\NJangan remehkan kami!
Dialogue: 0,0:13:22.01,0:13:27.39,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau kita gagal memenangkan ini,\Nlupakan saja soal mengalahkan K.
Dialogue: 0,0:13:27.47,0:13:28.76,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hei, Enishiro.
Dialogue: 0,0:13:29.68,0:13:32.72,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terima kasih sudah membela kami\Ndi gunung waktu itu.
Dialogue: 0,0:13:34.14,0:13:35.52,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku senang banget.
Dialogue: 0,0:13:35.60,0:13:36.85,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hasumi.
Dialogue: 0,0:13:37.31,0:13:39.90,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau sudah dipermalukan oleh Surugi.
Dialogue: 0,0:13:40.36,0:13:43.44,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau tahu kenapa kami tidak membalasnya?
Dialogue: 0,0:13:44.32,0:13:46.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Enishiro, itu karena...
Dialogue: 0,0:13:48.24,0:13:50.74,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}kami menyimpan dendam itu untuk diselesaikan\Nsaat pertarungan sesungguhnya dimulai.
Dialogue: 0,0:13:53.20,0:13:56.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pertarungan Kelas 1-A melawan Kelas 1-B
Dialogue: 0,0:14:02.67,0:14:03.80,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dimulai.
Dialogue: 0,0:14:06.59,0:14:08.18,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Suara yang lemah.
Dialogue: 0,0:14:08.26,0:14:10.76,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Beri kami pembukaan yang lebih megah!
Dialogue: 0,0:14:14.39,0:14:15.93,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ayo, Kelas 1-B!
Dialogue: 0,0:14:16.56,0:14:17.60,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya!
Dialogue: 0,0:14:20.23,0:14:22.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Astaga, padahal kita baru saja mulai.
Dialogue: 0,0:14:22.48,0:14:25.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Raungan barbar itu jelas suara si Kasubata.
Dialogue: 0,0:14:25.32,0:14:26.53,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tunggu sebentar, Wagari.
Dialogue: 0,0:14:27.99,0:14:29.28,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Biarkan aku bicara dulu.
Dialogue: 0,0:14:30.62,0:14:32.87,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku mulai terinspirasi, nih.
Dialogue: 0,0:14:33.45,0:14:34.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aktifkan siber!
Dialogue: 0,0:14:36.58,0:14:38.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Millennium Suspension!
Dialogue: 0,0:14:44.42,0:14:47.22,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Judul sementara, "Keindahan yang Membeku".
Dialogue: 0,0:14:50.30,0:14:51.55,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bagaimana bisa begini?
Dialogue: 0,0:14:52.39,0:14:54.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku tak sengaja membekukannya lagi.
Dialogue: 0,0:14:54.89,0:14:57.02,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ashina luar biasa!
Dialogue: 0,0:14:57.10,0:14:59.19,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukannya itu terlalu besar?
Dialogue: 0,0:14:59.27,0:15:01.90,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ashina, seberapa besar kekuatan\Nyang barusan kau gunakan?
Dialogue: 0,0:15:03.82,0:15:05.57,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sekitar setengah!
Dialogue: 0,0:15:05.65,0:15:06.69,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa kau bodoh, ya?
Dialogue: 0,0:15:06.78,0:15:08.03,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terus pakai bajumu yang benar, dong!
Dialogue: 0,0:15:08.70,0:15:10.78,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hei, apa itu?
Dialogue: 0,0:15:10.86,0:15:12.87,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ashina itu pembasmi roh tipe es.
Dialogue: 0,0:15:12.95,0:15:14.70,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Seleranya masih seburuk biasanya.
Dialogue: 0,0:15:14.79,0:15:17.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi aku tak benci penampilannya, sih.
Dialogue: 0,0:15:17.12,0:15:19.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kukira kita bisa\Nmenghabisi mereka sekaligus,
Dialogue: 0,0:15:19.41,0:15:21.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tapi ternyata tak semudah itu.
Dialogue: 0,0:15:23.13,0:15:23.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Emo!
Dialogue: 0,0:15:25.63,0:15:28.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aktifkan Siber! Nirvana Wall!
Dialogue: 0,0:15:28.84,0:15:30.34,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hampir saja!
Dialogue: 0,0:15:30.43,0:15:32.85,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Untung kita sudah beri sinyal.
Dialogue: 0,0:15:32.93,0:15:35.22,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Luar biasa, Emoto! Bagus!
Dialogue: 0,0:15:35.31,0:15:37.52,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lumayan juga.
Dialogue: 0,0:15:40.19,0:15:43.11,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa Wagari sedang membalas kita?
Dialogue: 0,0:15:43.19,0:15:45.23,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia menyerang dari sana?
Dialogue: 0,0:15:45.32,0:15:46.86,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia benar-benar hebat.
Dialogue: 0,0:15:47.28,0:15:50.70,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kapten Kelas 1-A, Minoru Wagari.
Dialogue: 0,0:15:50.78,0:15:53.37,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Siberkinesisnya, Worry-Cutter,
Dialogue: 0,0:15:53.45,0:15:57.62,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}bisa dengan bebas mengubah target\Ndan kekuatan tebasannya.
Dialogue: 0,0:15:57.70,0:16:02.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sebagai anak kelas 1, dia berpartisipasi\Ndi banyak misi akun gaib.
Dialogue: 0,0:16:02.83,0:16:05.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia siswa genius yang sanggup membasmi\Nakun gaib tingkat menengah sendirian.
Dialogue: 0,0:16:05.96,0:16:07.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Wagari itu kuat.
Dialogue: 0,0:16:07.46,0:16:08.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dengar, ya.
Dialogue: 0,0:16:08.46,0:16:11.68,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau kau menghadapinya sendirian,\Npastikan untuk segera mundur.
Dialogue: 0,0:16:11.76,0:16:13.18,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau tak perlu memberitahuku soal itu.
Dialogue: 0,0:16:13.72,0:16:16.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tak kusangka Kasubata\Nbakal memberi peringatan seserius itu.
Dialogue: 0,0:16:17.01,0:16:18.98,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Haijima yang asli harus bergerak terpisah.
Dialogue: 0,0:16:19.00,0:16:20.60,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan sampai tertangkap.
Dialogue: 0,0:16:20.68,0:16:22.27,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Siap!
Dialogue: 0,0:16:22.35,0:16:24.19,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Master, kau pasti bisa!
Dialogue: 0,0:16:25.40,0:16:27.36,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Di mana aku harus bersembunyi?
Dialogue: 0,0:16:27.44,0:16:29.69,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Di tempat mereka takkan bisa menemukanku.
Dialogue: 0,0:16:30.53,0:16:32.49,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Satu lawan satu!
Dialogue: 0,0:16:32.57,0:16:35.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Begitu memastikan lokasi mereka,\Nkita cocokkan dengan yang lain.
Dialogue: 0,0:16:35.70,0:16:38.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Enishiro, kau ada di mana?
Dialogue: 0,0:16:38.33,0:16:41.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku pasti akan merawatmu dengan baik lagi.
Dialogue: 0,0:16:42.54,0:16:44.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku masih sangat ingin\Nterus melihat patung es ini.
Dialogue: 0,0:16:44.92,0:16:46.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini keindahan yang paling sempurna.
Dialogue: 0,0:16:49.13,0:16:51.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Di mana Kasubata?
Dialogue: 0,0:16:53.63,0:16:58.43,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hei, Owada, aku mau buang air.
Dialogue: 0,0:16:58.51,0:17:00.72,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pergilah ke toilet terdekat.
Dialogue: 0,0:17:01.43,0:17:02.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aneh banget.
Dialogue: 0,0:17:03.56,0:17:06.19,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku tak melihat siapa pun dari Kelas 1-B.
Dialogue: 0,0:17:09.15,0:17:11.65,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Siswa Kelas 1-B, di mana kalian?
Dialogue: 0,0:17:11.74,0:17:14.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku akan meraih kemenangan besar\Ndi pertarungan antarkelas kali ini.
Dialogue: 0,0:17:14.28,0:17:18.86,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lalu aku akan ikut serta\Ndalam Operasi Pembasmian Autophobia K.
Dialogue: 0,0:17:19.11,0:17:22.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini batu loncatan yang sempurna bagiku\Nuntuk menjadi medium roh hebat.
Dialogue: 0,0:17:25.17,0:17:26.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Slice!
Dialogue: 0,0:17:29.88,0:17:31.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hiyori Haijima!
Dialogue: 0,0:17:31.67,0:17:33.84,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dalang penyendiri.
Dialogue: 0,0:17:33.92,0:17:37.85,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sungguh terpuji kau berani\Nterjun ke medan perang.
Dialogue: 0,0:17:38.93,0:17:42.06,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Memilih deretan toko suvenir\Nsebagai persembunyian itu lumayan pintar.
Dialogue: 0,0:17:42.14,0:17:46.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau mau sembunyikan kotak,\Nsembunyikanlah di tumpukan kardus.
Dialogue: 0,0:17:46.48,0:17:48.31,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi sayang sekali.
Dialogue: 0,0:17:48.40,0:17:50.02,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau gemetar, tuh.
Dialogue: 0,0:17:51.90,0:17:54.28,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Wagari itu kuat. Ingatlah untuk mundur.
Dialogue: 0,0:17:56.82,0:17:57.82,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bodoh banget.
Dialogue: 0,0:17:58.32,0:18:00.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Seharusnya kau menyerah saja.
Dialogue: 0,0:18:00.70,0:18:01.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Slice!
Dialogue: 0,0:18:08.50,0:18:12.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sebagian besar penyerang Kelas 1-A itu kuat.
Dialogue: 0,0:18:12.46,0:18:16.55,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi, mereka pasti akan berpencar\Ndan mencari musuh lebih dahulu.
Dialogue: 0,0:18:17.09,0:18:19.30,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Haijima, kau yang bertugas jadi umpan.
Dialogue: 0,0:18:19.39,0:18:22.93,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pancing dia pakai jebakan\N"anak buah yang sendirian dan pengecut".
Dialogue: 0,0:18:24.18,0:18:25.68,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau tertipu, Bodoh!
Dialogue: 0,0:18:25.77,0:18:30.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Diriku yang sesungguhnya\Nsedang sembunyi di tempat lain.
Dialogue: 0,0:18:30.61,0:18:34.19,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Gang yang sempit,\Nditambah celah setelah aku bergerak.
Dialogue: 0,0:18:34.28,0:18:35.11,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini jebakan, ya.
Dialogue: 0,0:18:35.78,0:18:37.57,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau pernah dengar pepatah ini?
Dialogue: 0,0:18:40.57,0:18:42.41,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Inti dari pembasmian roh\Nadalah kerja sama tim.
Dialogue: 0,0:18:42.87,0:18:47.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pertama, kita kalahkan\Norang terkuatnya bersama-sama.
Dialogue: 0,0:18:48.33,0:18:49.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kerja sama tim?
Dialogue: 0,0:18:50.21,0:18:52.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Malah lebih mirip tawuran geng.
Dialogue: 0,0:18:52.63,0:18:55.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Strategi memalukan khas orang lemah.
Dialogue: 0,0:18:55.55,0:18:59.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lupakan soal satu lawan satu.\NBiar aku urus kalian semua sendirian.
Dialogue: 0,0:18:59.47,0:19:00.39,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Emo!
Dialogue: 0,0:19:01.05,0:19:02.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Slice!
Dialogue: 0,0:19:03.47,0:19:05.52,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ren, di mana anak Kelas 1-A yang lain?
Dialogue: 0,0:19:06.06,0:19:08.31,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tenang saja, kami belum melihat mereka.
Dialogue: 0,0:19:08.39,0:19:10.94,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Habisi dia sebelum bala bantuan datang!
Dialogue: 0,0:19:24.08,0:19:25.37,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tepat sasaran!
Dialogue: 0,0:19:25.79,0:19:27.16,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pengguna kemampuan Api Hantu...
Dialogue: 0,0:19:29.16,0:19:31.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya, bagus.
Dialogue: 0,0:19:31.38,0:19:33.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu kerja sama tim yang sangat terlatih.
Dialogue: 0,0:19:33.38,0:19:37.01,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Siberkinesis Kelas 1-B,\Ndinding, lendir, dan palu,
Dialogue: 0,0:19:37.09,0:19:39.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}masing-masing memainkan perannya\Ndengan koordinasi yang pas.
Dialogue: 0,0:19:39.68,0:19:42.27,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dengan tambahan Enishiro sang tipe pembakar,
Dialogue: 0,0:19:42.29,0:19:44.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}mobilitas mereka jadi makin meningkat.
Dialogue: 0,0:19:46.60,0:19:48.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kelas 1-B lumayan juga.
Dialogue: 0,0:19:50.10,0:19:54.36,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Denmi-chan manis banget kalau lagi gerak.
Dialogue: 0,0:19:54.44,0:19:56.90,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kepala Sekolah, masih ada satu orang lagi.
Dialogue: 0,0:19:57.69,0:20:00.45,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia sosok yang paling berbahaya\Ndi Kelas 1-B.
Dialogue: 0,0:20:04.32,0:20:05.37,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa dia berhasil?
Dialogue: 0,0:20:05.45,0:20:07.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Wagari sudah dikalahkan.
Dialogue: 0,0:20:07.66,0:20:09.37,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bagus! Satu tumbang!
Dialogue: 0,0:20:11.25,0:20:13.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bagaimana ini bisa terjadi?
Dialogue: 0,0:20:13.17,0:20:16.09,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sampai kena serangan dari Kelas 1-B.
Dialogue: 0,0:20:16.17,0:20:18.84,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini takkan lagi jadi\Nkemenangan yang sempurna.
Dialogue: 0,0:20:19.55,0:20:22.43,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Padahal aku harus jadi sosok yang sempurna.
Dialogue: 0,0:20:25.72,0:20:28.02,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia masih berniat melawan!\NTekan terus serangannya!
Dialogue: 0,0:20:28.39,0:20:31.69,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Karena terjebak di lendir, dia tidak bisa\Nmengayunkan pedangnya dengan benar.
Dialogue: 0,0:20:34.94,0:20:36.06,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bunuh diri?
Dialogue: 0,0:20:36.15,0:20:37.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukan.
Dialogue: 0,0:20:38.40,0:20:39.23,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Gawat!
Dialogue: 0,0:20:39.99,0:20:42.86,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kekuatan listrik Wagari memungkinkannya\Nmemilih apa saja yang mau dipotong.
Dialogue: 0,0:20:42.95,0:20:44.87,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia cuma menebas lendirnya.
Dialogue: 0,0:20:44.95,0:20:46.45,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mundur!
Dialogue: 0,0:20:47.33,0:20:51.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Worry-Cutter bisa memilih target\Ndan kekuatan tebasannya.
Dialogue: 0,0:20:51.75,0:20:54.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Biasanya dia pakai\Ntebasan tipe robek, Slice.
Dialogue: 0,0:20:54.67,0:20:56.34,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Untuk tebasan tipe potong,
Dialogue: 0,0:20:56.71,0:20:58.84,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}aku dilarang menggunakannya pada manusia.
Dialogue: 0,0:20:59.42,0:21:01.09,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sebab kalau tak waspada,\Nakibatnya bisa fatal.
Dialogue: 0,0:21:06.68,0:21:07.85,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sever!
Dialogue: 0,0:21:15.44,0:21:16.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia itu monster.
Dialogue: 0,0:21:20.65,0:21:23.03,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak, jangan lakukan itu!
Dialogue: 0,0:21:23.11,0:21:26.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku malah biarkan kemarahan menguasaiku.
Dialogue: 0,0:21:26.28,0:21:29.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku hampir saja membunuh mereka.
Dialogue: 0,0:21:30.58,0:21:33.62,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tarik napas panjang,\Nlalu susun ulang strateginya.
Dialogue: 0,0:21:36.29,0:21:40.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baiklah, mulai sekarang, aku akan memilih\Nuntuk memotong-motong anggota tubuhmu.
Dialogue: 0,0:21:40.59,0:21:43.55,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan syok dan mati, ya.\NBertahanlah sekuat tenaga.
Dialogue: 0,0:21:44.13,0:21:46.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia sudah benar-benar gila.
Dialogue: 0,0:21:46.68,0:21:48.22,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Rute pelarian kita sudah diblokir.
Dialogue: 0,0:21:48.30,0:21:50.64,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Memancingnya ke gang malah jadi bumerang.
Dialogue: 0,0:21:50.72,0:21:53.48,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bodoh! Kita bisa pakai Nirvana Wall\Nuntuk membela diri!
Dialogue: 0,0:21:53.56,0:21:56.10,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan pernah remehkan Emoto...
Dialogue: 0,0:22:02.32,0:22:05.03,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dindingnya hilang?
