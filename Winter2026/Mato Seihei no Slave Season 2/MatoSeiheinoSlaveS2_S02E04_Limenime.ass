Dialogue: 0,0:00:00.98,0:00:03.07,tanda,,0000,0000,0000,,MARKAS PUSAT KORPS DEFENSI IBLIS\N[MARKAS SKUAD SEPULUH]
Dialogue: 0,0:01:40.79,0:01:45.04,tanda,,0000,0000,0000,,BUDAK KORPS ELITE KOTA IBLIS - MUSIM KEDUA
Dialogue: 0,0:02:58.53,0:03:01.04,tanda,,0000,0000,0000,,EPISODE 04: LEPAS KENDALI
Dialogue: 0,0:04:42.47,0:04:44.43,tanda,,0000,0000,0000,,RANTAI ABADI (SLAVE): TARING PEMBUNUH
Dialogue: 0,0:05:49.87,0:05:51.75,tanda,,0000,0000,0000,,KAMBING TUMBAL
Dialogue: 0,0:09:17.08,0:09:18.50,tanda,,0000,0000,0000,,BUDAK KORPS ELITE KOTA IBLIS - MUSIM KEDUA
Dialogue: 0,0:09:52.36,0:09:56.49,tanda,,0000,0000,0000,,PERDANA MENTERI - PRESIDEN SUATU NEGARA
Dialogue: 0,0:11:31.04,0:11:31.63,tanda,,0000,0000,0000,,SANDAR
Dialogue: 0,0:11:33.30,0:11:34.80,tanda,,0000,0000,0000,,GOSOK-GOSOK
Dialogue: 0,0:18:02.60,0:18:04.31,tanda,,0000,0000,0000,,PIJATAN AZUMA YANG TERHORMAT
Dialogue: 0,0:23:14.04,0:23:16.54,tanda,,0000,0000,0000,,YANG TERJADI SETELAH\NYAMASHIRO REN MENJADI SLAVE...
Dialogue: 0,0:23:26.34,0:23:27.09,tanda,,0000,0000,0000,,EPISODE 05: PERJAMUAN AZUMA
Dialogue: 0,0:23:40.06,0:23:41.07,tanda,,0000,0000,0000,,BUDAK KORPS ELITE KOTA IBLIS - MUSIM KEDUA\NIKUTI AKU, ANJING KECIL
Dialogue: 0,0:00:03.07,0:00:06.61,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Selamat datang, Skuad Tujuh sekalian!
Dialogue: 0,0:00:06.61,0:00:09.57,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Nona Kyo dan Tuan Yuki,\Nsilakan lewat sini.
Dialogue: 0,0:00:09.96,0:00:11.87,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Cuma kami yang dipisah?
Dialogue: 0,0:00:11.87,0:00:15.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Benar!\NIni perintah Nona Ren nan agung.
Dialogue: 0,0:00:18.19,0:00:22.16,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ginna tidak takut sama Komandan?
Dialogue: 0,0:00:22.16,0:00:25.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kengeriannya itu juga daya tarik Nona Ren!
Dialogue: 0,0:00:25.81,0:00:30.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Menjadi bawahan langsung Nona Ren\Nadalah kehormatan bagiku!
Dialogue: 0,0:00:31.19,0:00:33.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ternyata dia bukan cuma ditakuti.
Dialogue: 0,0:00:36.61,0:00:38.93,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Silakan lanjut masuk lewat sini.
Dialogue: 0,0:00:52.75,0:00:55.57,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Di dalam Kota Iblis,\Nada taman tradisional Jepang?
Dialogue: 0,0:00:56.77,0:01:00.86,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tempat ini dibangun setelah komandan\Nyang sekarang menjabat.
Dialogue: 0,0:01:00.86,0:01:03.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yamashiro punya kuasa seperti itu?
Dialogue: 0,0:01:04.94,0:01:07.64,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia punya koneksi yang erat\Ndengan para petinggi.
Dialogue: 0,0:01:07.64,0:01:12.80,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bahkan Perdana Menteri terkadang\Nmenunjuk dia jadi pengawalnya.
Dialogue: 0,0:01:13.16,0:01:15.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi itu pekerjaannya di dunia nyata?
Dialogue: 0,0:03:09.96,0:03:11.44,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kelihatannya seru.
Dialogue: 0,0:03:12.14,0:03:13.55,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Komandan.
Dialogue: 0,0:03:18.01,0:03:19.76,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Imut sekali!
Dialogue: 0,0:03:21.41,0:03:24.23,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ada anjing di ruang bergaya Jepang.
Dialogue: 0,0:03:24.23,0:03:25.77,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Menawan, bukan?
Dialogue: 0,0:03:26.37,0:03:29.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tampaknya Kaname dan Miyabi juga menyukaimu.
Dialogue: 0,0:03:30.33,0:03:33.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mereka ini senior yang akan menjagamu.
Dialogue: 0,0:03:33.79,0:03:35.49,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sebagai juniornya.
Dialogue: 0,0:03:36.25,0:03:37.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Junior?
Dialogue: 0,0:03:39.24,0:03:42.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa alasannya hanya memanggil\Nkami berdua ke sini?
Dialogue: 0,0:03:45.31,0:03:46.93,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sana, pergi ke kamar kalian.
Dialogue: 0,0:03:51.59,0:03:55.51,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Untuk meminjam budak,\Nselagi kamu belum kelelahan.
Dialogue: 0,0:03:55.88,0:03:56.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu...
Dialogue: 0,0:03:56.97,0:03:59.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bisa jadi muncul sifat\Nyang belum kita ketahui.
Dialogue: 0,0:03:59.71,0:04:01.73,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Harus dicoba dulu sekali.
Dialogue: 0,0:04:02.93,0:04:03.85,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Benar, bukan?
Dialogue: 0,0:04:11.06,0:04:14.35,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau memang perintah, tidak ada pilihan.
Dialogue: 0,0:04:24.15,0:04:26.87,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Oh, ternyata enak juga dinaiki.
Dialogue: 0,0:04:49.15,0:04:50.60,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yuki!
Dialogue: 0,0:04:55.37,0:04:58.65,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Komandan, bentuk ini berbahaya!
Dialogue: 0,0:04:58.65,0:05:00.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak perlu dilepas.
Dialogue: 0,0:05:04.96,0:05:06.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ada Jurik, ya?
Dialogue: 0,0:05:07.70,0:05:09.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yuki!
Dialogue: 0,0:05:22.53,0:05:24.89,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sekarang, tunjukkan kekuatanmu.
Dialogue: 0,0:05:28.39,0:05:31.06,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hebat sekali!\NMati dalam sekejap!
Dialogue: 0,0:05:32.73,0:05:35.06,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Wah, ada banyak.
Dialogue: 0,0:05:56.49,0:05:58.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini menyenangkan sekali dilihat.
Dialogue: 0,0:06:02.73,0:06:03.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hei!
Dialogue: 0,0:06:08.66,0:06:13.48,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berani sekali mencoba menggigitku.\NDasar anjing nakal.
Dialogue: 0,0:06:14.54,0:06:16.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Biar kuberi pelajaran dulu.
Dialogue: 0,0:06:21.53,0:06:24.95,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku baru pertama kali lihat\Nbentuk seganas itu!
Dialogue: 0,0:06:25.46,0:06:26.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yuki!
Dialogue: 0,0:06:32.88,0:06:34.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak takut, ya?
Dialogue: 0,0:06:35.33,0:06:37.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berbeda dengan saat di wujud manusia.
Dialogue: 0,0:06:39.92,0:06:43.05,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku bisa menjalin hubungan baik dengan orang
Dialogue: 0,0:06:43.05,0:06:45.34,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}yang merasa takut dengan aura intimidasiku.
Dialogue: 0,0:06:50.06,0:06:51.68,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sedangkan yang tidak takut
Dialogue: 0,0:06:54.18,0:06:55.77,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}akan kuberi paham
Dialogue: 0,0:06:57.30,0:06:58.86,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}perbedaan di antara kita!
Dialogue: 0,0:07:12.09,0:07:13.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jurus itu
Dialogue: 0,0:07:16.96,0:07:19.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}akan kuhancurkan sebelum digunakan!
Dialogue: 0,0:07:23.03,0:07:25.76,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mau menggigitku lagi, ya?
Dialogue: 0,0:07:43.33,0:07:46.45,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lukanya pulih dalam sekejap.
Dialogue: 0,0:07:51.62,0:07:54.95,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berarti aku boleh memberimu\Npelajaran dengan lebih keras.
Dialogue: 0,0:07:56.36,0:07:58.37,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kemampuan Yamashiro Ren adalah
Dialogue: 0,0:07:58.37,0:08:01.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kesatuan Alam Semesta Tak Terhingga\Nyang Meliputi Segalanya.
Dialogue: 0,0:08:02.96,0:08:04.95,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dengan melepaskan mandala,
Dialogue: 0,0:08:04.95,0:08:07.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}ia meletakkan kekuatan\Ndari delapan Buddha ke bola matanya
Dialogue: 0,0:08:07.88,0:08:10.10,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dan menggunakannya sesuka hati.
Dialogue: 0,0:08:10.10,0:08:11.43,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yang mengerikan adalah
Dialogue: 0,0:08:11.43,0:08:15.23,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dia bisa menggunakan dua kekuatan\Nsecara bersamaan.
Dialogue: 0,0:08:18.78,0:08:21.90,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak ada yang tahu pasti\Ndelapan kekuatan itu
Dialogue: 0,0:08:22.32,0:08:25.28,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}kecuali Ren seorang.
Dialogue: 0,0:08:43.17,0:08:45.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Inilah kekuatan majikanmu.
Dialogue: 0,0:08:46.59,0:08:49.85,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kamu paham, anjing kecil?
Dialogue: 0,0:08:58.61,0:09:02.56,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yah, amukanmu di awal tadi\Nsungguh luar biasa.
Dialogue: 0,0:09:03.24,0:09:06.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sini, biar kuelus-elus sebagai hadiahmu.
Dialogue: 0,0:09:14.32,0:09:15.62,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hm?
Dialogue: 0,0:09:27.69,0:09:30.84,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Puncak dari kehidupan, Yamashiro Ren.
Dialogue: 0,0:09:30.84,0:09:33.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jawaban dari Bumi, Yamashiro Ren.
Dialogue: 0,0:09:35.40,0:09:38.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tentu peringkat satu ujian simulasi.
Dialogue: 0,0:09:39.19,0:09:41.59,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Juara di bidang olahraga.
Dialogue: 0,0:09:43.07,0:09:46.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lalu, juga selalu menjadi ketua OSIS.
Dialogue: 0,0:09:48.43,0:09:52.36,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pasukan elite pemilik kekuatan\Ndikalahkan semudah ini?
Dialogue: 0,0:09:52.36,0:09:55.03,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Meski begini, dia masih menahan diri,
Dialogue: 0,0:09:55.03,0:09:56.49,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bu Perdana Menteri.
Dialogue: 0,0:09:57.20,0:09:59.74,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Manusia menatap ke atas langit
Dialogue: 0,0:10:00.58,0:10:03.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dan melangkah demi menuju puncak.
Dialogue: 0,0:10:03.94,0:10:08.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Di akhir perjalanan,\Nakhirnya kami sampai padamu.
Dialogue: 0,0:10:09.01,0:10:12.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Puncak dari kehidupan, Yamashiro Ren.
Dialogue: 0,0:10:12.42,0:10:16.01,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jawaban dari Bumi, Yamashiro Ren.
Dialogue: 0,0:10:16.51,0:10:20.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bu Presiden menyanjungku\Ndengan berbagai kata-kata manis.
Dialogue: 0,0:10:21.52,0:10:24.14,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lalu diriku itu...
Dialogue: 0,0:10:27.90,0:10:32.19,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bukannya mengelus-elus,\Ntapi malah ini hadiahnya?
Dialogue: 0,0:10:41.02,0:10:43.85,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kukira kamu anjing yang manis,
Dialogue: 0,0:10:45.08,0:10:46.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}ternyata binatang buas.
Dialogue: 0,0:10:51.88,0:10:56.74,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tindakan konyol ini tidak bisa dibatalkan\Ndengan kemampuanku.
Dialogue: 0,0:10:57.44,0:11:01.18,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saat menguji kekuatan\Ndengan Kyouka melawan Juriknya,
Dialogue: 0,0:11:01.18,0:11:04.06,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}perubahannya bisa kubatalkan.
Dialogue: 0,0:11:04.57,0:11:08.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi, kontrak budaknya sendiri\Ntidak bisa dibatalkan.
Dialogue: 0,0:11:08.24,0:11:10.23,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Entah prinsipnya sama atau tidak.
Dialogue: 0,0:11:10.80,0:11:12.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Slave.
Dialogue: 0,0:11:13.16,0:11:16.03,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kekuatan ini lebih unik\Ndari yang dipikirkan orang-orang.
Dialogue: 0,0:11:19.67,0:11:21.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini masih berlanjut lagi?
Dialogue: 0,0:11:22.81,0:11:24.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mohon maaf!
Dialogue: 0,0:11:38.72,0:11:42.98,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kenapa malah aku yang dielus-elus?
Dialogue: 0,0:11:45.06,0:11:48.44,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Elus?\NAku harus mengelusnya?
Dialogue: 0,0:11:48.44,0:11:51.03,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mengelus komandan?
Dialogue: 0,0:11:54.97,0:11:56.55,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Begini?
Dialogue: 0,0:12:00.63,0:12:02.49,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Se-Seram!
Dialogue: 0,0:12:03.66,0:12:05.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Padahal kamu anjingnya!
Dialogue: 0,0:12:10.51,0:12:12.41,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Paham tidak, sih?
Dialogue: 0,0:12:19.79,0:12:22.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Main elus tanpa segan...
Dialogue: 0,0:12:26.12,0:12:27.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dasar binatang!
Dialogue: 0,0:12:30.32,0:12:32.40,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tubuhku bisa digerakkan!\NSudah selesai?
Dialogue: 0,0:12:37.40,0:12:38.82,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yuki!
Dialogue: 0,0:12:42.97,0:12:44.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia tidak apa-apa.
Dialogue: 0,0:12:44.81,0:12:48.89,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bentuk perubahanku\Nterlalu melelahkan untuk budak.
Dialogue: 0,0:12:48.89,0:12:50.37,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Latih dia lebih keras lagi.
Dialogue: 0,0:12:51.06,0:12:54.49,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ditambah bagaimana dia lepas kendali,\Nini berbahaya!
Dialogue: 0,0:12:54.49,0:12:56.34,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak perlu khawatir soal itu.
Dialogue: 0,0:12:57.53,0:13:00.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku pasti akan membuatnya patuh.
Dialogue: 0,0:13:14.29,0:13:15.41,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yuki!
Dialogue: 0,0:13:43.50,0:13:45.84,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Wakura Yuki!
Dialogue: 0,0:13:53.13,0:13:54.44,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aduh!
Dialogue: 0,0:13:55.51,0:13:58.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sekujur tubuhku nyeri semua.
Dialogue: 0,0:13:58.24,0:14:01.86,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Oh, iya, setelah itu aku pingsan...
Dialogue: 0,0:14:03.15,0:14:06.20,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku sama sekali tidak bisa\Nmengendalikan diri.
Dialogue: 0,0:14:07.51,0:14:09.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku harus lebih banyak berlatih!
Dialogue: 0,0:14:20.09,0:14:22.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pikiran kalutku... tidak mau hilang!
Dialogue: 0,0:14:25.09,0:14:29.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa meminjamkan Yuki\Nbisa mengacaukan hatiku sejauh ini?
Dialogue: 0,0:14:29.63,0:14:31.18,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kyouka!
Dialogue: 0,0:14:33.25,0:14:34.62,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kamu sudah bangun?
Dialogue: 0,0:14:34.62,0:14:37.15,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mohon latihannya!
Dialogue: 0,0:14:39.24,0:14:43.10,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kamu masih kelelahan, kan?
Dialogue: 0,0:14:43.10,0:14:45.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku sudah tidur, jadi pasti bisa!
Dialogue: 0,0:14:47.46,0:14:52.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Beban dari bentuk\Nperubahan Komandan terlalu besar.
Dialogue: 0,0:14:53.11,0:14:55.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku terkesan dengan tekadmu,
Dialogue: 0,0:14:55.54,0:14:57.85,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tapi sudah kubilang istirahat\Nsampai kamu pulih.
Dialogue: 0,0:14:57.85,0:15:01.76,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maaf, habisnya belakangan ini\Nsibuk ke sana ke mari.
Dialogue: 0,0:15:02.15,0:15:07.26,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku jadi ingin segera berlatih\Ndengan Kyouka lagi.
Dialogue: 0,0:15:09.85,0:15:11.23,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudahlah, istirahat saja.
Dialogue: 0,0:15:11.23,0:15:12.06,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi...
Dialogue: 0,0:15:15.31,0:15:17.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak akan efektif kalau babak belur, kan?
Dialogue: 0,0:15:17.46,0:15:18.77,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini perintah.
Dialogue: 0,0:15:19.60,0:15:20.82,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baik.
Dialogue: 0,0:15:30.15,0:15:33.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Efek latihan gabungan\Nkelihatannya cukup tinggi.
Dialogue: 0,0:15:33.29,0:15:35.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ke depannya, mari kita teruskan\Nkalau ada kesempatan.
Dialogue: 0,0:15:40.50,0:15:43.77,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Suasana hatinya kelihatan sangat jelek!
Dialogue: 0,0:15:44.65,0:15:46.01,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi angin...
Dialogue: 0,0:15:46.01,0:15:48.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadilah angin, agar tidak jadi incaran.
Dialogue: 0,0:15:49.36,0:15:52.98,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Omong-omong, Kyouka, di mana Wakura Yuki?
Dialogue: 0,0:15:52.98,0:15:55.10,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudah kuperintah untuk mengajak dia, kan?
Dialogue: 0,0:15:57.48,0:16:01.28,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa Yuki sudah menarik perhatiannya?
Dialogue: 0,0:16:01.91,0:16:03.48,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Akan merepotkan banyak hal.
Dialogue: 0,0:16:03.48,0:16:06.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia kelelahan setelah berlatih\Ndengan Skuad Sepuluh!
Dialogue: 0,0:16:06.38,0:16:07.99,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saat ini dia tidak bisa apa-apa!
Dialogue: 0,0:16:07.99,0:16:10.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dik Budak kasihan banget!
Dialogue: 0,0:16:11.49,0:16:12.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Oh.
Dialogue: 0,0:16:13.24,0:16:14.95,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau begitu...
Dialogue: 0,0:16:15.35,0:16:17.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bell harus tinggal di sini, ya.
Dialogue: 0,0:16:24.02,0:16:28.22,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yuki sudah bisa dipinjamkan, ya?
Dialogue: 0,0:16:29.49,0:16:32.68,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudah kubilang di rapat,\NYuki sedang kelelahan!
Dialogue: 0,0:16:32.68,0:16:37.35,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya, dia sedang kelelahan.\NKasihan sekali.
Dialogue: 0,0:16:37.82,0:16:41.19,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau ada sesuatu,\NSkuad Enam akan membantu.
Dialogue: 0,0:16:41.19,0:16:42.93,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Biarkan dia istirahat.
Dialogue: 0,0:16:44.91,0:16:48.43,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kukira kamu juga akan minta untuk dipinjami.
Dialogue: 0,0:16:49.03,0:16:50.74,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku tertarik, kok.
Dialogue: 0,0:16:50.74,0:16:53.28,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dulu aku juga pernah menyarankan begitu.
Dialogue: 0,0:16:54.39,0:16:57.32,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hanya saja, tidak perlu buru-buru.
Dialogue: 0,0:16:57.32,0:17:00.32,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hubungan kami sudah bukan seperti dulu lagi.
Dialogue: 0,0:17:02.50,0:17:04.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ketua Uzen.
Dialogue: 0,0:17:06.01,0:17:08.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ada yang ingin kubicarakan.
Dialogue: 0,0:17:09.82,0:17:13.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terima kasih, berkat kalian bergantian,
Dialogue: 0,0:17:13.63,0:17:15.49,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}aku mulai pulih.
Dialogue: 0,0:17:15.49,0:17:18.48,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku mau kamu cepat kerja lagi soalnya.
Dialogue: 0,0:17:18.90,0:17:21.65,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Masih ada latihan denganku juga.
Dialogue: 0,0:17:21.65,0:17:22.90,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Iya.
Dialogue: 0,0:17:23.82,0:17:25.35,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Oh, iya...
Dialogue: 0,0:17:27.05,0:17:29.61,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sebelumnya, sudah seperti itu.
Dialogue: 0,0:17:30.52,0:17:33.73,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Selanjutnya bakal secabul apa, ya?
Dialogue: 0,0:17:37.16,0:17:38.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku kembali.
Dialogue: 0,0:17:38.43,0:17:40.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ketua, selamat datang!
Dialogue: 0,0:17:44.50,0:17:46.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Akhirnya datang juga.
Dialogue: 0,0:17:48.11,0:17:50.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Minggir dulu, Himari.
Dialogue: 0,0:17:50.25,0:17:50.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Eh?
Dialogue: 0,0:17:53.17,0:17:55.89,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kamu belum terbiasa mengurus otot.
Dialogue: 0,0:17:58.73,0:18:01.56,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini bukanlah kekuatanku.
Dialogue: 0,0:18:06.13,0:18:07.98,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku jadi agak segar lagi!
Dialogue: 0,0:18:08.68,0:18:10.34,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Akan kulanjutkan, ya.
Dialogue: 0,0:18:10.34,0:18:12.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lemaskan tubuhmu.
Dialogue: 0,0:18:12.24,0:18:14.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi tangannya digerakkan seperti itu.
Dialogue: 0,0:18:15.41,0:18:18.53,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini teknik untuk meredakan rasa lelah\Nyang menumpuk akibat kesibukan.
Dialogue: 0,0:18:19.25,0:18:20.32,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Himari.
Dialogue: 0,0:18:21.35,0:18:23.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Perjamuan Azuma akan digelar.
Dialogue: 0,0:18:24.26,0:18:26.19,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Perjamuan Azuma?
Dialogue: 0,0:18:26.19,0:18:28.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pertarungan untuk menentukan\Nkepala keluarga selanjutnya!
Dialogue: 0,0:18:29.75,0:18:32.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Melihat Skuad Satu yang ganti generasi,
Dialogue: 0,0:18:32.67,0:18:34.95,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ibunda pun mengumumkannya.
Dialogue: 0,0:18:34.95,0:18:38.30,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Normalnya, aku yang akan\Njadi penerus selaku putrinya.
Dialogue: 0,0:18:38.75,0:18:42.52,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi di situlah bedanya Azuma,\Nakan ditentukan dengan kekuatan.
Dialogue: 0,0:18:43.44,0:18:45.82,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Perjamuan Azuma digelar lima hari lagi.
Dialogue: 0,0:18:45.82,0:18:48.87,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ambillah cuti dan berkumpul\Ndi kediaman Azuma.
Dialogue: 0,0:18:48.87,0:18:51.36,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudah kusampaikan ke semua ketua skuad.
Dialogue: 0,0:18:51.36,0:18:52.99,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tunggu dulu!
Dialogue: 0,0:18:52.99,0:18:55.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku tidak akan datang.
Dialogue: 0,0:18:55.46,0:18:58.53,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku juga tidak sudi melihat kediaman Azuma.
Dialogue: 0,0:18:58.53,0:19:00.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lakukan saja sendiri!
Dialogue: 0,0:19:00.54,0:19:03.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Siapa pun kepala keluarganya,\Naku tidak peduli!
Dialogue: 0,0:19:10.46,0:19:11.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Himari.
Dialogue: 0,0:19:14.97,0:19:16.97,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mau mandi bersama tidak?
Dialogue: 0,0:19:16.97,0:19:18.22,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak mau!
Dialogue: 0,0:19:22.72,0:19:24.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Otot yang bagus.
Dialogue: 0,0:19:24.67,0:19:26.81,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudah banyak yang tumbuh.
Dialogue: 0,0:19:27.59,0:19:29.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Meski begitu, kamu tetap terluka parah.
Dialogue: 0,0:19:29.79,0:19:32.35,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lemah di saat genting bisa berakibat fatal.
Dialogue: 0,0:19:32.99,0:19:37.12,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dari sekarang akan makin banyak orang terluka\Nmelindungi Himari yang lemah.
Dialogue: 0,0:19:37.99,0:19:39.74,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sama seperti Yachiho.
Dialogue: 0,0:19:39.74,0:19:41.77,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan asal memutuskan!
Dialogue: 0,0:19:41.77,0:19:45.31,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau Himari ingin lanjut bertarung.
Dialogue: 0,0:19:45.31,0:19:48.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sebelum membuat malu Klan Azuma,
Dialogue: 0,0:19:49.31,0:19:53.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}aku akan menghabisimu selaku ibumu.
Dialogue: 0,0:19:56.07,0:19:58.76,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau tidak mau seperti itu,\Nikutilah perjamuannya.
Dialogue: 0,0:19:59.63,0:20:01.72,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lalu buktikan kekuatanmu, begitu?
Dialogue: 0,0:20:02.19,0:20:03.44,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baiklah!
Dialogue: 0,0:20:03.44,0:20:06.18,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku juga sudah memutuskan\Nuntuk menghajar Klan Azuma!
Dialogue: 0,0:20:11.86,0:20:16.39,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku jarang mengobrol\Ndengan anggota Skuad Sembilan.
Dialogue: 0,0:20:16.39,0:20:19.61,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak usah mengobrol kecuali soal pekerjaan.
Dialogue: 0,0:20:20.02,0:20:21.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Klan Azuma.
Dialogue: 0,0:20:22.35,0:20:27.12,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Klan unggul yang punya sejarah panjang\Ndengan Korps Defensi Iblis.
Dialogue: 0,0:20:27.12,0:20:31.23,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terutama Tobera yang merupakan\Nmantan komandan.
Dialogue: 0,0:20:31.23,0:20:33.34,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Entah dia manusia atau bukan.
Dialogue: 0,0:20:36.18,0:20:38.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hubunganmu kelihatan baik\Ndengan Keluarga Azuma.
Dialogue: 0,0:20:38.88,0:20:41.93,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku dititipi putri mereka yang berharga.
Dialogue: 0,0:20:42.42,0:20:45.47,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Makanya, aku juga ahli\Nsoal urusan bertetangga.
Dialogue: 0,0:20:45.47,0:20:48.47,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tenka benar-benar hebat!
Dialogue: 0,0:20:49.95,0:20:52.70,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kamu sudah lebih segar sekarang, Yuki.
Dialogue: 0,0:20:52.70,0:20:58.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Iya, tapi bentuk perubahan Komandan\Nbenar-benar menyiksa.
Dialogue: 0,0:20:58.38,0:21:01.57,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berarti ke depannya,\Nakan sulit melakukan peminjaman.
Dialogue: 0,0:21:01.57,0:21:03.37,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu benar!
Dialogue: 0,0:21:03.37,0:21:05.87,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku tidak akan dengan mudah\Nmeminjamkannya lagi!
Dialogue: 0,0:21:05.87,0:21:08.69,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bentuk perubahanku harus lebih diperkuat!
Dialogue: 0,0:21:08.69,0:21:10.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini yang terbaik!
Dialogue: 0,0:21:10.39,0:21:14.39,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tenang saja! Aku pasti pulih total\Ndalam beberapa hari!
Dialogue: 0,0:21:14.39,0:21:18.01,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini juga berkat pijatan Bu Fubuki.
Dialogue: 0,0:21:21.51,0:21:22.95,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudah kulihat gerakan tangannya!
Dialogue: 0,0:21:22.95,0:21:24.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kurasa aku juga bisa!
Dialogue: 0,0:21:24.83,0:21:28.02,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Akan kupelajari juga sebagai ketua.
Dialogue: 0,0:21:28.02,0:21:30.11,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Nei juga ingin menguasainya!
Dialogue: 0,0:21:30.11,0:21:31.64,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku juga!
Dialogue: 0,0:21:32.09,0:21:36.07,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudah, sudah,\Nayo serahkan tubuhmu sini, Yuki.
Dialogue: 0,0:21:36.62,0:21:40.26,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ucapannya ingin menghabisiku\Nbukanlah penyemangat belaka.
Dialogue: 0,0:21:40.26,0:21:42.24,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Azuma pasti tega melakukannya.
Dialogue: 0,0:23:18.83,0:23:21.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku pulang, Kaname, Miyabi.
Dialogue: 0,0:23:27.09,0:23:30.65,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Selanjutnya, episode lima,\NPerjamuan Azuma!
Dialogue: 0,0:23:30.65,0:23:32.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sekarang bonus untuk hari ini.
Dialogue: 0,0:23:32.58,0:23:37.06,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Nona Ren yang menghukum orang bodoh\Ndi Kota Iblis luar biasa menawan!
Dialogue: 0,0:23:37.06,0:23:40.06,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau aku yang di sana,\Npasti takut setengah mati!
