Dialogue: 0,0:06:00.13,0:06:05.00,tanda,,0000,0000,0000,,EPISODE 2\NLAHIRNYA SANG DIVA
Dialogue: 0,0:06:47.46,0:06:51.00,tanda,,0000,0000,0000,,PANGERAN KE-4\NHENRY ARARAT
Dialogue: 0,0:14:00.92,0:14:03.75,tanda,,0000,0000,0000,,OSCAR ARARAT\NPANGERAN KE-8
Dialogue: 0,0:00:05.21,0:00:06.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Ibukota Kekaisaran Meeres.{\i0}
Dialogue: 0,0:00:09.13,0:00:11.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Jantung pemerintahan bangsa ini.{\i0}
Dialogue: 0,0:00:12.88,0:00:14.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yah, kau tahu...
Dialogue: 0,0:00:14.67,0:00:16.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Persis seperti terakhir kali.
Dialogue: 0,0:00:16.92,0:00:17.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau bicara apa...
Dialogue: 0,0:00:19.83,0:00:21.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Pusat kebudayaan...{\i0}
Dialogue: 0,0:00:24.75,0:00:27.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}dan pusat ekonomi yang sibuk.{\i0}
Dialogue: 0,0:00:27.75,0:00:31.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Orang-orang akhirnya mulai menyebutnya...{\i0}
Dialogue: 0,0:00:32.17,0:00:35.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}"kota paling makmur di Bumi."{\i0}
Dialogue: 0,0:00:38.25,0:00:39.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Jadi ini Raid Street.{\i0}
Dialogue: 0,0:00:40.21,0:00:43.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Para kasim menyebut{\i0}\N{\i1}ada penyanyi baru di sini.{\i0}
Dialogue: 0,0:00:44.21,0:00:45.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mau kita lihat?
Dialogue: 0,0:00:45.96,0:00:47.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Izinkan saya menemani Anda.
Dialogue: 0,0:00:47.67,0:00:49.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Kau bersikeras ingin membalas budi,{\i0}
Dialogue: 0,0:00:49.96,0:00:51.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}jadi aku memberimu banyak pekerjaan.{\i0}
Dialogue: 0,0:00:51.75,0:00:52.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Silakan.
Dialogue: 0,0:00:52.71,0:00:53.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya.
Dialogue: 0,0:00:53.96,0:00:57.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Rajin dan penuh perhatian{\i0}\N{\i1}pada hal terkecil.{\i0}
Dialogue: 0,0:00:59.63,0:01:01.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Kau benar-benar telah berkembang.{\i0}
Dialogue: 0,0:01:04.63,0:01:09.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Tak ada ruginya punya{\i0}\N{\i1}banyak orang berbakat.{\i0}
Dialogue: 0,0:01:47.46,0:01:49.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Kalau tidak salah, namanya Alichey?{\i0}
Dialogue: 0,0:01:50.58,0:01:52.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Begitu ya. Boleh juga.{\i0}
Dialogue: 0,0:01:52.42,0:01:54.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Meski agak kasar,{\i0}\N{\i1}tapi dia bernyanyi dengan hati.{\i0}
Dialogue: 0,0:01:55.04,0:01:57.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Aku bisa merasakan dedikasinya.{\i0}
Dialogue: 0,0:01:59.08,0:02:00.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Dia pasti gadis yang serius.{\i0}
Dialogue: 0,0:02:06.21,0:02:09.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Punya nyali juga kau,\Nbernyanyi santai di sini.
Dialogue: 0,0:02:09.96,0:02:10.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hah?!
Dialogue: 0,0:02:17.96,0:02:19.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}To-Tolong hentikan, Pak!
Dialogue: 0,0:02:20.04,0:02:21.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mundur!
Dialogue: 0,0:02:25.46,0:02:29.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Uang receh begini tak akan\Nmenutup utang ayahmu!
Dialogue: 0,0:02:30.96,0:02:34.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi, hanya ini cara saya cari uang...
Dialogue: 0,0:02:40.21,0:02:45.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan sampai kusebutkan.\NWanita punya cara sendiri untuk cari uang.
Dialogue: 0,0:02:51.42,0:02:52.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hei.
Dialogue: 0,0:02:52.29,0:02:53.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hah?!
Dialogue: 0,0:02:53.58,0:02:54.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tuan.
Dialogue: 0,0:02:57.96,0:03:01.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pulang sana dan mengadu\Npada ibumu, dasar bocah.
Dialogue: 0,0:03:01.88,0:03:04.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hukum Kekaisaran,\NPasal 67, Ayat 2 menyatakan:
Dialogue: 0,0:03:05.08,0:03:08.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}"Anak tidak berkewajiban untuk\Nmenanggung utang orang tuanya."
Dialogue: 0,0:03:13.50,0:03:15.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Memaksa seorang anak\Nmembayar utang ayahnya
Dialogue: 0,0:03:15.83,0:03:19.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dan menjual tubuhnya untuk itu,\Nadalah tindak pidana yang jelas.
Dialogue: 0,0:03:19.21,0:03:21.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jaga mulutmu, Bocah!
Dialogue: 0,0:03:25.83,0:03:26.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aduh!
Dialogue: 0,0:03:26.83,0:03:28.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa-apaan orang ini?!
Dialogue: 0,0:03:31.25,0:03:32.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mau lagi?
Dialogue: 0,0:03:34.25,0:03:35.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau akan membayarnya!
Dialogue: 0,0:03:44.50,0:03:47.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jika mereka kembali,\Nsuruh mereka mencariku di mansion.
Dialogue: 0,0:03:48.75,0:03:49.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baik, Tuan.
Dialogue: 0,0:03:52.29,0:03:53.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya-Yang Mulia!
Dialogue: 0,0:03:53.96,0:03:56.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Eh, Pangeran?
Dialogue: 0,0:04:01.63,0:04:03.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Suaramu sangatlah indah.
Dialogue: 0,0:04:03.54,0:04:05.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berapa pengeluaran bulananmu?
Dialogue: 0,0:04:05.83,0:04:08.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Eh? Eng...
Dialogue: 0,0:04:08.50,0:04:10.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Termasuk biaya pengobatan ibu saya...
Dialogue: 0,0:04:10.33,0:04:11.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}25 riin?
Dialogue: 0,0:04:12.25,0:04:13.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hah?
Dialogue: 0,0:04:13.75,0:04:16.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku akan menggajimu tiap bulan,\Njadi bernyanyilah yang tenang.
Dialogue: 0,0:04:17.42,0:04:19.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau berkembang, gajinya kunaikkan.
Dialogue: 0,0:04:21.79,0:04:23.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terima kasih banyak!
Dialogue: 0,0:04:26.42,0:04:28.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sekarang, lanjutkan yang tadi.
Dialogue: 0,0:04:38.63,0:04:41.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Break off chaos{\i0}
Dialogue: 0,0:04:42.88,0:04:46.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Break off chaos{\i0}
Dialogue: 0,0:04:50.00,0:04:51.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Orang-orang adalah harta{\i0}\N{\i1}Teman pun adanya{\i0}
Dialogue: 0,0:04:52.04,0:04:54.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Permata bersinar di belakangku{\i0}\N{\i1}Setetes air di tengah kekacauan{\i0}
Dialogue: 0,0:04:54.13,0:04:58.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Demi kehormatan{\i0}\N{\i1}Kumelaju ke tingkat baru{\i0}
Dialogue: 0,0:04:58.75,0:05:00.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Dengan imaji dan naluri{\i0}\N{\i1}Kuterkam batas level itu{\i0}
Dialogue: 0,0:05:00.63,0:05:02.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Semua di depan mataku kan tunduk{\i0}
Dialogue: 0,0:05:02.58,0:05:04.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Nilai dari namaku bagi rakyat tak bersalah{\i0}\N{\i1}Adalah tolok ukur yang tak tergoyahkan{\i0}
Dialogue: 0,0:05:05.04,0:05:07.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Itulah sumber kebenaran{\i0}\N{\i1}Yang kehormatan pun pontang-panting{\i0}
Dialogue: 0,0:05:07.17,0:05:08.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Lalui kemelut{\i0}
Dialogue: 0,0:05:09.21,0:05:11.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Kuakan tegakkan keadilan{\i0}
Dialogue: 0,0:05:12.13,0:05:15.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Di tengah ketakutan merajalela{\i0}
Dialogue: 0,0:05:15.38,0:05:19.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Bebaskan tiap insan dari rasa takut{\i0}
Dialogue: 0,0:05:19.71,0:05:23.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Kuakan selamatkan dunia{\i0}
Dialogue: 0,0:05:23.96,0:05:28.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Menelusuri memori{\i0}\N{\i1}Gunakan apa yang kupunya{\i0}
Dialogue: 0,0:05:28.79,0:05:30.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Membangun tempat untuk semua{\i0}
Dialogue: 0,0:05:30.92,0:05:32.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Kutakkan gagal{\i0}
Dialogue: 0,0:05:32.79,0:05:36.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Masa depan sulit 'tuk ditebak{\i0}
Dialogue: 0,0:05:36.92,0:05:40.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Kulakukan apa yang kubisa{\i0}
Dialogue: 0,0:05:41.29,0:05:46.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Jika hanya yakin dan menunggu{\i0}
Dialogue: 0,0:05:46.21,0:05:48.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Tiada yang mulai{\i0}
Dialogue: 0,0:05:48.13,0:05:49.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Aku kan meraihnya!{\i0}
Dialogue: 0,0:05:49.96,0:05:53.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}chaos chaos{\i0}
Dialogue: 0,0:05:54.29,0:05:57.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Break off chaos{\i0}
Dialogue: 0,0:06:17.50,0:06:18.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Selamat datang kembali.
Dialogue: 0,0:06:21.50,0:06:22.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Siapa mereka?
Dialogue: 0,0:06:22.96,0:06:24.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Para lintah darat, Tuanku.
Dialogue: 0,0:06:25.33,0:06:26.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ah, orang-orang itu.
Dialogue: 0,0:06:28.13,0:06:31.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mereka memohon untuk\Nmeminta maaf pada Anda.
Dialogue: 0,0:06:32.17,0:06:34.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baru sadar aku keluarga kerajaan, ya?
Dialogue: 0,0:06:35.67,0:06:36.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Usir mereka.
Dialogue: 0,0:06:36.96,0:06:41.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Katakan pada mereka, "Keluarga kerajaan\Ntak bisa kalian temui seenaknya."
Dialogue: 0,0:06:41.58,0:06:43.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dimengerti.
Dialogue: 0,0:06:43.83,0:06:45.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ditangani dengan baik.
Dialogue: 0,0:06:51.67,0:06:54.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bergaul dengan sampah seperti itu
Dialogue: 0,0:06:54.33,0:06:57.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}hanya akan mencoreng nama Kerajaan.
Dialogue: 0,0:06:57.92,0:07:01.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu benar, tapi saya punya alasan lain.
Dialogue: 0,0:07:01.54,0:07:02.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hmm?
Dialogue: 0,0:07:02.54,0:07:04.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mari kita bicara di dalam.
Dialogue: 0,0:07:05.25,0:07:07.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ah, menyokong seorang penyanyi?
Dialogue: 0,0:07:07.88,0:07:11.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saya membuat mereka berpikir\Nsaya masih marah.
Dialogue: 0,0:07:11.75,0:07:13.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mereka tak akan berani\Nmenyentuhnya sekarang.
Dialogue: 0,0:07:14.21,0:07:17.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa suaranya sebagus itu?
Dialogue: 0,0:07:17.42,0:07:19.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tolong jangan rebut dia dari saya.
Dialogue: 0,0:07:19.21,0:07:22.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau sangat dewasa untuk usiamu.
Dialogue: 0,0:07:23.38,0:07:26.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Frank dan Gary bahkan\Ntak bisa dibandingkan.
Dialogue: 0,0:07:27.42,0:07:28.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Begitukah?
Dialogue: 0,0:07:28.96,0:07:30.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mereka masih kekanak-kanakan.
Dialogue: 0,0:07:31.13,0:07:32.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terutama Gary.
Dialogue: 0,0:07:33.04,0:07:35.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia tak pernah belajar sopan santun.
Dialogue: 0,0:07:35.50,0:07:37.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saya yakin dia akan belajar pada akhirnya.
Dialogue: 0,0:07:39.33,0:07:42.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Kami mungkin bersaudara,{\i0}\N{\i1}tapi ibu kami berbeda.{\i0}
Dialogue: 0,0:07:43.17,0:07:46.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Meski Kanda Henry{\i0}\N{\i1}dan Kanda Gary satu ibu,{\i0}
Dialogue: 0,0:07:46.71,0:07:49.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}ikatan mereka pasti berbeda.{\i0}
Dialogue: 0,0:07:50.67,0:07:54.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi, kau sudah menguasai\NPedang Iblis itu?
Dialogue: 0,0:07:55.42,0:07:57.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berkat peringatan Kanda.
Dialogue: 0,0:07:57.13,0:07:59.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pedang itu benar-benar\Npunya kehendak sendiri.
Dialogue: 0,0:07:59.67,0:08:01.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lantas, bagaimana kau membawanya?
Dialogue: 0,0:08:02.04,0:08:03.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu terlalu besar untukmu.
Dialogue: 0,0:08:04.54,0:08:06.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak juga.
Dialogue: 0,0:08:06.88,0:08:07.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hm?
Dialogue: 0,0:08:09.83,0:08:11.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dari mana munculnya?
Dialogue: 0,0:08:11.50,0:08:13.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Disembunyikan di lengan baju saya.
Dialogue: 0,0:08:14.08,0:08:15.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Seperti ini.
Dialogue: 0,0:08:19.71,0:08:21.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pedang ini mengajari saya caranya.
Dialogue: 0,0:08:23.21,0:08:24.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tak pernah terdengar sebelumnya.
Dialogue: 0,0:08:25.08,0:08:27.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu sudah jadi Harta Nasional\Nselama 200 tahun,
Dialogue: 0,0:08:28.13,0:08:30.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tapi tak ada catatan\Nyang menyebutkan ini.
Dialogue: 0,0:08:31.25,0:08:33.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Harus kuakui, kau itu mengesankan.
Dialogue: 0,0:08:43.75,0:08:45.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu mereka?
Dialogue: 0,0:08:45.13,0:08:46.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya.
Dialogue: 0,0:08:48.42,0:08:50.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Cukup sudah!
Dialogue: 0,0:08:50.33,0:08:54.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan abaikan aku, dasar bocah tolol!
Dialogue: 0,0:08:54.29,0:08:56.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}- Bodoh! Apa yang kau lakukan?!\N- Woi!
Dialogue: 0,0:08:56.58,0:09:00.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku bisa membunuhmu di sini!
Dialogue: 0,0:09:06.33,0:09:08.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bodoh! Cepat minta maaf!
Dialogue: 0,0:09:09.17,0:09:10.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa yang kau lakukan?
Dialogue: 0,0:09:11.67,0:09:14.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Melepaskan sedikit kekuatannya.
Dialogue: 0,0:09:14.88,0:09:16.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hanya untuk mengancam mereka.
Dialogue: 0,0:09:17.42,0:09:19.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi kau benar-benar menjinakkannya.
Dialogue: 0,0:09:24.92,0:09:27.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Selamat datang, Pangeran Noah.
Dialogue: 0,0:09:28.08,0:09:30.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terima kasih undangannya, Raydork.
Dialogue: 0,0:09:30.08,0:09:32.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sepertinya malam yang meriah.
Dialogue: 0,0:09:49.58,0:09:51.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa itu Pangeran Ke-13?
Dialogue: 0,0:09:51.46,0:09:53.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Muda, tapi berwibawa.
Dialogue: 0,0:09:53.21,0:09:55.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Putra Kaisar memang hebat.
Dialogue: 0,0:09:57.08,0:10:00.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yang Mulia,\Napa yang ada di pinggang Anda itu?
Dialogue: 0,0:10:00.29,0:10:01.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini?
Dialogue: 0,0:10:01.38,0:10:05.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini adalah Leviathan,\Nhadiah dari Putra Mahkota.
Dialogue: 0,0:10:05.75,0:10:07.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Harta Nasional itu?
Dialogue: 0,0:10:07.21,0:10:08.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Diberikan langsung oleh Putra Mahkota?
Dialogue: 0,0:10:09.00,0:10:12.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pangeran Noah memerintah Almeria, kan?
Dialogue: 0,0:10:12.29,0:10:14.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ah, aku paham hubungannya sekarang.
Dialogue: 0,0:10:14.50,0:10:17.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Halah. Paling cuma mainan palsu.
Dialogue: 0,0:10:17.63,0:10:19.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Eh? Ah!
Dialogue: 0,0:10:34.54,0:10:35.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tahan.
Dialogue: 0,0:10:41.25,0:10:44.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Kau marah karena aku dihina?{\i0}
Dialogue: 0,0:10:44.71,0:10:46.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Loyalitasmu yang tadi itu berlebihan.{\i0}
Dialogue: 0,0:10:46.96,0:10:49.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Luar biasa, Yang Mulia.
Dialogue: 0,0:10:50.00,0:10:54.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sesaat, semua orang di sini\Nmerasakan "kematian".
Dialogue: 0,0:10:54.38,0:10:56.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itulah kekuatan Harta Nasional.
Dialogue: 0,0:10:57.00,0:11:01.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dan Anda mengendalikan kekuatan besar itu.
Dialogue: 0,0:11:01.25,0:11:02.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Be-Benar!
Dialogue: 0,0:11:02.63,0:11:05.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kita selamat\Nberkat beliau menghentikannya!
Dialogue: 0,0:11:05.63,0:11:07.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sudah pasti itu barang asli!
Dialogue: 0,0:11:07.13,0:11:09.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Menakjubkan, Pangeran Noah!
Dialogue: 0,0:11:14.13,0:11:16.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa yang harus kita lakukan padanya?
Dialogue: 0,0:11:19.54,0:11:20.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lepaskan saja.
Dialogue: 0,0:11:21.13,0:11:22.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia tidak punya tempat lagi.
Dialogue: 0,0:11:35.21,0:11:37.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sebuah kehormatan\Nbertemu dengan Anda, Yang Mulia.
Dialogue: 0,0:11:37.71,0:11:38.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya.
Dialogue: 0,0:11:40.33,0:11:43.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saya pedagang, Lance Rawlinson.
Dialogue: 0,0:11:43.58,0:11:47.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sungguh sebuah kehormatan tertinggi\Nbisa berada di hadapan Anda.
Dialogue: 0,0:11:47.63,0:11:48.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baik.
Dialogue: 0,0:11:51.04,0:11:53.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saya mengelola Serikat Dagang Alan.
Dialogue: 0,0:11:53.50,0:11:55.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saya Byron Alan.
Dialogue: 0,0:11:55.63,0:11:59.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kehormatan mutlak bagi saya\Nbisa bertemu Anda.
Dialogue: 0,0:12:01.17,0:12:03.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saya putrinya, Cindy Alan.
Dialogue: 0,0:12:03.92,0:12:04.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya.
Dialogue: 0,0:12:05.33,0:12:07.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa dia diadopsi?
Dialogue: 0,0:12:07.46,0:12:09.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}A-Anda tahu?
Dialogue: 0,0:12:09.67,0:12:11.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak, tapi itu terlihat jelas.
Dialogue: 0,0:12:11.63,0:12:14.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Alan, kau orang kaya turunan\Ngenerasi kedua atau ketiga?
Dialogue: 0,0:12:14.42,0:12:15.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Y-Ya.
Dialogue: 0,0:12:16.04,0:12:19.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau punya keanggunan alami kalangan atas.
Dialogue: 0,0:12:20.71,0:12:22.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi dia tidak.
Dialogue: 0,0:12:23.38,0:12:26.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ternyata Pangeran punya mata yang jeli.
Dialogue: 0,0:12:26.50,0:12:29.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jadi, kau mengadopsi mantan budak?
Dialogue: 0,0:12:29.25,0:12:32.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}A-Anda bisa tahu sampai sejauh itu?!
Dialogue: 0,0:12:32.54,0:12:36.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Saat dia panik, matanya berubah waspada.{\i0}
Dialogue: 0,0:12:37.25,0:12:39.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Tatapan khas seseorang{\i0}\N{\i1}yang pernah mengenal perbudakan.{\i0}
Dialogue: 0,0:12:40.96,0:12:42.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tepat seperti yang Anda katakan.
Dialogue: 0,0:12:42.88,0:12:45.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saya membelinya sebagai budak,
Dialogue: 0,0:12:45.21,0:12:49.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}tapi dia cepat belajar,\Njadi saya mengadopsinya.
Dialogue: 0,0:12:49.71,0:12:50.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bagus.
Dialogue: 0,0:12:51.00,0:12:52.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku suka itu.
Dialogue: 0,0:12:52.38,0:12:53.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Eh?
Dialogue: 0,0:12:54.08,0:12:56.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Datanglah ke mansionku kapan-kapan.
Dialogue: 0,0:12:56.21,0:12:58.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ceritakan lebih banyak tentang putrimu.
Dialogue: 0,0:13:00.08,0:13:01.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ba-Baik. Dengan senang hati.
Dialogue: 0,0:13:03.50,0:13:05.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baiklah, kami permisi.
Dialogue: 0,0:13:09.58,0:13:11.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia tahu kapan harus pergi.
Dialogue: 0,0:13:11.58,0:13:15.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Dia cerdas. Tinggal lebih lama\Nakan mengundang kecemburuan.
Dialogue: 0,0:13:15.88,0:13:18.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pertama kalinya ada\Nyang menarik perhatian beliau.
Dialogue: 0,0:13:18.58,0:13:20.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bikin iri saja.
Dialogue: 0,0:13:20.54,0:13:24.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ada satu alasan lagi yang aku tahu.
Dialogue: 0,0:13:24.71,0:13:25.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Oh?
Dialogue: 0,0:13:29.54,0:13:30.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Seperti yang kukatakan.
Dialogue: 0,0:13:31.17,0:13:34.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saat aku mengancam mereka,\Ndia mencoba melindungi Byron.
Dialogue: 0,0:13:34.71,0:13:38.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Biasanya, anak seusianya\Nyang akan dilindungi.
Dialogue: 0,0:13:38.21,0:13:43.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Benar juga.\NTernyata Anda bisa melihat semua itu.
Dialogue: 0,0:13:43.25,0:13:45.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saya sangat menghormati Anda,
Dialogue: 0,0:13:45.92,0:13:47.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}dari lubuk hati saya yang terdalam.
Dialogue: 0,0:13:59.08,0:14:00.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Noah? Apa itu kau?
Dialogue: 0,0:14:09.08,0:14:10.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kanda Oscar.
Dialogue: 0,0:14:11.13,0:14:13.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sedang apa kau di sini?
Dialogue: 0,0:14:13.71,0:14:15.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hanya jalan-jalan.
Dialogue: 0,0:14:15.79,0:14:17.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tanpa kereta kuda?
Dialogue: 0,0:14:17.58,0:14:19.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau menarik seperti biasa, ya.
Dialogue: 0,0:14:20.79,0:14:23.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Benar juga. Mau berburu harta karun?
Dialogue: 0,0:14:24.58,0:14:25.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hm?
Dialogue: 0,0:14:26.83,0:14:29.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Begitu. Cobalt Street, ya?
Dialogue: 0,0:14:29.63,0:14:31.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Oh, kau pernah ke sana?
Dialogue: 0,0:14:32.25,0:14:34.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak, tapi saya pernah dengar.
Dialogue: 0,0:14:34.83,0:14:37.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tempat barang antik\Ndari seluruh penjuru negeri.
Dialogue: 0,0:14:37.88,0:14:39.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Juga banyak barang palsu.
Dialogue: 0,0:14:39.83,0:14:44.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Menemukan permata di antara sampah\Nadalah hobi yang mulia.
Dialogue: 0,0:14:46.63,0:14:47.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Oh.
Dialogue: 0,0:14:52.83,0:14:55.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tak bagus. Tak ada hasil hari ini.
Dialogue: 0,0:14:55.58,0:14:56.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Begitu?
Dialogue: 0,0:15:01.00,0:15:02.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Berapa harga ini?
Dialogue: 0,0:15:02.50,0:15:04.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tiga riin.
Dialogue: 0,0:15:04.33,0:15:05.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hmm.
Dialogue: 0,0:15:06.54,0:15:07.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ada apa?
Dialogue: 0,0:15:07.96,0:15:09.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Menemukan sesuatu yang bagus?
Dialogue: 0,0:15:10.00,0:15:11.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak, hanya penasaran.
Dialogue: 0,0:15:12.17,0:15:13.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ini...
Dialogue: 0,0:15:13.33,0:15:14.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tidak salah lagi.
Dialogue: 0,0:15:14.46,0:15:17.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kitab sihir milik\Npenyihir legendaris Andrea!
Dialogue: 0,0:15:17.79,0:15:18.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Asli juga!
Dialogue: 0,0:15:20.25,0:15:21.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Harta karun sungguhan!
Dialogue: 0,0:15:22.38,0:15:25.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tergantung isinya,\Nharganya setidaknya seribu riin!
Dialogue: 0,0:15:26.08,0:15:28.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Luar biasa! Bagaimana kau tahu?
Dialogue: 0,0:15:29.21,0:15:30.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Aku pun tidak tahu.{\i0}
Dialogue: 0,0:15:31.42,0:15:34.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Saat menyentuhnya,{\i0}\N{\i1}elemen anginku menunjukkan tanda plus.{\i0}
Dialogue: 0,0:15:35.13,0:15:36.83,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Jadi, aku membelinya.{\i0}
Dialogue: 0,0:15:38.00,0:15:39.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Tanda plus...{\i0}
Dialogue: 0,0:15:43.88,0:15:44.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Noah?
Dialogue: 0,0:15:45.13,0:15:48.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Elemenku hanya meningkat{\i0}\N{\i1}untuk benda yang punya kehendak.{\i0}
Dialogue: 0,0:15:49.33,0:15:50.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Jadi, di dalam sini...{\i0}
Dialogue: 0,0:16:04.92,0:16:05.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa-apaan?!
Dialogue: 0,0:16:13.96,0:16:17.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Buku terlarang yang berisi\Njiwa pemiliknya?!
Dialogue: 0,0:16:20.75,0:16:25.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lari, Noah! Itu roh pendendam\Ndari penyihir penguasa angin!
Dialogue: 0,0:16:34.50,0:16:35.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Enyahlah.
Dialogue: 0,0:16:57.67,0:16:58.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa Kanda terluka?
Dialogue: 0,0:16:59.04,0:17:01.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}A-Aku tidak apa-apa.
Dialogue: 0,0:17:02.13,0:17:03.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi kota ini...
Dialogue: 0,0:17:04.79,0:17:06.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan khawatir.
Dialogue: 0,0:17:06.33,0:17:08.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saya menembakkannya ke langit.
Dialogue: 0,0:17:09.46,0:17:11.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau memutuskan itu dalam sekejap?
Dialogue: 0,0:17:12.04,0:17:14.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kapan kau jadi sekuat ini?
Dialogue: 0,0:17:14.58,0:17:16.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maaf mengganggu.
Dialogue: 0,0:17:16.67,0:17:17.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Siapa kau?
Dialogue: 0,0:17:17.92,0:17:21.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saya menjalankan toko antik di sini.
Dialogue: 0,0:17:21.46,0:17:23.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Alan Birdsley, siap melayani.
Dialogue: 0,0:17:23.92,0:17:26.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Setelah melihat aksi heroik Anda,
Dialogue: 0,0:17:26.29,0:17:29.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}saya punya barang\Nyang harus Anda lihat.
Dialogue: 0,0:17:29.71,0:17:31.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ada harta karun?
Dialogue: 0,0:17:31.42,0:17:33.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saya sangat berharap begitu.
Dialogue: 0,0:17:37.17,0:17:39.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Silakan lewat sini.
Dialogue: 0,0:17:40.67,0:17:45.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Cincin ini hanya menunjukkan\Nwujud aslinya pada tuan sejatinya.
Dialogue: 0,0:17:45.42,0:17:47.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Setidaknya begitu kata legendanya.
Dialogue: 0,0:17:48.04,0:17:52.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saya sudah tunjukkan pada banyak orang,\Ntapi tak ada yang memicu reaksi.
Dialogue: 0,0:17:53.29,0:17:55.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Artinya, ini sampah, kan?
Dialogue: 0,0:17:55.25,0:17:58.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saya juga berpikir begitu,\Nlalu membuangnya.
Dialogue: 0,0:17:58.50,0:18:01.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi cincin ini kembali.
Dialogue: 0,0:18:01.13,0:18:02.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kembali?
Dialogue: 0,0:18:02.42,0:18:05.63,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya. Kembali ke toko dengan sendirinya.
Dialogue: 0,0:18:05.71,0:18:08.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pasti ada sesuatu yang istimewa padanya.
Dialogue: 0,0:18:09.21,0:18:10.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Menarik, kan?
Dialogue: 0,0:18:11.08,0:18:12.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Cobalah pakai, Noah.
Dialogue: 0,0:18:13.04,0:18:14.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jika Kanda memaksa...
Dialogue: 0,0:18:21.88,0:18:23.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Oh?
Dialogue: 0,0:18:23.83,0:18:25.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Memeriksa batas level.{\i0}
Dialogue: 0,0:18:26.75,0:18:27.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Cincin ini bicara.{\i0}
Dialogue: 0,0:18:27.96,0:18:30.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Rarity: SSS.{\i0}
Dialogue: 0,0:18:30.29,0:18:32.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Menerima Tuan baru.{\i0}
Dialogue: 0,0:18:32.63,0:18:33.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Mode zirah.{\i0}
Dialogue: 0,0:18:42.50,0:18:45.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Inikah... wujud aslinya...?
Dialogue: 0,0:18:47.46,0:18:48.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Zirah?{\i0}
Dialogue: 0,0:18:50.46,0:18:52.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Tak ada perubahan status.{\i0}
Dialogue: 0,0:18:53.08,0:18:56.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Berbeda dengan buku tadi atau Leviathan.{\i0}
Dialogue: 0,0:18:57.33,0:18:58.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Tapi kelihatannya berguna.{\i0}
Dialogue: 0,0:18:59.46,0:19:00.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Alan, aku ambil ini.
Dialogue: 0,0:19:01.33,0:19:03.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku akan kirim pembayarannya nanti.
Dialogue: 0,0:19:03.25,0:19:05.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tak apa. Biar Kanda yang bayar.
Dialogue: 0,0:19:06.25,0:19:07.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kanda?
Dialogue: 0,0:19:07.42,0:19:09.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Anggap saja hadiah.
Dialogue: 0,0:19:09.33,0:19:11.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pedang Iblis, sekarang zirah sihir.
Dialogue: 0,0:19:11.83,0:19:14.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau benar-benar hebat.
Dialogue: 0,0:19:18.58,0:19:20.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yakin tak perlu tumpangan?
Dialogue: 0,0:19:20.63,0:19:22.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saya tak apa. Ada yang menjemput.
Dialogue: 0,0:19:28.33,0:19:30.75,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terima kasih untuk hari ini, Kanda.
Dialogue: 0,0:19:30.96,0:19:34.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Mati kau, bocah tolol!
Dialogue: 0,0:19:34.21,0:19:35.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tuan!
Dialogue: 0,0:19:40.63,0:19:42.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Hei! Apa yang kau lakukan?!
Dialogue: 0,0:19:43.04,0:19:44.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Lepaskan aku!
Dialogue: 0,0:19:44.54,0:19:47.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau lagi? Gigih juga.
Dialogue: 0,0:19:47.13,0:19:48.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Anda tidak apa-apa?
Dialogue: 0,0:19:48.54,0:19:49.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya.
Dialogue: 0,0:19:49.42,0:19:50.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Noah, apa itu tadi?
Dialogue: 0,0:19:51.46,0:19:54.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Saya hubungkan benda baru ini\Ndengan Leviathan.
Dialogue: 0,0:19:55.46,0:19:59.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sepertinya dia bereaksi pada niat membunuh\Ndan bertahan secara otomatis.
Dialogue: 0,0:19:59.29,0:20:00.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pertahanan otomatis?
Dialogue: 0,0:20:01.29,0:20:03.13,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kau sudah menguasainya?
Dialogue: 0,0:20:03.21,0:20:04.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Luar biasa.
Dialogue: 0,0:20:15.75,0:20:18.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Karena mereka menyerangku,{\i0}\N{\i1}para lintah darat itu sudah tamat.{\i0}
Dialogue: 0,0:20:19.54,0:20:21.71,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Aku harus memberitahunya kabar baik ini.{\i0}
Dialogue: 0,0:20:22.46,0:20:25.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Wah, suara yang bagus.
Dialogue: 0,0:20:25.50,0:20:26.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Sini tuangkan aku minuman.
Dialogue: 0,0:20:27.04,0:20:28.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Maaf, saya tak melakukan itu...
Dialogue: 0,0:20:29.00,0:20:32.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Minggir! Aku yang lihat dia duluan!
Dialogue: 0,0:20:32.96,0:20:35.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aduh! Kau mau berantem?!
Dialogue: 0,0:20:35.67,0:20:37.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Cih, belagu.
Dialogue: 0,0:20:43.75,0:20:46.00,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ma-Maaf!
Dialogue: 0,0:20:46.21,0:20:48.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Permainan pedang yang hebat!
Dialogue: 0,0:20:48.29,0:20:50.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Itu Pangeran, tolol!
Dialogue: 0,0:20:51.00,0:20:53.92,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}A-Ampuni saya, Yang Mulia!
Dialogue: 0,0:20:53.96,0:20:55.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pemilik toko.
Dialogue: 0,0:20:55.58,0:20:57.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Jangan biarkan dia masuk.
Dialogue: 0,0:20:57.54,0:21:02.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Tapi kami bergantung\Npada pelanggan seperti itu...
Dialogue: 0,0:21:03.38,0:21:05.29,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bisnis saya harus dijalankan.
Dialogue: 0,0:21:05.63,0:21:08.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baiklah, kalau begitu aku beli tokonya.
Dialogue: 0,0:21:08.42,0:21:09.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Eh?
Dialogue: 0,0:21:09.25,0:21:10.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Pangeran?
Dialogue: 0,0:21:10.29,0:21:11.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Zoey.
Dialogue: 0,0:21:11.25,0:21:12.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya?
Dialogue: 0,0:21:16.58,0:21:18.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Seribu riin.
Dialogue: 0,0:21:18.42,0:21:19.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Cukup?
Dialogue: 0,0:21:20.67,0:21:21.67,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya!
Dialogue: 0,0:21:22.13,0:21:24.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Terima kasih, Yang Mulia!\NTerima kasih banyak!
Dialogue: 0,0:21:25.04,0:21:27.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}E-Eng, Pangeran...
Dialogue: 0,0:21:27.42,0:21:29.38,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kenapa sampai sejauh ini demi saya?
Dialogue: 0,0:21:29.75,0:21:32.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bagaimana saya bisa membalas budi?
Dialogue: 0,0:21:33.29,0:21:34.46,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bernyanyi saja.
Dialogue: 0,0:21:35.42,0:21:37.79,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Yang kuinginkan hanyalah suaramu.
Dialogue: 0,0:21:38.46,0:21:40.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Bernyanyi untukku sudah cukup.
Dialogue: 0,0:21:40.96,0:21:42.88,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Apa nyanyian saya seberharga itu?
Dialogue: 0,0:21:43.38,0:21:45.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Ya. Aku punya harapan besar.
Dialogue: 0,0:21:45.46,0:21:47.33,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Aku ingin Yang Mulia\Nmendengarmu.
Dialogue: 0,0:21:47.67,0:21:49.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Asah kemampuanmu sampai saatnya.
Dialogue: 0,0:21:53.21,0:21:54.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Baik!
Dialogue: 0,0:22:06.25,0:22:09.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}Kalau begitu, izinkan saya\Nmenyanyikan satu lagu lagi.
Dialogue: 0,0:22:17.88,0:22:22.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Mimpi singkat yang masih tak sampai{\i0}
Dialogue: 0,0:22:22.46,0:22:29.21,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Kutakut akan peluh{\i0}\N{\i1}Yang tak kunjung sembuh{\i0}
Dialogue: 0,0:22:29.79,0:22:34.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Siluet terpantul di tepi jendela{\i0}
Dialogue: 0,0:22:34.38,0:22:39.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Kenanganku bersamamu{\i0}\N{\i1}pelahan memudar{\i0}
Dialogue: 0,0:22:41.67,0:22:46.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Diterangi sinar rembulan{\i0}
Dialogue: 0,0:22:48.17,0:22:50.25,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Mataku tak menipu.{\i0}
Dialogue: 0,0:22:51.46,0:22:55.42,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Itulah harta karun sejati.{\i0}
Dialogue: 0,0:22:59.96,0:23:03.58,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Rasaku padamu tidak{\i0}
Dialogue: 0,0:23:03.88,0:23:09.54,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Harapku agar sajak cinta abadi ini{\i0}\N{\i1}Kian sampai padamu{\i0}
Dialogue: 0,0:23:09.67,0:23:15.50,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Walau kutahu kan terwujud{\i0}\N{\i1}Aku tetap memimpikannya{\i0}
Dialogue: 0,0:23:15.63,0:23:17.04,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Air mataku mengering{\i0}
Dialogue: 0,0:23:17.17,0:23:19.96,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Pemandangan mengabur{\i0}\N{\i1}Di seberang keabadian{\i0}
Dialogue: 0,0:23:20.08,0:23:23.08,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Walau aku terlepas bayang-bayangmu{\i0}
Dialogue: 0,0:23:23.21,0:23:30.17,Default,,0000,0000,0000,,{\blur3}{\fad(00,40)}{\i1}Hatiku kan terpaut di sisimu{\i0}
